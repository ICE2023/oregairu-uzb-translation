    # voice "audio/log/A099ED20_YUK015.ogg"
    # yuk "「誕生日だもの。たとえ相手があなただとしても、プレゼントくらいはさせてもらうわ」"
    voice "audio/log/A099ED20_YUK015.ogg"
    yuk "「Tug‘ilgan kuning-ku. Garchi sen bo‘lsang ham, sovg‘a berib qo‘yishim kerak-da.」"

# game/scripts/game/OG_A099ED20.rpy:339
translate uzbek OG_A099ED20_0_47c9ef6f:

    # voice "audio/log/A099ED20_HCM014.ogg"
