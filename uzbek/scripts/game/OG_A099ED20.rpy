# game/scripts/game/OG_A099ED20.rpy:296
translate uzbek OG_A099ED20_0_4018fe54:

    # voice "audio/log/A099ED20_HCM012.ogg"
    # hcm "「お前だって、最初渋ってたわりにはノリノリで歌ってたじゃねーか」"
    voice "audio/log/A099ED20_HCM012.ogg"
    hcm "「O‘zing ham boshida istamagan bo‘lsang ham, keyin berilib ketib qo‘shiq aytding-ku.」"

# game/scripts/game/OG_A099ED20.rpy:309
translate uzbek OG_A099ED20_0_0dfefce6:

    # voice "audio/log/A099ED20_YUK012.ogg"
    # yuk "「あれは、由比ケ浜さんや小町さんが強引に……そういえば、ふたりとも遅いわね」"
    voice "audio/log/A099ED20_YUK012.ogg"
    yuk "「Unga Yuigahama va Komachi majburlashdi-da... Aytgancha, ikkalasi kechikyapti.」"

# game/scripts/game/OG_A099ED20.rpy:313
translate uzbek OG_A099ED20_0_333bb24c:

    # narrator "と、ほんのり赤面した雪ノ下は、小町たちが順番待ちで並んでいるカウンターの方をちらりと見やった。"
    narrator "Biroz qizarib ketgan Yukinoshita, navbatda turgan Komachilar tomon ko‘z qiri bilan qaradi."

# game/scripts/game/OG_A099ED20.rpy:323
