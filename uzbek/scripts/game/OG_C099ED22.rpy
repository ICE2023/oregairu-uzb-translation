# game/scripts/game/OG_C099ED22.rpy:34
translate uzbek OG_C099ED22_0_ab2f46d6:

    # voice "audio/log/C099ED22_HCM001.ogg"
    # narrator "駅前に着くと、いつものようにキラキラとした粒子を纏った戸塚が、かわいらしく手を降って俺を待っていた。"
    voice "audio/log/C099ED22_HCM001.ogg"
    narrator "Stansiya oldiga yetib kelganimda, har doimgidek yorqin zarrachalar bilan o'ralgan Totsuka, yoqimtoy tarzda qo'l silkitib meni kutib turardi."

# game/scripts/game/OG_C099ED22.rpy:38
