    voice "audio/log/B022ESS5_YUI000.ogg"
    yui "「Hikki! Xayriyat, zo‘rg‘a topishdik-a~. Ko‘risha olmaymiz deb o‘ylagandim.」"

# game/scripts/game/OG_B022ESS5.rpy:33
translate uzbek OG_B022ESS5_0_1597f0a1:

    # voice "audio/log/B022ESS5_HCM001.ogg"
    # hcm "「本当に悪い！　お詫びと言っちゃなんだけど、なんかおごるわ」"
    voice "audio/log/B022ESS5_HCM001.ogg"
    hcm "「Rostdan ham uzr! Aybimni yuvish uchun biron narsa olib beraman.」"

# game/scripts/game/OG_B022ESS5.rpy:43
translate uzbek OG_B022ESS5_0_dbc3636b:

    # voice "audio/log/B022ESS5_YUI001.ogg"
    # yui "「へっ？　あ、ううん、そんなのいいよー。もともと、サブレの面倒を見てもらったお礼なんだし」"
    voice "audio/log/B022ESS5_YUI001.ogg"
    yui "「E? A, yo‘q, shart emas. Bu o‘zi Soblga qaraganingiz uchun minnatdorchilik edi-ku.」"

# game/scripts/game/OG_B022ESS5.rpy:47
translate uzbek OG_B022ESS5_0_66990886:

    # voice "audio/log/B022ESS5_YUI002.ogg"
    # yui "「なかなか来ないから、ヒッキーもしかして待ち合わせ場所間違えてるのかなぁって、ちょっと思ってたんだよね」"
    voice "audio/log/B022ESS5_YUI002.ogg"
    yui "「Kelishing kechikib ketgach, Hikki uchrashuv joyini adashtirdimikin deb o‘ylab qolgandim.」"

# game/scripts/game/OG_B022ESS5.rpy:51
translate uzbek OG_B022ESS5_0_b6eae95b:

    # voice "audio/log/B022ESS5_HCM002.ogg"
    # hcm "「だったらメールでも電話でもなんでもいいから連絡しろよ……余計に待たせちまっただろ」"
    voice "audio/log/B022ESS5_HCM002.ogg"
    hcm "「Unda xatmi, qo‘ng‘iroqmi qilib xabar bermaysanmi... Shuncha kuttirib qo‘ydim.」"

# game/scripts/game/OG_B022ESS5.rpy:61
translate uzbek OG_B022ESS5_0_6cd2548e:

    # voice "audio/log/B022ESS5_YUI003.ogg"
    # yui "「んー、でもね。なんか急かして連絡するのも、ヒッキーを信用してないみたいでイヤかなぁって思って」"
    voice "audio/log/B022ESS5_YUI003.ogg"
    yui "「Hmm, lekin. Shoshiltirib xabar yozsam, xuddi Hikkiga ishonmayotgandek tuyuladimi deb o‘yladim-da.」"

# game/scripts/game/OG_B022ESS5.rpy:65
