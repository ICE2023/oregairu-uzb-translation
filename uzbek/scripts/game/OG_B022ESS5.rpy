    # hcm "「……」"
    hcm "「......」"

# game/scripts/game/OG_B022ESS5.rpy:76
translate uzbek OG_B022ESS5_0_a4f0ba95:

    # voice "audio/log/B022ESS5_YUI004.ogg"
    # yui "「そ、それにどのみち会場近くは電波悪いし、こういうトラブルがある方がイベントが盛り上がる気がするしねっ！」"
    voice "audio/log/B022ESS5_YUI004.ogg"
    yui "「Qo-qolaversa, bu yerda aloqa yaxshi emas ekan. Yana shunday sarguzashtlar bo‘lsa, yanada qiziqroq bo‘ladi-ku!」"

# game/scripts/game/OG_B022ESS5.rpy:80
translate uzbek OG_B022ESS5_0_131a054a:

    # voice "audio/log/B022ESS5_HCM003.ogg"
    # hcm "「なんだ、そのムダなポジティブシンキング……」"
    voice "audio/log/B022ESS5_HCM003.ogg"
    hcm "「Bu qanaqa bema’ni pozitiv fikrlash...」"

# game/scripts/game/OG_B022ESS5.rpy:93
translate uzbek OG_B022ESS5_0_81dbd9fb:

    # voice "audio/log/B022ESS5_YUI005.ogg"
    # yui "「ムダとか言わなーいっ！　こういうこと、なんていうんだっけ？　雨降って自分固まる？」"
    voice "audio/log/B022ESS5_YUI005.ogg"
    yui "「Bema’ni demang! Bunday paytda nima deyilardi? Yomg‘irdan keyin odam qotib qoladimi?」"

# game/scripts/game/OG_B022ESS5.rpy:97
translate uzbek OG_B022ESS5_0_cf7aaf90:

    # voice "audio/log/B022ESS5_HCM004.ogg"
    # hcm "「聞いた側が固まっちゃうぞ、それ……。地だ地。地固まるんだよ」"
    voice "audio/log/B022ESS5_HCM004.ogg"
    hcm "「Eshitgan odamni qotib qoldiradigan gap bo‘ldi bu... Yer deyiladi. Yomg‘irdan keyin yer qotadi.」"

# game/scripts/game/OG_B022ESS5.rpy:107
translate uzbek OG_B022ESS5_0_92ccb901:

    # voice "audio/log/B022ESS5_YUI006.ogg"
    # yui "「と、とにかく！　待ち合わせのことは忘れてさ、気を取り直して行こ？　小町ちゃんのお土産買わなきゃね」"
    voice "audio/log/B022ESS5_YUI006.ogg"
    yui "「Xu-xullas! Uchrashuvni unutaylik, kayfiyatni ko‘tarib ketdik! Komachiga sovg‘a olishimiz kerak.」"

# game/scripts/game/OG_B022ESS5.rpy:111
translate uzbek OG_B022ESS5_0_c7f51884:

    # voice "audio/log/B022ESS5_HCM005.ogg"
