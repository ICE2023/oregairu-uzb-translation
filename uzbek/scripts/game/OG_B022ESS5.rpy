translate uzbek OG_B022ESS5_0_75ec9ca1:

    # voice "audio/log/B022ESS5_YUI007.ogg"
    # yui "「……？　どうしたの？　じっと見て」"
    voice "audio/log/B022ESS5_YUI007.ogg"
    yui "「...? Nima bo‘ldi? Buncha tikilib qolding?」"

# game/scripts/game/OG_B022ESS5.rpy:125
translate uzbek OG_B022ESS5_0_5410ce45:

    # voice "audio/log/B022ESS5_HCM006.ogg"
    # hcm "「ん？　ああ、いや……その浴衣、いいな」"
    voice "audio/log/B022ESS5_HCM006.ogg"
    hcm "「Hm? A, yo‘q... shu yukata yarashibdi.」"

# game/scripts/game/OG_B022ESS5.rpy:138
translate uzbek OG_B022ESS5_0_f4f93c7f:

    # voice "audio/log/B022ESS5_YUI008.ogg"
