    voice "audio/log/D020ESS0_KOM002.ogg"
    kom "Hmm. Negadir o'zingni majburlayotganga o'xshaysan-da."

# game/scripts/game/OG_D020ESS0.rpy:48
translate uzbek OG_D020ESS0_0_a484db4b:

    # hcm "（……うう。だが、今さらやっぱり行くなんて言えるわけないし、ていうか別に行きたくないし）"
