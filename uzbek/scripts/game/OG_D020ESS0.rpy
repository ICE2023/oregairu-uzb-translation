# game/scripts/game/OG_D020ESS0.rpy:42
translate uzbek OG_D020ESS0_0_c73b1483:

    # hcm "（……まあ、実はこの３日間だったら、授業もないし行こうと思えば行けたんだけど……）"
    hcm "(...Aslida, shu uch kunda darslar yo'q edi, xohlasam borishim mumkin edi-yu...)"

# game/scripts/game/OG_D020ESS0.rpy:44
translate uzbek OG_D020ESS0_0_6d5fd6df:

    # voice "audio/log/D020ESS0_KOM002.ogg"
