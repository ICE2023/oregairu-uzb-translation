# game/scripts/game/OG_N031ESS1.rpy:90
translate uzbek OG_N031ESS1_0_ad176905:

    # voice "audio/log/N031ESS1_YUK009.ogg"
    # yuk "「……いいえ。まさかあなたが人の誕生日を覚えているなんて思わなかったから」"
    voice "audio/log/N031ESS1_YUK009.ogg"
    yuk "「...Yo‘q. Shunchaki sen birovning tug‘ilgan kunini eslab qolishingni kutmagandim.」"

# game/scripts/game/OG_N031ESS1.rpy:94
translate uzbek OG_N031ESS1_0_39f8469b:

    # voice "audio/log/N031ESS1_HCM008.ogg"
    # hcm "「……ああ。いや、前そんな話しただろ。記憶力いいから覚えてんだよ。向こうが話したこと忘れてても俺が覚えてるからよくキモがられてストーカー扱いされたもんだ」"
    voice "audio/log/N031ESS1_HCM008.ogg"
    hcm "「...Ha. Oldin gaplashgan edik-ku. Xotiram yaxshi, eslab qolganman. Boshqalar gapirganini unutib yuborsa ham, men eslab qolganim uchun, meni g‘alati deb, ta’qibchi deb o‘ylashardi.」"

# game/scripts/game/OG_N031ESS1.rpy:98
translate uzbek OG_N031ESS1_0_00e0ae97:

    # voice "audio/log/N031ESS1_YUK010.ogg"
    # yuk "「私も今まさにそう思ったわ」"
    voice "audio/log/N031ESS1_YUK010.ogg"
    yuk "「Men ham hozir aynan shuni o‘ylab turgandim.」"

# game/scripts/game/OG_N031ESS1.rpy:102
translate uzbek OG_N031ESS1_0_8ae68edf:

    # voice "audio/log/N031ESS1_HCM009.ogg"
    # hcm "「誕生日知ってるくらいで犯罪者扱いされてたまるか。だいたいお前だって知ってるじゃねぇか俺の誕生日」"
    voice "audio/log/N031ESS1_HCM009.ogg"
    hcm "「Tug‘ilgan kunni bilish jinoyatchi degani emas-ku. O‘zing ham mening tug‘ilgan kunimni bilasan-ku.」"

# game/scripts/game/OG_N031ESS1.rpy:106
translate uzbek OG_N031ESS1_0_0c5d6606:

    # voice "audio/log/N031ESS1_YUK011.ogg"
    # yuk "「……ええ。私も記憶力がいいものだから」"
    voice "audio/log/N031ESS1_YUK011.ogg"
    yuk "「...Ha. Mening ham xotiram yaxshi.」"

