# game/scripts/game/OG_N031ESS1.rpy:38
translate uzbek OG_N031ESS1_0_eca6f0bd:

    # voice "audio/log/N031ESS1_HCM001.ogg"
    # hcm "「普段どんだけ死んでんだ俺の顔。あと人の寝顔見るな」"
    voice "audio/log/N031ESS1_HCM001.ogg"
    hcm "「Odatda yuzim shunchalik o‘likdek ko‘rinadimi? Keyin, uxlayotgan odamga tikilib turma.」"

# game/scripts/game/OG_N031ESS1.rpy:42
translate uzbek OG_N031ESS1_0_2cafb0fd:

    # voice "audio/log/N031ESS1_YUK003.ogg"
    # yuk "「安否確認も部長の仕事のうちよ。こんな夏場に放置したら腐敗が進むだろうし……ああ、ごめんなさい。あなたの目は夏場に限らず年中腐っていたわね」"
    voice "audio/log/N031ESS1_YUK003.ogg"
    yuk "「Xavfsizligingni tekshirish klub rahbarining vazifasi-da. Yozning issig‘ida qoldirib ketsam, chirib ketishing mumkin... A, uzr. Ko‘zlaring yozda emas, yil bo‘yi chirib yotar ekan.」"

# game/scripts/game/OG_N031ESS1.rpy:46
translate uzbek OG_N031ESS1_0_ca52133b:

    # voice "audio/log/N031ESS1_HCM002.ogg"
    # hcm "「死んでる前提で話進めるのやめませんか。だいたい謝るポイントがおかしいし……、目が常夏とかまったく嬉しくねぇんだよ」"
    voice "audio/log/N031ESS1_HCM002.ogg"
    hcm "「Meni o‘lik deb hisoblab gapirishni bas qilsang-chi. Uzr so‘rashing ham g‘alati... Ko‘zlarim hamisha yozdek bo‘lishi meni xursand qilmaydi.」"

# game/scripts/game/OG_N031ESS1.rpy:50
translate uzbek OG_N031ESS1_0_044e6596:

    # voice "audio/log/N031ESS1_YUK004.ogg"
    # yuk "「頭も常夏、のほうが良かったかしら？」"
    voice "audio/log/N031ESS1_YUK004.ogg"
    yuk "「Boshing ham hamisha yozdek bo‘lsin deganim ma’qulmidi?」"

# game/scripts/game/OG_N031ESS1.rpy:54
translate uzbek OG_N031ESS1_0_46591e67:

    # voice "audio/log/N031ESS1_HCM003.ogg"
    # hcm "「残念。このぼっち思考が宿る俺の頭は常冬なんだよ」"
    voice "audio/log/N031ESS1_HCM003.ogg"
    hcm "「Afsus. Yolg‘izlikni xush ko‘radigan mening boshimda doim qish hukmron.」"

# game/scripts/game/OG_N031ESS1.rpy:58
translate uzbek OG_N031ESS1_0_203ebea5:

    # voice "audio/log/N031ESS1_YUK005.ogg"
    # yuk "「将来的にはそうなるかもしれないわね」"
    voice "audio/log/N031ESS1_YUK005.ogg"
    yuk "「Kelajakda shunday bo‘lishi mumkin.」"

# game/scripts/game/OG_N031ESS1.rpy:62
translate uzbek OG_N031ESS1_0_f7e29ffe:

    # voice "audio/log/N031ESS1_HCM004.ogg"
    # hcm "「おい、俺の生え際に視線を向けるのをすぐにやめろ」"
    voice "audio/log/N031ESS1_HCM004.ogg"
    hcm "「Hoy, sochimning chetiga tikilishni bas qil.」"

# game/scripts/game/OG_N031ESS1.rpy:66
translate uzbek OG_N031ESS1_0_8a15fec0:

    # voice "audio/log/N031ESS1_YUK006.ogg"
    # yuk "「涼しくていいんじゃないかしら。あなた、夏生まれの割に夏が嫌いそうだものね」"
    voice "audio/log/N031ESS1_YUK006.ogg"
    yuk "「Salqin bo‘ladi-ku, yomon emas. Yozda tug‘ilgan bo‘lsang ham, yozni unchalik yoqtirmaydiganga o‘xshaysan.」"

# game/scripts/game/OG_N031ESS1.rpy:70
translate uzbek OG_N031ESS1_0_b1844b8d:

    # voice "audio/log/N031ESS1_HCM005.ogg"
    # hcm "「ああ、夏は嫌いだ。というか、夏生まれは生まれてすぐにクーラーが効いてる環境下で育つからな。暑さが得意なわけがないんだよ」"
    voice "audio/log/N031ESS1_HCM005.ogg"
    hcm "「Ha, yozni yomon ko‘raman. Chunki yozda tug‘ilganlar darrov konditsionerli xonada o‘sadi-da. Issiqqa chidamli bo‘lishi mumkin emas.」"

# game/scripts/game/OG_N031ESS1.rpy:74
translate uzbek OG_N031ESS1_0_5db71ac8:

    # voice "audio/log/N031ESS1_YUK007.ogg"
