    # voice "audio/log/N026ESS0_HIR005.ogg"
    # hir "「そろそろ、君たちが揃って暗い顔をしているわけを聞こうか」"
    voice "audio/log/N026ESS0_HIR005.ogg"
    hir "Endi, hammangizning yuzingiz tushib ketganining sababini eshitaychi."

# game/scripts/game/OG_N026ESS0.rpy:835
translate uzbek OG_N026ESS0_0_e447d582:

    # voice "audio/log/N026ESS0_HAY007.ogg"
    # hay "「ちょっと、孤立しちゃってる子がいたので」"
