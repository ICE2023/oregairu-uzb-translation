translate uzbek OG_N026ESS0_0_8647e251:

    # voice "audio/log/N026ESS0_HCM014.ogg"
    # hcm "「……世の中は大概バカばっかだ。早めに気付いて良かったな」"
    voice "audio/log/N026ESS0_HCM014.ogg"
    hcm "...Dunyoda ahmoqlar ko'p. Buni erta tushunganing yaxshi."

# game/scripts/game/OG_N026ESS0.rpy:592
translate uzbek OG_N026ESS0_0_c34f9982:

    # voice "audio/log/N026ESS0_RUM003.ogg"
