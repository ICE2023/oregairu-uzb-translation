    voice "audio/log/N026ESS0_RUM003.ogg"
    tur "E?"

# game/scripts/game/OG_N026ESS0.rpy:596
translate uzbek OG_N026ESS0_0_50d9c61c:

    # voice "audio/log/N026ESS0_YUK006.ogg"
    # yuk "「あなたもその、大概でしょう」"
    voice "audio/log/N026ESS0_YUK006.ogg"
    yuk "Sen ham o'sha ko'pchilikdan birisan."

# game/scripts/game/OG_N026ESS0.rpy:608
translate uzbek OG_N026ESS0_0_1046fdb8:

    # voice "audio/log/N026ESS0_HCM015.ogg"
    # hcm "「聞いてたのかよ……っておいちょっと待て、大概とかその他大勢の中ですらひとりになれる逸材だぞ俺は」"
    voice "audio/log/N026ESS0_HCM015.ogg"
    hcm "Eshitib turganmiding... hoy, to'xtab tur, men ko'pchilik ichida ham yolg'iz qola oladigan noyob iste'dod egasiman."

# game/scripts/game/OG_N026ESS0.rpy:618
translate uzbek OG_N026ESS0_0_14cce928:

    # voice "audio/log/N026ESS0_YUK007.ogg"
    # yuk "「そんなことをそこまで誇らしげに言えるのはあなたくらいでしょうね……呆れるのを通り越して軽蔑するわ」"
    voice "audio/log/N026ESS0_YUK007.ogg"
    yuk "Bunday narsani shunchalik faxr bilan aytadigan sendan boshqa odam bo'lmasa kerak... Hayratlanish u yoqda tursin, nafratlanaman."

# game/scripts/game/OG_N026ESS0.rpy:622
translate uzbek OG_N026ESS0_0_5d09ed2a:

    # voice "audio/log/N026ESS0_HCM016.ogg"
    # hcm "「通り越したら尊敬しねえか、普通……」"
    voice "audio/log/N026ESS0_HCM016.ogg"
    hcm "Odatda hayratlanishdan o'tib, hurmat qilish kerak emasmi..."

# game/scripts/game/OG_N026ESS0.rpy:634
translate uzbek OG_N026ESS0_0_656336fb:

    # voice "audio/log/N026ESS0_RUM004.ogg"
    # tur "「……ねぇ、名前。名前は？」"
    voice "audio/log/N026ESS0_RUM004.ogg"
    tur "...Isming nima?"

# game/scripts/game/OG_N026ESS0.rpy:644
translate uzbek OG_N026ESS0_0_aac98f56:

    # voice "audio/log/N026ESS0_YUK008.ogg"
    # yuk "「人に名前をたずねるときは、まず自分から名乗るものよ」"
    voice "audio/log/N026ESS0_YUK008.ogg"
    yuk "Birovning ismini so'rashdan oldin, o'zini tanishtirish kerak."

# game/scripts/game/OG_N026ESS0.rpy:654
translate uzbek OG_N026ESS0_0_b1ea0461:

    # voice "audio/log/N026ESS0_RUM005.ogg"
    # tur "「……鶴見、留美」"
    voice "audio/log/N026ESS0_RUM005.ogg"
    tur "...Tsurumi Rumi."

# game/scripts/game/OG_N026ESS0.rpy:661
translate uzbek OG_N026ESS0_0_5acd630f:

    # voice "audio/log/N026ESS0_YUK009.ogg"
    # yuk "「私は雪ノ下雪乃。そこのは……ヒキ、ヒキガ、ヒキガエルくん、だったかしら」"
    voice "audio/log/N026ESS0_YUK009.ogg"
    yuk "Men Yukinoshita Yukinoman. Anavisi esa... Hiki, Hikiga, Hikigaeru (qurbaqa)-kunmidi?"

# game/scripts/game/OG_N026ESS0.rpy:665
translate uzbek OG_N026ESS0_0_b329a06f:

    # voice "audio/log/N026ESS0_HCM017.ogg"
    # hcm "「おい、なんで俺の小４の頃のあだ名知ってんだよ……比企谷八幡だ」"
    voice "audio/log/N026ESS0_HCM017.ogg"
    hcm "Hoy, nega mening 4-sinfdagi laqabimni bilasan... Hikigaya Hachimanman."

# game/scripts/game/OG_N026ESS0.rpy:675
translate uzbek OG_N026ESS0_0_1f0b1d70:

    # voice "audio/log/N026ESS0_RUM006.ogg"
    # tur "「ふーん。なんか、ふたりは違う気がする。あのへんの人たちと」"
    voice "audio/log/N026ESS0_RUM006.ogg"
    tur "Hm. Negadir ikkalangiz anavilardan farq qilasizlar."

# game/scripts/game/OG_N026ESS0.rpy:679
translate uzbek OG_N026ESS0_0_b1be64a0:

    # hcm "（あのへん、な……まあな、確かに違うだろうよ、俺と葉山とじゃ）"
    hcm "(Anavilar, a... Ha, men va Hayama aniq farq qilamiz.)"

# game/scripts/game/OG_N026ESS0.rpy:687
translate uzbek OG_N026ESS0_0_d95f64fe:

    # voice "audio/log/N026ESS0_RUM007.ogg"
    # tur "「……私も違うの。あのへんと」"
    voice "audio/log/N026ESS0_RUM007.ogg"
    tur "...Men ham farq qilaman. Anavilardan."

# game/scripts/game/OG_N026ESS0.rpy:691
translate uzbek OG_N026ESS0_0_0b7a6942:

    # voice "audio/log/N026ESS0_HCM018.ogg"
    # hcm "「違うって、なにが？」"
    voice "audio/log/N026ESS0_HCM018.ogg"
    hcm "Farq qilaman deganing nimasi?"

# game/scripts/game/OG_N026ESS0.rpy:701
translate uzbek OG_N026ESS0_0_199808f2:

    # voice "audio/log/N026ESS0_RUM008.ogg"
    # tur "「……周りはみんなガキなんだもん。前は、その中で結構うまく立ち回ってたけど、なんかそういうのくだらないから、やめた」"
    voice "audio/log/N026ESS0_RUM008.ogg"
    tur "...Atrofdagilarning hammasi yosh bola. Oldin men ham ular bilan yaxshi chiqishardim, lekin keyin hammasi bema'ni tuyulib, bas qildim."

# game/scripts/game/OG_N026ESS0.rpy:705
translate uzbek OG_N026ESS0_0_1fc1b8de:

    # voice "audio/log/N026ESS0_YUK010.ogg"
    # yuk "「そう」"
    voice "audio/log/N026ESS0_YUK010.ogg"
    yuk "Shunaqami."

# game/scripts/game/OG_N026ESS0.rpy:709
translate uzbek OG_N026ESS0_0_c5fc8da2:

    # voice "audio/log/N026ESS0_RUM009.ogg"
    # tur "「ひとりでも別にいいかなって……中学入れば、よそから来た人と友達になればいいし」"
    voice "audio/log/N026ESS0_RUM009.ogg"
    tur "Yolg'iz qolsam ham mayli... o'rta maktabga kirsam, boshqa joydan kelganlar bilan do'stlashib olaman."

# game/scripts/game/OG_N026ESS0.rpy:713
translate uzbek OG_N026ESS0_0_38a14843:

    # voice "audio/log/N026ESS0_YUK011.ogg"
    # yuk "「残念だけど、そうはならないわ」"
    voice "audio/log/N026ESS0_YUK011.ogg"
    yuk "Afsuski, unday bo'lmaydi."

