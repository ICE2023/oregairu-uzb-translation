# game/scripts/game/OG_A050ESS0.rpy:178
translate uzbek OG_A050ESS0_0_9ed0d85b:

    # voice "audio/log/A050ESS0_KOM012.ogg"
    # kom "「小町的には、それもいいけど、テレテレ。はっ！　今の小町的にポイント高い！　けどアホ毛的に義妹の可能性は低い！」"
    voice "audio/log/A050ESS0_KOM012.ogg"
    kom "「Komachi bunga qarshi emas-u, hi-hi. O! Hozir Komachi ochkolarni qo‘lga kiritdi! Lekin sochlaringizga qarab, o‘gay singil bo‘lish ehtimoli past deb o‘ylayman!」"

# game/scripts/game/OG_A050ESS0.rpy:182
translate uzbek OG_A050ESS0_0_d83b92c5:

