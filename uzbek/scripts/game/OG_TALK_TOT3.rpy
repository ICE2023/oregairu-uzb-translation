# game/scripts/game/OG_TALK_TOT3.rpy:300
translate uzbek OG_TALK_TOT3_8_068cc5dc:

    # hcm "「なんか楽しそうに話してたし、邪魔しても悪いしな」"
    hcm "「Mazza qilib gaplashayotganding, xalaqit berishni xohlamadim.」"

# game/scripts/game/OG_TALK_TOT3.rpy:302
translate uzbek OG_TALK_TOT3_8_49735025:

    # voice "audio/log/TOT3B3_TOT002.ogg"
    # tot mca02 "「邪魔だなんて……、そういう考えよくないよ。部活の友達と話してるのも楽しいけど……八幡と話してるのが一番楽しいんだから！」"
    voice "audio/log/TOT3B3_TOT002.ogg"
    tot "「Xalaqit emish... bunday o‘ylama. To‘garakdagi do‘stlarim bilan gaplashish ham qiziq-u, lekin... Hachiman bilan gaplashish eng qizig‘i!」"

# game/scripts/game/OG_TALK_TOT3.rpy:306
translate uzbek OG_TALK_TOT3_8_952e16d2:

    # hcm "「へ？」"
    hcm "「E?」"

# game/scripts/game/OG_TALK_TOT3.rpy:308
translate uzbek OG_TALK_TOT3_8_8d33ff37:

    # voice "audio/log/TOT3B3_TOT003.ogg"
    # tot mca00 "「だ、だからさ、あんまり気とか遣わないで、もっと話そうよ」"
    voice "audio/log/TOT3B3_TOT003.ogg"
    tot "「Sh-shuning uchun, tortinmasdan bemalol gaplashaver.」"

# game/scripts/game/OG_TALK_TOT3.rpy:312
translate uzbek OG_TALK_TOT3_8_9f798eee:

    # hcm "「そ、そうか」"
    hcm "「Shu-shunaqami.」"

# game/scripts/game/OG_TALK_TOT3.rpy:314
translate uzbek OG_TALK_TOT3_8_22e7b883:

    # voice "audio/log/TOT3B3_TOT004.ogg"
    # tot "「うん……」"
    voice "audio/log/TOT3B3_TOT004.ogg"
    tot "「Ha...」"

# game/scripts/game/OG_TALK_TOT3.rpy:330
translate uzbek OG_TALK_TOT3_9_7ad030eb:

    # hcm "「俺、花火に来るの久しぶりなんだよな……こんなに混雑するもんだったか？　花火って」"
    hcm "「Men mushakbozlikka anchadan beri kelmagandim... Doim shunday tiqilinch bo‘ladimi?」"

# game/scripts/game/OG_TALK_TOT3.rpy:332
translate uzbek OG_TALK_TOT3_9_61312842:

    # voice "audio/log/TOT3B4_TOT000.ogg"
    # tot "「思い出って楽しいことばっかり覚えてるから、イヤだったことは忘れちゃうよね」"
    voice "audio/log/TOT3B4_TOT000.ogg"
    tot "「Odam faqat yaxshi xotiralarni eslab qoladi, yomonlarini esa unutib yuboradi-da.」"

# game/scripts/game/OG_TALK_TOT3.rpy:336
translate uzbek OG_TALK_TOT3_9_e0eac21a:

    # hcm "「そうか？　俺はイヤなことほどクッキリハッキリ覚えてるがな」"
    hcm "「Shunaqami? Men esa yomon narsalarni aniq-tiniq eslab qolaman.」"

# game/scripts/game/OG_TALK_TOT3.rpy:338
translate uzbek OG_TALK_TOT3_9_b2e24d8a:

    # voice "audio/log/TOT3B4_TOT001.ogg"
    # tot mca01 "「でも、花火が混雑するって忘れてたんでしょ？　きっと八幡にとって、花火は楽しいものだったんじゃないかな」"
    voice "audio/log/TOT3B4_TOT001.ogg"
    tot mca01 "「Lekin, tiqilinch bo‘lishini unutib qo‘yibsan-ku? Demak, sen uchun mushakbozlik quvonchli xotira bo‘lgan.」"

# game/scripts/game/OG_TALK_TOT3.rpy:342
translate uzbek OG_TALK_TOT3_9_b5210ffb:

    # hcm "「うっ……ま、まぁな」"
    hcm "「Uh... ha, to‘g‘ri.」"

# game/scripts/game/OG_TALK_TOT3.rpy:344
translate uzbek OG_TALK_TOT3_9_0426c8fe:

    # voice "audio/log/TOT3B4_TOT002.ogg"
    # tot "「今日も、いい思い出ばっかりが残る一日になるといいね」"
    voice "audio/log/TOT3B4_TOT002.ogg"
    tot "「Bugun ham faqat yaxshi xotiralar qoladigan kun bo‘lsin.」"

# game/scripts/game/OG_TALK_TOT3.rpy:348
translate uzbek OG_TALK_TOT3_9_68edb49b:

    # hcm "「……そうだな」"
    hcm "「...Shunday bo‘lsin.」"

# game/scripts/game/OG_TALK_TOT3.rpy:362
translate uzbek OG_TALK_TOT3_10_64715d70:

    # hcm "「戸塚は、相模ともよく話すのか？」"
    hcm "「Totsuka, Sagami bilan ham tez-tez gaplashasanmi?」"

# game/scripts/game/OG_TALK_TOT3.rpy:364
translate uzbek OG_TALK_TOT3_10_c7c4f4f8:

    # voice "audio/log/TOT3B5_TOT000.ogg"
    # tot mca02 "「ううん、あんまり……声をかけてきてくれたら話すくらい、かな？」"
    voice "audio/log/TOT3B5_TOT000.ogg"
    tot mca02 "「Yo‘q, unchalik emas... O‘zi gapirsa javob beraman xolos.」"

# game/scripts/game/OG_TALK_TOT3.rpy:368
translate uzbek OG_TALK_TOT3_10_2f8d4104:

    # hcm "「そっか……」"
    hcm "「Shunaqami...」"

# game/scripts/game/OG_TALK_TOT3.rpy:370
translate uzbek OG_TALK_TOT3_10_3ae8d21d:

    # voice "audio/log/TOT3B5_TOT001.ogg"
    # tot "「女子のグループで行動してることが多いから、ぼくからはあんまり話しかけないよ」"
    voice "audio/log/TOT3B5_TOT001.ogg"
    tot "「Ko‘pincha qizlar bilan yuradi, shuning uchun men gapirmayman.」"

# game/scripts/game/OG_TALK_TOT3.rpy:374
translate uzbek OG_TALK_TOT3_10_86adfcec:

    # hcm "「俺が最も苦手とするタイプだな」"
    hcm "「Men eng yomon ko‘radigan toifa ekan-da.」"

# game/scripts/game/OG_TALK_TOT3.rpy:376
translate uzbek OG_TALK_TOT3_10_0602f8cb:

    # voice "audio/log/TOT3B5_TOT002.ogg"
    # tot mca06 "「うん……こんなこと言ったら失礼かもしれないけど、ぼくも少し苦手かも……」"
    voice "audio/log/TOT3B5_TOT002.ogg"
    tot mca06 "「Ha... bunday deyish odobsizlik bo‘lar-u, lekin men ham unchalik yoqtirmayman...」"

# game/scripts/game/OG_TALK_TOT3.rpy:392
translate uzbek OG_TALK_TOT3_11_2f97ecf0:

    # hcm "「しかしなんだ、こういう花火大会って恋人同士とか友達同士で来るもんなんかね。さっきからそういうの目立つよな？」"
    hcm "「Lekin, bunday mushakbozlikka odatda sevishganlar yoki do‘stlar keladi shekilli. Boyadan beri shular ko‘p ko‘rinyapti.」"

# game/scripts/game/OG_TALK_TOT3.rpy:394
translate uzbek OG_TALK_TOT3_11_36c378bb:

    # voice "audio/log/TOT3B6_TOT000.ogg"
    # tot "「そうだね」"
    voice "audio/log/TOT3B6_TOT000.ogg"
    tot "「Shunaqa.」"

# game/scripts/game/OG_TALK_TOT3.rpy:398
translate uzbek OG_TALK_TOT3_11_053cd916:

    # hcm "「まぁ、そうしたら一緒に来てる俺たちはなんなのかってことになるけど……」"
    hcm "「Unda, birga kelgan bizlar kim bo‘ldik...?」"

# game/scripts/game/OG_TALK_TOT3.rpy:400
translate uzbek OG_TALK_TOT3_11_bad93d73:

    # hcm "「友達……。そうか、友達か……」"
    hcm "「Do‘st... Ha, do‘st-da...」"

# game/scripts/game/OG_TALK_TOT3.rpy:402
translate uzbek OG_TALK_TOT3_11_f5702d24:

    # hcm "「え！？　友達じゃないとかそういうことじゃなくて、ほら、俺、友達っていたことないからよくわからないというか……」"
    hcm "「E!? Do‘st emasmiz demoqchi emasman, shunchaki menda do‘st bo‘lmagani uchun, yaxshi tushunmayman...」"

# game/scripts/game/OG_TALK_TOT3.rpy:404
translate uzbek OG_TALK_TOT3_11_0c74ea04:

    # voice "audio/log/TOT3B6_TOT001.ogg"
    # tot mca03 "「八幡っ！」"
    voice "audio/log/TOT3B6_TOT001.ogg"
    tot mca03 "「Hachiman!」"

# game/scripts/game/OG_TALK_TOT3.rpy:408
translate uzbek OG_TALK_TOT3_11_1d3afd60:

    # hcm "「は、はい！」"
    hcm "「Ha-ha!」"

# game/scripts/game/OG_TALK_TOT3.rpy:410
translate uzbek OG_TALK_TOT3_11_aced98b5:

    # voice "audio/log/TOT3B6_TOT002.ogg"
    # tot mca04 "「そういう考え方はよくないと思う。八幡と仲良くしたいって思ってる人だっているんだから、その人の気持ちも考えないと」"
    voice "audio/log/TOT3B6_TOT002.ogg"
    tot mca04 "「Bunday fikrlash yaxshi emas. Sen bilan inoq bo‘lishni istaydigan odamlar bor, ularning his-tuyg‘ularini ham o‘yla.」"

# game/scripts/game/OG_TALK_TOT3.rpy:414
translate uzbek OG_TALK_TOT3_11_2a0ce923:

    # hcm "「は、はい。そうですね……すいませんでした……」"
    hcm "「Ha, to‘g‘ri... Kechirasan...」"

# game/scripts/game/OG_TALK_TOT3.rpy:416
translate uzbek OG_TALK_TOT3_11_e80fb8aa:

    # voice "audio/log/TOT3B6_TOT003.ogg"
    # tot mca06 "「……」"
    voice "audio/log/TOT3B6_TOT003.ogg"
    tot mca06 "「...」"

# game/scripts/game/OG_TALK_TOT3.rpy:420
translate uzbek OG_TALK_TOT3_11_bd6bd7e2:

    # hcm "「あぅ……」"
    hcm "「Ah...」"

