    voice "audio/log/C042ESS0_TOT003.ogg"
    tot "A, hali shunaqami! Mana! Men ham qarab turmayman!"

# game/scripts/game/OG_C042ESS0.rpy:62
translate uzbek OG_C042ESS0_0_1d086fe0:

    # voice "audio/log/C042ESS0_HCM003.ogg"
    # hcm "「くっ、やるな戸塚。こうなったらいつも議論を放り投げて嫌われるように、戸塚も海に放り投げるしかないようだな……」"
    voice "audio/log/C042ESS0_HCM003.ogg"
    hcm "Obbo, bo'sh kelmayapsan-ku, Totsuka. Unday bo'lsa, xuddi bahslarni tashlab ketib, yomon ko'ringanim kabi, seni ham dengizga uloqtirishdan boshqa ilojim yo'q..."

# game/scripts/game/OG_C042ESS0.rpy:72
translate uzbek OG_C042ESS0_0_082252b4:

    # voice "audio/log/C042ESS0_TOT004.ogg"
    # tot "「わわっ、やだよ、つかまらないよ？」"
    voice "audio/log/C042ESS0_TOT004.ogg"
    tot "Voy-voy, yo'q, baribir tutolmaysan!"

# game/scripts/game/OG_C042ESS0.rpy:78
translate uzbek OG_C042ESS0_0_b45a7e46:

    # voice "audio/log/C042ESS0_HCM004.ogg"
    # hcm "「待て〜、逃げるな戸塚〜！」"
    voice "audio/log/C042ESS0_HCM004.ogg"
    hcm "To'xta~, qochma Totsuka~!"

# game/scripts/game/OG_C042ESS0.rpy:82
translate uzbek OG_C042ESS0_0_1d508cdc:

    # voice "audio/log/C042ESS0_TOT005.ogg"
    # tot "「あははっ、八幡、こっちだよ〜」"
    voice "audio/log/C042ESS0_TOT005.ogg"
    tot "Ahaha, Hachiman, bu yoqdaman~"

# game/scripts/game/OG_C042ESS0.rpy:92
translate uzbek OG_C042ESS0_0_41e673ee:

    # voice "audio/log/C042ESS0_KOM000.ogg"
    # kom "「お、お兄ちゃんと戸塚さんが青春してるラブラブカップルにしか見えない……こ、小町も！　小町も混ぜてっ！」"
    voice "audio/log/C042ESS0_KOM000.ogg"
    kom "A-akam va Totsuka-san xuddi yoshlik gashtini surayotgan sevishganlarga o'xshaydi-ya... K-Komachi ham! Komachini ham qo'shinglar!"

# game/scripts/game/OG_C042ESS0.rpy:96
translate uzbek OG_C042ESS0_0_c21e9213:

    # voice "audio/log/C042ESS0_HCM005.ogg"
    # hcm "「わかったわかった。じゃあ、小町から放り投げるから。ほれ、手」"
    voice "audio/log/C042ESS0_HCM005.ogg"
    hcm "Tushunarli, tushunarli. Unda birinchi Komachini uloqtiramiz. Qani, qo'lingni ber."

# game/scripts/game/OG_C042ESS0.rpy:106
translate uzbek OG_C042ESS0_0_9bf77bde:

