    # voice "audio/log/C042ESS0_KOM012.ogg"
    # kom "「あ、やっと笑ってくれましたね、よかった。戸塚さん、食べ終わったらまたいっぱい遊びましょうねっ」"
    voice "audio/log/C042ESS0_KOM012.ogg"
    kom "A, nihoyat kuldingiz-a, xayriyat. Totsuka-san, ovqatlanib bo'lgach, yana mazza qilib o'ynaymiz, xo'pmi?"

# game/scripts/game/OG_C042ESS0.rpy:378
translate uzbek OG_C042ESS0_0_5d967688:

    # voice "audio/log/C042ESS0_TOT015.ogg"
    # tot "「うん、そうだね。ありがとう小町ちゃん」"
    voice "audio/log/C042ESS0_TOT015.ogg"
