    voice "audio/log/C042ESS0_KOM002.ogg"
    kom "Voh-ho-hooo!"

# game/scripts/game/OG_C042ESS0.rpy:132
translate uzbek OG_C042ESS0_0_12d2c2e7:

    # voice "audio/log/C042ESS0_HCM006.ogg"
    # hcm "「しっかし……行列なげーなぁおい……」"
    voice "audio/log/C042ESS0_HCM006.ogg"
