    voice "audio/log/C042ESS0_HCM014.ogg"
    hcm "Totsuka...?"

# game/scripts/game/OG_C042ESS0.rpy:342
translate uzbek OG_C042ESS0_0_eaf24b97:

    # voice "audio/log/C042ESS0_KOM010.ogg"
    # kom "「あのですね、きっとそれはお腹がすいているからだと小町は思います！　美味しいものいっぱい買ってきたから、ドンドン食べて下さいねっ！」"
    voice "audio/log/C042ESS0_KOM010.ogg"
    kom "Bilmasam-u, Komachining fikricha, qorningiz ochib ketgani uchun shunday deyapsiz! Mazali narsalar olib keldik, to'yib-to'yib yeb oling!"

# game/scripts/game/OG_C042ESS0.rpy:352
translate uzbek OG_C042ESS0_0_b0bd48b2:

    # voice "audio/log/C042ESS0_TOT013.ogg"
    # tot "「小町ちゃん」"
    voice "audio/log/C042ESS0_TOT013.ogg"
