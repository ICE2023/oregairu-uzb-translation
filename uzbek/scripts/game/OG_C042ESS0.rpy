# game/scripts/game/OG_C042ESS0.rpy:146
translate uzbek OG_C042ESS0_0_787341b0:

    # voice "audio/log/C042ESS0_KOM003.ogg"
    # kom "「うぅ〜、売店がこんなに大繁盛だなんて……さっきから全然進まないよー」"
    voice "audio/log/C042ESS0_KOM003.ogg"
    kom "Uuu~, do'kon bunchalik gavjum bo'ladi deb o'ylamagandim... ancha vaqtdan beri umuman siljimayapti."

# game/scripts/game/OG_C042ESS0.rpy:150
translate uzbek OG_C042ESS0_0_8ec995c9:

    # voice "audio/log/C042ESS0_HCM007.ogg"
    # hcm "「昼のピークは外したつもりだったんだけどな……侮れねぇな、プールの売店」"
    voice "audio/log/C042ESS0_HCM007.ogg"
    hcm "Tushlik vaqtini o'tkazib keldik deb o'ylagandim... Basseyn do'konini kamsitib bo'lmas ekan."

# game/scripts/game/OG_C042ESS0.rpy:160
translate uzbek OG_C042ESS0_0_6690f861:

    # voice "audio/log/C042ESS0_KOM004.ogg"
    # kom "「戸塚さんにビーチパラソルの場所取りお願いしちゃったけど、大丈夫かな？　まだ時間がかかりそうですって、言ってきた方がいいかなぁ……」"
    voice "audio/log/C042ESS0_KOM004.ogg"
    kom "Totsuka-sandan soyabon tagidagi joyni egallab turishni iltimos qilgandik, o'zi qiynalib qolmadimikan? Yana ozroq vaqt ketishini aytib kelsammikan-a..."

# game/scripts/game/OG_C042ESS0.rpy:164
translate uzbek OG_C042ESS0_0_228826ea:

    # voice "audio/log/C042ESS0_HCM008.ogg"
    # hcm "「微妙だな。どっちかが言いに行ってる間に順番が来そうだし、そうなるとひとりで３人ぶんを持っていくのはちょっとキツイな」"
    voice "audio/log/C042ESS0_HCM008.ogg"
    hcm "Bilmadim. Birimiz aytgani ketgunimizcha navbatimiz kelib qolishi mumkin, unda bir kishi uch kishilik ovqatni ko'tarib ketishi qiyin bo'ladi."

# game/scripts/game/OG_C042ESS0.rpy:168
translate uzbek OG_C042ESS0_0_66881c5b:

    # voice "audio/log/C042ESS0_KOM005.ogg"
    # kom "「携帯はロッカーに置いてきちゃったし……ねぇお兄ちゃん、ちょっと戸塚さんとテレパシーで交信してみてよ」"
    voice "audio/log/C042ESS0_KOM005.ogg"
    kom "Telefonlarimiz shkafchada qolib ketgan... Hoy aka, Totsuka-san bilan telepatiya orqali gaplashib ko'ring-chi."

# game/scripts/game/OG_C042ESS0.rpy:172
translate uzbek OG_C042ESS0_0_eb7d295d:

    # voice "audio/log/C042ESS0_HCM009.ogg"
    # hcm "「俺と戸塚の愛の力を持ってすれば不可能じゃねぇけどな。まだ精度がイマイチなんだ、悪いな」"
    voice "audio/log/C042ESS0_HCM009.ogg"
    hcm "Men va Totsuka o'rtasidagi sevgi kuchi bilan bu imkonsiz emas-ku, lekin aniqligi hali biroz pastroq, uzr."

# game/scripts/game/OG_C042ESS0.rpy:182
