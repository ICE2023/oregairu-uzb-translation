# game/scripts/game/OG_TALK_YUI2.rpy:318
translate uzbek OG_TALK_YUI2_9_a53651cc:

    # voice "audio/log/YUI2B4_YUI000.ogg"
    # yui "「全然平気。ついでに買い物してくるわぁ〜って言ってたし、気にしないで」"
    voice "audio/log/YUI2B4_YUI000.ogg"
    yui "「Hechqisi yo‘q. Yo‘l-yo‘lakay xarid qilib kelaman dedi, xavotir olma.」"

# game/scripts/game/OG_TALK_YUI2.rpy:322
translate uzbek OG_TALK_YUI2_9_cdd78129:

    # voice "audio/log/YUI2B4_YUI001.ogg"
    # yui "「本当はヒッキーのお家に行きたがってたんだけど……さすがにいっしょにお邪魔すると迷惑かなって思って」"
    voice "audio/log/YUI2B4_YUI001.ogg"
    yui "「Aslida Hikkinikiga bormoqchi edi... lekin birga borish noqulay bo‘ladi deb o‘yladim.」"

# game/scripts/game/OG_TALK_YUI2.rpy:326
translate uzbek OG_TALK_YUI2_9_c1119121:

    # hcm "「親を連れて挨拶とか、行く家間違ってんだろ……」"
    hcm "「Ota-onasi bilan kelish... boshqa uyga borayotgandek tuyuladi...」"

# game/scripts/game/OG_TALK_YUI2.rpy:328
translate uzbek OG_TALK_YUI2_9_69379df7:

    # voice "audio/log/YUI2B4_YUI002.ogg"
    # yui mca02 "「ヘンな意味じゃないってば！　ママ、ヒッキーに会いたがってたから」"
    voice "audio/log/YUI2B4_YUI002.ogg"
    yui mca02 "「Noto‘g‘ri tushunma! Oyim Hikini ko‘rmoqchi edi.」"

# game/scripts/game/OG_TALK_YUI2.rpy:332
translate uzbek OG_TALK_YUI2_9_9e695cad:

    # hcm "「なんで俺？」"
    hcm "「Nega aynan men?」"

# game/scripts/game/OG_TALK_YUI2.rpy:334
translate uzbek OG_TALK_YUI2_9_5413ccc6:

    # voice "audio/log/YUI2B4_YUI003.ogg"
    # yui "「あたしが学校の話をするとよく出てくるし、それに……事故のとき、ヒッキーは入院しててちゃんとお礼言えなかったから」"
    voice "audio/log/YUI2B4_YUI003.ogg"
    yui "「Maktab haqida gapirganimda isming ko‘p chiqadi, keyin... avariya bo‘lganda, Hikki shifoxonada bo‘lgani uchun rahmat aytolmagan edi.」"

# game/scripts/game/OG_TALK_YUI2.rpy:338
translate uzbek OG_TALK_YUI2_9_c76a99d5:

    # hcm "「……終わったことだ、もう気を遣わないでくれって伝えておいてくれ」"
    hcm "「...O‘tib ketdi, endi xijolat bo‘lmasin deb aytib qo‘y.」"

# game/scripts/game/OG_TALK_YUI2.rpy:340
translate uzbek OG_TALK_YUI2_9_57c20c97:

    # voice "audio/log/YUI2B4_YUI004.ogg"
    # yui mca04 "「う、うん……でも」"
    voice "audio/log/YUI2B4_YUI004.ogg"
    yui "「X-xo‘p... lekin.」"

# game/scripts/game/OG_TALK_YUI2.rpy:344
translate uzbek OG_TALK_YUI2_9_a7c4369f:

    # hcm "「……」"
    hcm "「...」"

