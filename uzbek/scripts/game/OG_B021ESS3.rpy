# game/scripts/game/OG_B021ESS3.rpy:123
translate uzbek OG_B021ESS3_0_16d6f0c0:

    # voice "audio/log/B021ESS3_KOM004.ogg"
    # kom "「ほら、小６のお兄ちゃんかわいいー。この頃からお兄ちゃんは変わってないね。小学生なのに捻くれた腐った目をしてるよ……」"
