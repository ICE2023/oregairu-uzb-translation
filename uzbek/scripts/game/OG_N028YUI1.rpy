translate uzbek OG_N028YUI1_0_b3744abb:

    # voice "audio/log/N028YUI1_HCM004.ogg"
    # hcm "「なあ由比ケ浜、雪ノ下に誕生日にエプロンもらってたよな。あれ、活用してんのか？」"
    voice "audio/log/N028YUI1_HCM004.ogg"
