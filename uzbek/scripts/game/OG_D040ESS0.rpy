# game/scripts/game/OG_D040ESS0.rpy:30
translate uzbek OG_D040ESS0_0_ebda0f46:

    # voice "audio/log/D040ESS0_HCM000.ogg"
    # hcm "「……いや、その」"
    voice "audio/log/D040ESS0_HCM000.ogg"
    hcm "...Yo'q, shunchaki."

# game/scripts/game/OG_D040ESS0.rpy:34
translate uzbek OG_D040ESS0_0_0cfe78cb:

    # hcm "（まぁ、暇でやることもねぇしな。小町もいないし、水着見られないし）"
    hcm "(To'g'risi, bekorchiman-da. Komachi yo'q, suzish kiyimlarini ham ko'rolmayman.)"

# game/scripts/game/OG_D040ESS0.rpy:36
translate uzbek OG_D040ESS0_0_2e6be4b9:

    # voice "audio/log/D040ESS0_KAW001.ogg"
    # kaw "「……」"
    voice "audio/log/D040ESS0_KAW001.ogg"
    kaw "..."

# game/scripts/game/OG_D040ESS0.rpy:40
translate uzbek OG_D040ESS0_0_af5de5dd:

    # voice "audio/log/D040ESS0_HCM001.ogg"
    # hcm "「……っつーか、そっちこそ毎日見かける気がするけどな」"
    voice "audio/log/D040ESS0_HCM001.ogg"
    hcm "...Aytgancha, seni ham har kuni ko'rayotgandekman."

# game/scripts/game/OG_D040ESS0.rpy:44
translate uzbek OG_D040ESS0_0_90483d3b:

    # voice "audio/log/D040ESS0_KAW002.ogg"
    # kaw "「家で勉強してるとクーラーつけちゃうし。電気代勿体ないから、暇なときは自転車で来ることにしてる……うち、そんなに余裕ないから」"
    voice "audio/log/D040ESS0_KAW002.ogg"
    kaw "Uyda o'qisam konditsionerni yoqishga to'g'ri keladi. Elektr pulini tejash uchun, vaqtim bo'lganda velosipedda shu yerga kelishga qaror qildim... oilaviy byudjetimiz unchalik ham katta emas."

# game/scripts/game/OG_D040ESS0.rpy:48
translate uzbek OG_D040ESS0_0_94102507:

    # hcm "（なんか、俺と似たような理由だな。小町と水着は含まれてないけど）"
    hcm "(Negadir mening sababimga o'xshab ketarkan. Lekin Komachi va suzish kiyimi sabablar ichida yo'q.)"

# game/scripts/game/OG_D040ESS0.rpy:50
translate uzbek OG_D040ESS0_0_1fcc12c8:

    # voice "audio/log/D040ESS0_HCM002.ogg"
    # hcm "「……ま、まあな。ここ、涼しくて勉強に集中しやすいよな」"
    voice "audio/log/D040ESS0_HCM002.ogg"
    hcm "...Hm, ha. Bu yer salqin, o'qishga diqqatni jamlash oson."

# game/scripts/game/OG_D040ESS0.rpy:54
translate uzbek OG_D040ESS0_0_2f5af9e5:

    # voice "audio/log/D040ESS0_KAW003.ogg"
    # kaw "「だね……あのさ」"
    voice "audio/log/D040ESS0_KAW003.ogg"
    kaw "Shunday... haligi."

# game/scripts/game/OG_D040ESS0.rpy:58
translate uzbek OG_D040ESS0_0_9d89d00a:

    # voice "audio/log/D040ESS0_HCM003.ogg"
    # hcm "「なんだ？」"
    voice "audio/log/D040ESS0_HCM003.ogg"
    hcm "Nima?"

# game/scripts/game/OG_D040ESS0.rpy:62
translate uzbek OG_D040ESS0_0_fd26546a:

    # voice "audio/log/D040ESS0_KAW004.ogg"
    # kaw "「大志、よくあんたの話してるよ」"
    voice "audio/log/D040ESS0_KAW004.ogg"
    kaw "Taishi sen haqingda ko'p gapiradi."

# game/scripts/game/OG_D040ESS0.rpy:66
translate uzbek OG_D040ESS0_0_ef2f9862:

    # voice "audio/log/D040ESS0_HCM004.ogg"
    # hcm "「……なん、だと？」"
    voice "audio/log/D040ESS0_HCM004.ogg"
    hcm "...Nima deding?"

# game/scripts/game/OG_D040ESS0.rpy:70
translate uzbek OG_D040ESS0_0_8f131092:

    # hcm "（……あの毒虫が俺についてなんの話をするって言うんだ？　小町か？　小町のことか？）"
    hcm "(...U zaharli hasharot men haqimda nima deyishi mumkin? Komachi haqidami? Komachi haqida gapiryaptimi?)"

# game/scripts/game/OG_D040ESS0.rpy:78
translate uzbek OG_D040ESS0_0_beccaf4f:

    # voice "audio/log/D040ESS0_KAW005.ogg"
    # kaw "「……なに、いきなり目ぇ血走らせてんの」"
    voice "audio/log/D040ESS0_KAW005.ogg"
    kaw "...Nega birdan ko'zlaring qizarib ketdi?"

# game/scripts/game/OG_D040ESS0.rpy:82
translate uzbek OG_D040ESS0_0_9ecf4bbe:

    # voice "audio/log/D040ESS0_HCM005.ogg"
    # hcm "「あ？　いや、なんでも……てか、お前の弟がなんで俺を話題に出すんだよ」"
    voice "audio/log/D040ESS0_HCM005.ogg"
    hcm "A? Yo'q, hech narsa... To'g'risi, ukang nega men haqimda gapiradi?"

# game/scripts/game/OG_D040ESS0.rpy:92
translate uzbek OG_D040ESS0_0_4197bf7a:

    # voice "audio/log/D040ESS0_KAW006.ogg"
    # kaw "「まあ、前あんたに相談したとき、頼りになるとか思ったらしいんだけどさ」"
    voice "audio/log/D040ESS0_KAW006.ogg"
    kaw "Oldin senga maslahat solganida, ishonchli odam ekan deb o'ylabdi."

# game/scripts/game/OG_D040ESS0.rpy:96
translate uzbek OG_D040ESS0_0_0b2964eb:

    # voice "audio/log/D040ESS0_HCM006.ogg"
    # hcm "「別に俺はたいしたことはしてねえけどな。スカラシップとれたのはお前の力なんだろうし」"
    voice "audio/log/D040ESS0_HCM006.ogg"
    hcm "Men arzigulik ish qilganim yo'q. Grant yutganing o'z kuching evaziga bo'lsa kerak."

# game/scripts/game/OG_D040ESS0.rpy:100
translate uzbek OG_D040ESS0_0_a01cd50c:

    # voice "audio/log/D040ESS0_KAW007.ogg"
    # kaw "「それはそうだけど、まあ教えてくれたのはあんただしね。それで、今後もいろいろ教えて欲しいとか、なんかそんな感じで話してるよ」"
    voice "audio/log/D040ESS0_KAW007.ogg"
    kaw "U-ku shunday, lekin baribir sen o'rgatding-ku. Shuning uchun, kelajakda ham sendan ko'p narsani o'rganmoqchiligini aytib yuripti."

# game/scripts/game/OG_D040ESS0.rpy:104
translate uzbek OG_D040ESS0_0_81e7f836:

    # hcm "（なにを教えろと言うんだ……悪い虫に小町の事は教えん。一撃必中見敵殺虫サーチアンドデストロイだ）"
    hcm "(Nimani o'rgatarmishman... yomon hasharotga Komachi haqida hech narsa o'rgatmayman. Bir zarbada yo'q qilish, qidirish va yo'q qilish operatsiyasi.)"

# game/scripts/game/OG_D040ESS0.rpy:112
translate uzbek OG_D040ESS0_0_2a4cb019:

    # voice "audio/log/D040ESS0_KAW008.ogg"
    # kaw "「……だからなんで目ぇ血走らせてんのさ」"
    voice "audio/log/D040ESS0_KAW008.ogg"
    kaw "...Nega ko'zing qizaryapti dedim-ku."

# game/scripts/game/OG_D040ESS0.rpy:116
translate uzbek OG_D040ESS0_0_96ccc8fc:

    # voice "audio/log/D040ESS0_HCM007.ogg"
    # hcm "「気のせいだ……まあ、なんにせよ、弟とうまくやってるようで良かったな」"
    voice "audio/log/D040ESS0_HCM007.ogg"
    hcm "Senga shunday tuyulyapti... Mayli, nima bo'lganda ham, ukang bilan munosabatlaring yaxshiligidan xursandman."

# game/scripts/game/OG_D040ESS0.rpy:126
translate uzbek OG_D040ESS0_0_e9b3116a:

    # voice "audio/log/D040ESS0_KAW009.ogg"
    # kaw "「ああ、うん……そんだけ。じゃ、また」"
    voice "audio/log/D040ESS0_KAW009.ogg"
    kaw "Ha, xo'p... shuni aytmoqchi edim. Bo'pti, ko'rishguncha."

# game/scripts/game/OG_D040ESS0.rpy:130
translate uzbek OG_D040ESS0_0_fe6364b7:

    # voice "audio/log/D040ESS0_HCM008.ogg"
    # hcm "「……ああ。またな」"
    voice "audio/log/D040ESS0_HCM008.ogg"
    hcm "...Ha. Ko'rishguncha."

# game/scripts/game/OG_D040ESS0.rpy:136
translate uzbek OG_D040ESS0_0_c6788c01:

    # hcm "（……川崎、前に比べてなんか態度柔らかくなったような。気のせいかな……まあそんなことはどうでもいいな。さて、勉強しよ）"
    hcm "(...Kawasaki avvalgiga qaraganda biroz yumshab qolganga o'xshaydimi. Menga shunday tuyulyaptimikin... Mayli, buning ahamiyati yo'q. Qani, dars qilaylik.)"