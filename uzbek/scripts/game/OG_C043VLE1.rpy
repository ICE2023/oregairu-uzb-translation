translate uzbek OG_C043VLE1_aedd3604:



    voice "audio/log/C043VLE1_KOM000.ogg"
    kom_nvl "{#car_width=240}「Uf, juda charchadim... Basseyn juda mazza bo'ldi.」"


translate uzbek OG_C043VLE1_3e503c7d:



    voice "audio/log/C043VLE1_HCM000.ogg"
    hcm_nvl "{#car_width=240}「Ha, ajoyib bo'ldi. Ayniqsa Totsuka. Tch, nega biz bir avtobusda emasmiz?」"


translate uzbek OG_C043VLE1_277c7256:



    voice "audio/log/C043VLE1_KOM001.ogg"
    kom_nvl "{#car_width=320}「To'g'rirog'i, nega Komachi sen bilan avtobusda ketishi kerak? Eshit, aka. Menda shubhalar paydo bo'lyapti.」"


translate uzbek OG_C043VLE1_5a2343fa:



    voice "audio/log/C043VLE1_HCM001.ogg"
    hcm_nvl "{#car_width=180}「A? Nima haqida?」"


