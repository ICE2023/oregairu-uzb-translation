translate uzbek OG_C043VLE1_41969e5a:



    voice "audio/log/C043VLE1_KOM002.ogg"
    kom_nvl "{#car_width=240}「Totsuka-san va akam... qandaydir shubhali.」"


translate uzbek OG_C043VLE1_2cce10a0:



    voice "audio/log/C043VLE1_HCM002.ogg"
    hcm_nvl "{#car_width=240}「A!? T-t-t-tentakmisan!? B-bizda hech qanday shubhali narsa yo'q!」"


translate uzbek OG_C043VLE1_b1634a04:



    voice "audio/log/C043VLE1_KOM003.ogg"
    kom_nvl "{#car_width=320}「Besh martacha tutilib qolding... Qanchalik vahimaga tushyapsan o'zi? Hoy, Totsuka-san bilan orangizda biror narsa bo'ldimi?」"


translate uzbek OG_C043VLE1_6e3ae4dc:



    voice "audio/log/C043VLE1_HCM003.ogg"
    hcm_nvl "{#car_width=240}「H-hech narsa... hozircha.」"


