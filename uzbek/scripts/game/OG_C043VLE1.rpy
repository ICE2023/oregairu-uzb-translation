    nvl clear


translate uzbek OG_C043VLE1_706227f9:



    voice "audio/log/C043VLE1_KOM004.ogg"
    kom_nvl "{#car_width=240}「Hozircha!? Nima qilishni rejalashtiryapsan o'zi...」"


translate uzbek OG_C043VLE1_4de75887:



    voice "audio/log/C043VLE1_HCM004.ogg"
    hcm_nvl "{#car_width=320}「Ko'ramiz... Nima qilsam ekan? Qo'limdan hamma narsa keladigandek his qilyapman. Bilasanmi, Komachi? Dunyo umid va orzularga to'la.」"


translate uzbek OG_C043VLE1_d1e55b79:



    voice "audio/log/C043VLE1_KOM005.ogg"
    kom_nvl "{#car_width=240}「A! Akam buzilib qoldi...」"


translate uzbek OG_C043VLE1_ada9b126:



    voice "audio/log/C043VLE1_HCM005.ogg"
    hcm_nvl "{#car_width=240}「Buzilganim uchun ham senga bir-ikki narsani o'rgatishim mumkin. Masalan, haqiqiy baxt haqida.」"


translate uzbek OG_C043VLE1_76b2fe88_2:


    nvl clear


translate uzbek OG_C043VLE1_d2d2796b:



    voice "audio/log/C043VLE1_KOM006.ogg"
    kom_nvl "{#car_width=400}「U butunlay buzilibdi... Garchi u yaxshi gaplarni aytayotgandek tuyulsa ham... To'g'ri, Komachi har doim akasining normal inson bo'lishini xohlagan, lekin u biroz noto'g'ri tomonga ketib qolgandek... Komachi xavotirda...」"


translate uzbek OG_C043VLE1_85290148:



    voice "audio/log/C043VLE1_HCM006.ogg"
    hcm_nvl "{#car_width=320}「Nimalarni g'uldirayapsan? Buni qo'y, kechki ovqatga nima yeymiz?」"


translate uzbek OG_C043VLE1_46b1b63b:



    voice "audio/log/C043VLE1_KOM007.ogg"
    kom_nvl "{#car_width=320}「Komachi shunchalik xavotirdaki, ovqat tomog'idan o'tmay qolishi mumkin. Isitilgan xitoycha taom bo'la qolsin.」"


translate uzbek OG_C043VLE1_101282a2:




    hcm_nvl "{#car_width=320}「Menimcha, bu haqiqiy ziyofat. Unda bir-ikki kun oldingi xitoycha taom bo'ladi. Kunjutli sous juda mazali...」"
