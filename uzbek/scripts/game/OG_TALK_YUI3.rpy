# game/scripts/game/OG_TALK_YUI3.rpy:384
translate uzbek OG_TALK_YUI3_11_de9c4202:

    # hcm "「……悪い。話すことが全く思い当たらない」"
    hcm "「...Uzr. Nima haqida gapirishni umuman bilmayapman.」"

# game/scripts/game/OG_TALK_YUI3.rpy:386
