    # yui mcc07 "「……そう、見えちゃうかな」"
    voice "audio/log/YUI3B5_YUI003.ogg"
    yui mcc07 "「...Shunaqa ko‘rinarmikinmiz.」"

# game/scripts/game/OG_TALK_YUI3.rpy:360
translate uzbek OG_TALK_YUI3_10_cac9ba9d:

    # hcm "「悪いな、俺で。まぁ、勘弁してくれ」"
    hcm "「Uzr, men bo‘lganim uchun. Endi, kechirasan.」"

# game/scripts/game/OG_TALK_YUI3.rpy:362
translate uzbek OG_TALK_YUI3_10_18197d72:

