# game/scripts/game/OG_N028TOT0.rpy:68
translate uzbek OG_N028TOT0_1_fec7ded7:

    # voice "audio/log/N028TOT0_HCM001.ogg"
    # hcm "「あ、いや、そ、そうだな。結構熱かったよな、あの風呂」"
    voice "audio/log/N028TOT0_HCM001.ogg"
    hcm "「A, yo‘q, shu-shunday. Hammom ancha issiq edi-a?」"

# game/scripts/game/OG_N028TOT0.rpy:81
translate uzbek OG_N028TOT0_1_dc90a566:

    # voice "audio/log/N028TOT0_TOT004.ogg"
    # tot "「ねー。でも、いいお湯だったよね！　あ、そういえば八幡、結局、材木座くん待たずにお風呂入っちゃったんだね」"
    voice "audio/log/N028TOT0_TOT004.ogg"
    tot "「Ha-a. Lekin suv yaxshi ekan! A, aytgancha Hachiman, oxiri Zaimokuzani kutmasdan kirib ketdingmi?」"

# game/scripts/game/OG_N028TOT0.rpy:85
translate uzbek OG_N028TOT0_1_b201f033:

    # voice "audio/log/N028TOT0_HCM002.ogg"
    # hcm "「あー。いつまでもうんうん唸ってたからな、小説のネタだかなんだか知らんが」"
    voice "audio/log/N028TOT0_HCM002.ogg"
    hcm "「Ha-a. U tinmay g‘udranib o‘tirgandi-da, romaniga mavzu topyaptimi, bilmadim.」"

# game/scripts/game/OG_N028TOT0.rpy:92
translate uzbek OG_N028TOT0_1_6ccccd0d:

    # voice "audio/log/N028TOT0_TOT005.ogg"
    # tot "「そっか。すごいね、材木座くん。でも、そしたら、やっぱり八幡といっしょにお風呂行けたら良かったな……」"
    voice "audio/log/N028TOT0_TOT005.ogg"
    tot "「Shunaqami. Zaimokuza zo‘r-a. Lekin, unda Hachiman bilan birga hammomga borganimda yaxshi bo‘lardi...」"

# game/scripts/game/OG_N028TOT0.rpy:96
translate uzbek OG_N028TOT0_1_a5179f18:

    # hcm "（え？　なにそれ？　俺どうしよう、『今からいっしょに風呂に行くか』とか言ってもいい？　言っちゃう！？　ごめんやっぱ無理！　はずかしくて俺が死ぬ！）"
    hcm "（E? Nima deding? Nima qilsam ekan, “Hozir birga boramizmi” desammikin? Aytib ko‘raymi!? Yo‘q, baribir aytolmayman! Uyalib o‘laman!）"

# game/scripts/game/OG_N028TOT0.rpy:104
translate uzbek OG_N028TOT0_1_9947a684:

    # voice "audio/log/N028TOT0_TOT006.ogg"
    # tot "「ああやって旅先でみんなとお風呂に入ったりとか、なんか、楽しいもんね」"
