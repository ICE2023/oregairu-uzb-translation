# game/scripts/game/OG_N028TOT0.rpy:45
translate uzbek OG_N028TOT0_0_177d4d1d:

    # voice "audio/log/N028TOT0_TOT002.ogg"
    # tot "「うん、ぼくも散歩。なんか、お風呂入ったら、のぼせちゃってさ」"
    voice "audio/log/N028TOT0_TOT002.ogg"
    tot "「Ha, men ham. Hammomdan keyin boshim aylanib ketgandek bo‘ldi.」"

# game/scripts/game/OG_N028TOT0.rpy:49
translate uzbek OG_N028TOT0_0_90478a9c:

    # hcm "（あ……そこはかとなく漂うシャンプーの香り、湯上りで上気した肌、湿った髪！　おい、よせよ、思わず見つめちゃうだろ……）"
    hcm "（A... shampunning yoqimli hidi, hammomdan chiqqanidan keyingi qizargan yuz, ho‘l sochlar! Bo‘ldi qil, ko‘zimni uzolmay qolyapman...）"

# game/scripts/game/OG_N028TOT0.rpy:57
