# game/scripts/game/OG_B033ESS1.rpy:63
translate uzbek OG_B033ESS1_0_f71bcc70:

    # voice "audio/log/B033ESS1_YUI003.ogg"
    # yui "「小さい頃にさ、人魚に憧れたんだよね。ほら、ディスティニーランドにも人魚のキャラクターがいるでしょ？」"
    voice "audio/log/B033ESS1_YUI003.ogg"
    yui "「Yoshligimda, suv parisi bo‘lishni orzu qilardim. Axir Disneylendda ham suv parisi bor-ku?」"

# game/scripts/game/OG_B033ESS1.rpy:67
translate uzbek OG_B033ESS1_0_17d40f45:

    # voice "audio/log/B033ESS1_HCM003.ogg"
    # hcm "「ああ……そういえば、小町がグッズを持ってたような」"
    voice "audio/log/B033ESS1_HCM003.ogg"
    hcm "「Ha... aytgancha, Komachida ham uning o‘yinchoqlari bor edi.」"

# game/scripts/game/OG_B033ESS1.rpy:71
translate uzbek OG_B033ESS1_0_e716df08:

    # voice "audio/log/B033ESS1_YUI004.ogg"
    # yui "「で、友達みんなとプールで人魚ごっこをやることになって、珍しくあたしが主役の人魚姫をできることになったの」"
    voice "audio/log/B033ESS1_YUI004.ogg"
    yui "「Xullas, dugonalarim bilan hovuzda suv parisi o‘yinini o‘ynaydigan bo‘ldik. Omadim kelib, men bosh qahramon – suv parisi bo‘ladigan bo‘ldim.」"

# game/scripts/game/OG_B033ESS1.rpy:75
