    voice "audio/log/D010ESS0_KOM002.ogg"
    kom "Tinchgina emas... aslo tinchgina emas! Axir, aka, sen uni tuzukroq o'qimading ham-ku!"

# game/scripts/game/OG_D010ESS0.rpy:56
translate uzbek OG_D010ESS0_0_4a543553:

    # voice "audio/log/D010ESS0_HCM002.ogg"
    # hcm "「山とか女子とか水着とかウフフとか書いてあったのは見たぞ」"
    voice "audio/log/D010ESS0_HCM002.ogg"
    hcm "Tog', qizlar, suzish kiyimi, uhu-hu degan joylarini ko'rdim-ku."

# game/scripts/game/OG_D010ESS0.rpy:66
translate uzbek OG_D010ESS0_0_cf89a750:

    # voice "audio/log/D010ESS0_KOM003.ogg"
