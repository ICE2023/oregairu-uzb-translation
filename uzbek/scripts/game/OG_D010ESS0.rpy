    # voice "audio/log/D010ESS0_HCM010.ogg"
    # hcm "「……はあ」"
    voice "audio/log/D010ESS0_HCM010.ogg"
    hcm "...Xo'sh."

# game/scripts/game/OG_D010ESS0.rpy:162
translate uzbek OG_D010ESS0_0_b06e013f:

    # voice "audio/log/D010ESS0_KOM011.ogg"
    # kom "「それもすべてこの自分へのご褒美のためでした！」"
    voice "audio/log/D010ESS0_KOM011.ogg"
    kom "Bularning bari o'zimni mukofotlash uchun edi!"

# game/scripts/game/OG_D010ESS0.rpy:166
translate uzbek OG_D010ESS0_0_5dae8c0c:

    # voice "audio/log/D010ESS0_HCM011.ogg"
