# game/scripts/game/OG_D010ESS0.rpy:190
translate uzbek OG_D010ESS0_0_b19568a3:

    # voice "audio/log/D010ESS0_KOM013.ogg"
    # kom "「お兄ちゃん、ホントにいいの？　小町ひとりで行っちゃうよ？　ウフフしてきちゃうよ？　今からでも先生にごめんなさいしたら行けるかもよ？」"
    voice "audio/log/D010ESS0_KOM013.ogg"
    kom "Aka, rostdan ham maylimi? O'zim yolg'iz ketamanmi? Mazza qilib kelamanmi? Hozir ham senseydan uzr so'rasang, borishing mumkin-ku?"

# game/scripts/game/OG_D010ESS0.rpy:194
translate uzbek OG_D010ESS0_0_1aeca0c3:

    # hcm "（……うう。そう言われると……どうしよう。いや。ここは初志貫徹だ。俺にはウフフなんて縁がない！）"
    hcm "(...Eh. Bunday deyishsa... nima qilsam ekan. Yo'q. Bir so'zli bo'lish kerak. Menga bunaqa o'yin-kulgilar yarashmaydi!)"

# game/scripts/game/OG_D010ESS0.rpy:196
