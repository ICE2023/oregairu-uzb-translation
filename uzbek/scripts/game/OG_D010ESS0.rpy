# game/scripts/game/OG_D010ESS0.rpy:88
translate uzbek OG_D010ESS0_0_f1a6eb2d:

    # voice "audio/log/D010ESS0_KOM005.ogg"
    # kom "「いいから、もう一度ちゃんと先生のメール読んで！　読まないと一生後悔するよ！」"
    voice "audio/log/D010ESS0_KOM005.ogg"
    kom "Bo'ldi qil, sensening xatini yana bir marta yaxshilab o'qi! O'qimasang, bir umr afsuslanasan!"

# game/scripts/game/OG_D010ESS0.rpy:92
translate uzbek OG_D010ESS0_0_569cc6fe:

    # voice "audio/log/D010ESS0_HCM005.ogg"
    # hcm "「な、なんだよ大袈裟な。ちょっと待て、もうひと目だけちらっと見るから……うん？」"
    voice "audio/log/D010ESS0_HCM005.ogg"
    hcm "N-buncha vahima qilmasang. Shoshmay tur, yana bir ko'z yugurtirib chiqaman... hm?"

# game/scripts/game/OG_D010ESS0.rpy:104
translate uzbek OG_D010ESS0_0_3ccc2d05:

    # hcm "（こ、これは……さらにメールが５件ほど追加できてるし。ホントにスパムかストーカーレベルだろ、怖すぎィ！）"
    hcm "(Bu-bu nima... yana 5 tacha xat kelibdi-ku. Haqiqiy spam yoki stalker darajasi, juda qo'rqinchli!)"

# game/scripts/game/OG_D010ESS0.rpy:106
translate uzbek OG_D010ESS0_0_06bc3f30:

    # voice "audio/log/D010ESS0_KOM006.ogg"
