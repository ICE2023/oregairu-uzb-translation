# game/scripts/game/OG_D010ESS0.rpy:70
translate uzbek OG_D010ESS0_0_a4f1ac86:

    # voice "audio/log/D010ESS0_HCM003.ogg"
    # hcm "「いや。だって、そんなん信じる方がどうかしてるだろ。ブラック企業に限っていい募集条件書いてるもんだし」"
    voice "audio/log/D010ESS0_HCM003.ogg"
    hcm "Yo'q. Axir, bunaqa narsalarga ishonish g'alati-ku. Qora kompaniyalar ham shunaqa zo'r shartlarni yozib qo'yishadi."

# game/scripts/game/OG_D010ESS0.rpy:80
translate uzbek OG_D010ESS0_0_27ad3186:

    # voice "audio/log/D010ESS0_KOM004.ogg"
    # kom "「今回は信じて！　平塚先生を信じてあげて！　ビリーブ・ミーだよ、お兄ちゃん！」"
    voice "audio/log/D010ESS0_KOM004.ogg"
    kom "Bu safar ishon! Hiratsuka-senseyga ishonib ko'r! 'Believe me', aka!"

# game/scripts/game/OG_D010ESS0.rpy:84
translate uzbek OG_D010ESS0_0_2d9c8855:

    # voice "audio/log/D010ESS0_HCM004.ogg"
    # hcm "「そもそも、あの人が甘いことを言うときは信用ならんのだが……」"
    voice "audio/log/D010ESS0_HCM004.ogg"
