# game/scripts/game/OG_D010ESS0.rpy:176
translate uzbek OG_D010ESS0_0_6bc08e94:

    # voice "audio/log/D010ESS0_KOM012.ogg"
    # kom "「すべてはお兄ちゃんといっしょに合宿へおでかけするためだったのに……それをお兄ちゃんが、自分でなしにするなんて！　小町ショック！　大ショック！」"
    voice "audio/log/D010ESS0_KOM012.ogg"
    kom "Hammasi akam bilan birga yig'inga borish uchun edi... sen esa buni o'z qo'ling bilan yo'qqa chiqarding! Komachi shokda! Qattiq shokda!"

# game/scripts/game/OG_D010ESS0.rpy:180
translate uzbek OG_D010ESS0_0_01bbf760:

    # voice "audio/log/D010ESS0_HCM012.ogg"
