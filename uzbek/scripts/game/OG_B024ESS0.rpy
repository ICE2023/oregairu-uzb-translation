    voice "audio/log/B024ESS0_HCM000.ogg"
    hcm "「Nimasi deganing nimasi, xuddi shunday bo‘ldi. Yukatada o‘tirsa bo‘ladigan joy qidirib yurgandik, yo‘qotib qo‘ydim.」"

# game/scripts/game/OG_B024ESS0.rpy:28
