    voice "audio/log/B024ESS0_KOM001.ogg"
    kom "「Telefon-chi? Bog‘lana olmadingizmi?」"

# game/scripts/game/OG_B024ESS0.rpy:32
translate uzbek OG_B024ESS0_0_a56d8115:

    # voice "audio/log/B024ESS0_HCM001.ogg"
    # hcm "「混雑しすぎて、通話どころかメールも送れねぇ……これだって公衆電話からかけてるぐらいだ」"
    voice "audio/log/B024ESS0_HCM001.ogg"
