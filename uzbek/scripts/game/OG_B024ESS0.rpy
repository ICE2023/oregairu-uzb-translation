    # voice "audio/log/B024ESS0_KOM002.ogg"
    # kom "「もうお兄ちゃん、なにやってんの！」"
    voice "audio/log/B024ESS0_KOM002.ogg"
    kom "「E akajon-e, nimalar qilib yurbsiz o‘zi!」"

# game/scripts/game/OG_B024ESS0.rpy:40
