# game/scripts/game/OG_B022ESS3.rpy:16
translate uzbek OG_B022ESS3_0_0e7edfd2:

    # hcm "（人が多いな。会場は別の駅だっつーのに、すでにいつもと雰囲気が違うというか……）"
    hcm "（Odam juda ko‘p-ku. Tomosha joyi boshqa bekatda bo‘lsa ham, allaqachon muhit o‘zgacha...）"

# game/scripts/game/OG_B022ESS3.rpy:18
translate uzbek OG_B022ESS3_0_9fbd2beb:

    # hcm "（そんなにみんな花火が見たいもんかね。というより、花火にかこつけて盛り上がりたいってだけなんだろうけど）"
    hcm "（Mushakbozlikni hamma shunchalik ko‘rgisi kelarmikin? To‘g‘rirog‘i, mushakbozlik bahona shunchaki xursandchilik qilishmoqchi bo‘lsa kerak.）"

# game/scripts/game/OG_B022ESS3.rpy:20
translate uzbek OG_B022ESS3_0_34baa669:

    # hcm "（由比ケ浜は……来てないみたいだな。適当にこのあたりで待つか）"
    hcm "（Yuigahama... hali kelmaganga o‘xshaydi. Shu atrofda kutib tursammikin.）"

# game/scripts/game/OG_B022ESS3.rpy:28
translate uzbek OG_B022ESS3_0_cde5ecfb:

    # hcm "（さっき通った集団は……確かウチの学校のヤツらだな。チラチラこっち見てたし）"
    hcm "（Boya o‘tib ketgan guruh... bizning maktabdan bo‘lsa kerak. Menga qarab-qarab qo‘yishdi.）"

# game/scripts/game/OG_B022ESS3.rpy:36
translate uzbek OG_B022ESS3_0_56c7641c:

    # hcm "（知り合いじゃないから別に関係ないんだが……ぼっち慣れした俺でも微妙に居心地が悪いのは、こういう浮かれイベントに慣れてないからか？）"
    hcm "（Tanish bo‘lmaganim uchun menga farqi yo‘q-u... lekin yolg‘izlikka o‘rgangan bo‘lsam ham, o‘zimni noqulay his qilyapman. Balki bunaqa shavvoz tadbirlarga o‘rganmaganim uchundir?）"

# game/scripts/game/OG_B022ESS3.rpy:38
translate uzbek OG_B022ESS3_0_a7c4369f:

    # hcm "「……」"
    hcm "「......」"

# game/scripts/game/OG_B022ESS3.rpy:46
translate uzbek OG_B022ESS3_0_a7c4369f_1:

    # hcm "「……」"
    hcm "「......」"

# game/scripts/game/OG_B022ESS3.rpy:54
translate uzbek OG_B022ESS3_0_ce74bee1:

    # hcm "（……っていうか、由比ケ浜遅ぇ……約束の時間はとっくに過ぎてるぞ）"
    hcm "（...O‘zi, Yuigahama buncha kechikdi... Kelishilgan vaqt ancha o‘tib ketdi.）"

# game/scripts/game/OG_B022ESS3.rpy:62
translate uzbek OG_B022ESS3_0_ad5f745d:

    # hcm "（電話してみるか……）"
    hcm "（Qo‘ng‘iroq qilib ko‘raymi...）"

# game/scripts/game/OG_B022ESS3.rpy:74
translate uzbek OG_B022ESS3_0_c5a4edf4:

    # voice "audio/log/b022ess3_X26000.ogg"
    # X26 "「おかけになった電話は、電波の繋がらない場所におられるか、電源が入っていないため――」"
    voice "audio/log/b022ess3_X26000.ogg"
    X26 "「Siz qo‘ng‘iroq qilayotgan raqam xizmat doirasidan tashqarida yoki telefon o‘chirilgan——」"

# game/scripts/game/OG_B022ESS3.rpy:78
translate uzbek OG_B022ESS3_0_955d0e99:

    # voice "audio/log/B022ESS3_HCM000.ogg"
    # hcm "「あ？　なんでだよ」"
    voice "audio/log/B022ESS3_HCM000.ogg"
    hcm "「A? Nega o‘chirilgan?」"

# game/scripts/game/OG_B022ESS3.rpy:84
translate uzbek OG_B022ESS3_0_45f8aecf:

    # voice "audio/log/b022ess3_X26001.ogg"
    # X26 "「おかけになった電話は、電波の――」"
    voice "audio/log/b022ess3_X26001.ogg"
    X26 "「Siz qo‘ng‘iroq qilayotgan raqam xizmat doirasidan tashqarida——」"

# game/scripts/game/OG_B022ESS3.rpy:88
translate uzbek OG_B022ESS3_0_384ee80e:

    # hcm "（なんで電源切ってるんだよ、由比ケ浜のヤツ。次にかけて出なかったら帰るぞ、俺は）"
    hcm "（Nega telefonini o‘chirib qo‘ydi, Yuigahama. Yana bir marta qo‘ng‘iroq qilaman, ko‘tarmasa uyga ketaman.）"

# game/scripts/game/OG_B022ESS3.rpy:94
translate uzbek OG_B022ESS3_0_228f7b6d:

    # hcm "（お、やっとかかったか）"
    hcm "（O, nihoyat tushdim.）"

# game/scripts/game/OG_B022ESS3.rpy:100
translate uzbek OG_B022ESS3_0_1670e4ba:

    # voice "audio/log/B022ESS3_YUI000.ogg"
    # yui "「あ、ヒッキー？　いまどこ？」"
    voice "audio/log/B022ESS3_YUI000.ogg"
    yui "「A, Hikki? Hozir qayerdasan?」"

# game/scripts/game/OG_B022ESS3.rpy:104
translate uzbek OG_B022ESS3_0_f6700fa9:

    # voice "audio/log/B022ESS3_HCM001.ogg"
    # hcm "「今どこ？　じゃねぇよ。さっきからずっと海浜幕張にいるぞ。お前こそ今どこだよ」"
    voice "audio/log/B022ESS3_HCM001.ogg"
    hcm "「Qayerda bo‘lardim. Boyadan beri Kaihinmakuhari bekatidaman. O‘zing qayerdasan?」"

# game/scripts/game/OG_B022ESS3.rpy:108
translate uzbek OG_B022ESS3_0_78b7f4e8:

    # voice "audio/log/B022ESS3_YUI001.ogg"
    # yui "「えっ？　待ち合わせ、現地集合って言ったよね……？」"
    voice "audio/log/B022ESS3_YUI001.ogg"
    yui "「E? Uchrashuvni o‘sha yerda ko‘rishamiz demaganmidik...?」"

# game/scripts/game/OG_B022ESS3.rpy:112
translate uzbek OG_B022ESS3_0_72bae513:

    # voice "audio/log/B022ESS3_HCM002.ogg"
    # hcm "「は？」"
    voice "audio/log/B022ESS3_HCM002.ogg"
    hcm "「A?」"

# game/scripts/game/OG_B022ESS3.rpy:116
translate uzbek OG_B022ESS3_0_60ba5369:

    # hcm "（そんなはずねぇだろ。確かに海浜幕張駅前集合って……）"
    hcm "（Bo‘lishi mumkin emas. Axir Kaihinmakuhari bekati oldida deb kelishgandik...）"

# game/scripts/game/OG_B022ESS3.rpy:118
translate uzbek OG_B022ESS3_0_be3f567a:

    # hcm "（いや、違う！　俺が駅前集合を面倒に思うだろうから、由比ケ浜はわざわざ現地集合って言ったんじゃ……？）"
    hcm "（Yo‘q, to‘xta! Bekat oldiga borishga erinaman deb aytganim uchun, Yuigahama to‘g‘ridan-to‘g‘ri o‘sha yerda ko‘rishamiz degandi-ku...?）"

# game/scripts/game/OG_B022ESS3.rpy:120
translate uzbek OG_B022ESS3_0_b1ea5b05:

    # voice "audio/log/B022ESS3_HCM003.ogg"
    # hcm "「悪い、素で間違えた！　お前、いま現地にいるんだな？」"
    voice "audio/log/B022ESS3_HCM003.ogg"
    hcm "「Uzr, men adashibman! Hozir o‘sha yerdamisan?」"

# game/scripts/game/OG_B022ESS3.rpy:124
translate uzbek OG_B022ESS3_0_a406fe3f:

    # voice "audio/log/B022ESS3_YUI002.ogg"
    # yui "「うん」"
    voice "audio/log/B022ESS3_YUI002.ogg"
    yui "「Ha.」"

# game/scripts/game/OG_B022ESS3.rpy:128
translate uzbek OG_B022ESS3_0_2bcbd24d:

    # voice "audio/log/B022ESS3_HCM004.ogg"
    # hcm "「今からすぐ行くから、そっちで待っててくれ」"
    voice "audio/log/B022ESS3_HCM004.ogg"
    hcm "「Hozir yetib boraman, o‘sha yerda kutib tur.」"

# game/scripts/game/OG_B022ESS3.rpy:132
translate uzbek OG_B022ESS3_0_eb22b1f4:

    # voice "audio/log/B022ESS3_YUI003.ogg"
    # yui "「もう、しょうがないなぁ……早く来てね」"
    voice "audio/log/B022ESS3_YUI003.ogg"
    yui "「E-e, iloj yo‘q-da... Tezroq kel.」"

# game/scripts/game/OG_B022ESS3.rpy:138
translate uzbek OG_B022ESS3_0_d72db9d9:

    # hcm "（やべぇ。マジですまん、由比ケ浜！）"
    hcm "（Yomon bo‘ldi. Rostdan ham uzr, Yuigahama!）"