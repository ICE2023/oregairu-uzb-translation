# game/scripts/game/OG_D051ESS3.rpy:18
translate uzbek OG_D051ESS3_0_03b2fda7:

    # voice "audio/log/D051ESS3_HCM000.ogg"
    # hcm "「ああ……まあ、しいて言うならあれだ。そこそこ自由な校風じゃねえか？　まあ、制服はあるが髪型とかは自由だしな」"
    voice "audio/log/D051ESS3_HCM000.ogg"
    hcm "Ha... agar aytish kerak bo'lsa, bu maktabdagi erkin muhit. Forma bor-u, lekin soch turmagiga cheklov yo'q."

# game/scripts/game/OG_D051ESS3.rpy:22
translate uzbek OG_D051ESS3_0_d4e24606:

    # hcm "（別に俺的にはそこはどうでもいいけどな。むしろ三浦みたいなのがどんどん増殖するだけで、かえって怖かったりもするし）"
    hcm "(Menga buning farqi yo'q. Aksincha, Miuraga o'xshaganlar ko'payib ketib, odamni battar qo'rqitadi.)"

# game/scripts/game/OG_D051ESS3.rpy:24
translate uzbek OG_D051ESS3_0_6e0e9097:

    # voice "audio/log/D051ESS3_TAI000.ogg"
    # tai "「ああ、まあ、うちの姉ちゃんとか見てればそれはわかるっすね」"
    voice "audio/log/D051ESS3_TAI000.ogg"
    tai "Ha, opamga qarab buni tushunsa bo'ladi."

# game/scripts/game/OG_D051ESS3.rpy:34
translate uzbek OG_D051ESS3_0_fa9c3602:

    # voice "audio/log/D051ESS3_KAW000.ogg"
    # kaw "「……どういう意味」"
    voice "audio/log/D051ESS3_KAW000.ogg"
    kaw "...Bu nima deganing?"

# game/scripts/game/OG_D051ESS3.rpy:44
translate uzbek OG_D051ESS3_0_060dd9e2:

    # voice "audio/log/D051ESS3_TAI001.ogg"
    # tai "「だって姉ちゃん結構派手じゃん、見た目」"
    voice "audio/log/D051ESS3_TAI001.ogg"
    tai "Axir, ko'rinishing ancha o'zgacha-ku."

# game/scripts/game/OG_D051ESS3.rpy:48
translate uzbek OG_D051ESS3_0_e783f5e7:

    # hcm "（見た目が派手と言うか、主に目が怖いんだよな川崎は。あとオーラも怖い）"
    hcm "(Ko'rinishi o'zgacha deymizmi, asosiysi Kawasakining ko'zlari qo'rqinchli. Aurasi ham vahimali.)"

# game/scripts/game/OG_D051ESS3.rpy:58
translate uzbek OG_D051ESS3_0_aab614b8:

    # voice "audio/log/D051ESS3_KOM000.ogg"
    # kom "「自由な校風、いいよね！　小町も憧れてるんだ！　もし総武高入れたら、いろんな髪型できるし、楽しいよね！」"
    voice "audio/log/D051ESS3_KOM000.ogg"
    kom "Erkin muhit, zo'r-a! Komachi ham shuni orzu qiladi! Agar Sobuga kirsam, sochimni har xil turmaklashim mumkin, juda qiziq bo'ladi!"

# game/scripts/game/OG_D051ESS3.rpy:62
translate uzbek OG_D051ESS3_0_ef9c7559:

    # voice "audio/log/D051ESS3_HCM001.ogg"
    # hcm "「……その髪型とやら、兄が逐一チェックをしてもいいか」"
    voice "audio/log/D051ESS3_HCM001.ogg"
    hcm "...O'sha soch turmaklarini akang birma-bir tekshirib tursa maylimi?"

# game/scripts/game/OG_D051ESS3.rpy:72
translate uzbek OG_D051ESS3_0_fd0088f3:

    # voice "audio/log/D051ESS3_KOM001.ogg"
    # kom "「えー？　お兄ちゃんのチェックなんてやだー」"
    voice "audio/log/D051ESS3_KOM001.ogg"
    kom "E-e? Akamning tekshiruvini xohlamayman."

# game/scripts/game/OG_D051ESS3.rpy:76
translate uzbek OG_D051ESS3_0_e2c7a4e6:

    # hcm "（うう……近い将来家に三浦みたいのがうろうろするのだけはごめんだ。校風が自由じゃなければよかったのに！　って、あれ？）"
    hcm "(Uh... Yaqin kelajakda uyda Miuraga o'xshaganlarning ivirsib yurishini istamayman. Qaniydi maktab muhiti erkin bo'lmaganda! Iye, to'xta?)"

# game/scripts/game/OG_D051ESS3.rpy:88
translate uzbek OG_D051ESS3_0_31098b3b:

    # voice "audio/log/D051ESS3_TAI002.ogg"
    # tai "「とりあえず……自由な校風が特色なんすね。参考になったっす。ありがとうございます」"
    voice "audio/log/D051ESS3_TAI002.ogg"
    tai "Xullas... o'ziga xosligi erkin muhit ekan-da. Tushunarli bo'ldi. Rahmat sizga."

# game/scripts/game/OG_D051ESS3.rpy:92
translate uzbek OG_D051ESS3_0_6080ffa0:

    # hcm "（……ああ、こいつも自由な校風ってことで納得しちまった。やはり小町がうちの高校に受かったあかつきには、嫌われない程度にチェックしなきゃ！）"
    hcm "(...Ha, bu ham erkin muhit deganiga ishondi. Komachi maktabimizga kirsa, o'zimni yomon ko'rsatib qo'ymaydigan darajada uni tekshirib turishim kerak!)"