    voice "audio/log/N032ESS0_HCM025.ogg"
    hcm "「...Shunaqami.」"

# game/scripts/game/OG_N032ESS0.rpy:595
translate uzbek OG_N032ESS0_0_1ab276fa:

    # voice "audio/log/N032ESS0_RUM018.ogg"
    # tur "「……私の状況も今の嫌な感じも高校生になれば変わるのかな」"
    voice "audio/log/N032ESS0_RUM018.ogg"
    tur "「...Mening ahvolim ham, hozirgi yoqimsiz hislarim ham yuqori sinfga o‘tsam o‘zgararmikin.」"

# game/scripts/game/OG_N032ESS0.rpy:599
translate uzbek OG_N032ESS0_0_aba69a47:

    # voice "audio/log/N032ESS0_YUK011.ogg"
    # yuk "「少なくとも、今のままでいるつもりなら絶対に変われないわね」"
    voice "audio/log/N032ESS0_YUK011.ogg"
    yuk "「Hozirgidek qolaversang, hech qachon o‘zgarmaysan.」"

# game/scripts/game/OG_N032ESS0.rpy:603
translate uzbek OG_N032ESS0_0_90bff40c:

    # voice "audio/log/N032ESS0_HCM026.ogg"
    # hcm "「けど、周りが変わることも充分あるからな。それまで無理して付き合う必要もないだろ」"
    voice "audio/log/N032ESS0_HCM026.ogg"
    hcm "「Lekin atrofingdagilar o‘zgarishi ham mumkin. Ungacha majburan ular bilan chiqishishing shart emas.」"

# game/scripts/game/OG_N032ESS0.rpy:607
translate uzbek OG_N032ESS0_0_e4a70a75:

    # voice "audio/log/N032ESS0_YUI016.ogg"
    # yui "「でも留美ちゃんは今がつらいんだから、それをどうにかしないと……」"
    voice "audio/log/N032ESS0_YUI016.ogg"
    yui "「Lekin Rumi-chan hozir qiynalyapti-ku, shuning uchun bir narsa qilish kerak...」"

# game/scripts/game/OG_N032ESS0.rpy:617
translate uzbek OG_N032ESS0_0_a668618e:

    # voice "audio/log/N032ESS0_RUM019.ogg"
    # tur "「つらいっていうか……ちょっと嫌だな。惨めっぽい。シカトされると自分が一番下なんだって感じる」"
    voice "audio/log/N032ESS0_RUM019.ogg"
    tur "「Qiynalishdan ko‘ra... biroz yoqimsiz. O‘zimni xorlangandek his qilaman. E’tibor berishmasa, o‘zimni eng pastkash odamdek sezaman.」"

# game/scripts/game/OG_N032ESS0.rpy:621
translate uzbek OG_N032ESS0_0_597471df:

    # voice "audio/log/N032ESS0_HCM027.ogg"
    # hcm "「そうか」"
    voice "audio/log/N032ESS0_HCM027.ogg"
    hcm "「Shunaqami.」"

# game/scripts/game/OG_N032ESS0.rpy:625
translate uzbek OG_N032ESS0_0_daba518b:

    # voice "audio/log/N032ESS0_RUM020.ogg"
    # tur "「嫌だけどさ。でも、もうどうしようもないし」"
    voice "audio/log/N032ESS0_RUM020.ogg"
    tur "「Yoqimsiz, lekin iloj yo‘q.」"

# game/scripts/game/OG_N032ESS0.rpy:629
translate uzbek OG_N032ESS0_0_d3f50319:

    # voice "audio/log/N032ESS0_YUK012.ogg"
    # yuk "「なぜ？」"
    voice "audio/log/N032ESS0_YUK012.ogg"
    yuk "「Nega?」"

# game/scripts/game/OG_N032ESS0.rpy:633
translate uzbek OG_N032ESS0_0_6dc1b8ce:

    # voice "audio/log/N032ESS0_RUM021.ogg"
    # tur "「私……見捨てちゃったし。もう仲良くできない。仲良くしてても、またいつこうなるかわかんないし。同じことになるなら、このままでいいかなって……」"
    voice "audio/log/N032ESS0_RUM021.ogg"
    tur "「Men... voz kechib bo‘ldim. Endi ular bilan chiqisha olmayman. Chiqishsam ham, qachon yana shunday bo‘lishini bilmayman. Agar baribir shunday bo‘ladigan bo‘lsa, shu holicha qolaversin...」"

# game/scripts/game/OG_N032ESS0.rpy:637
translate uzbek OG_N032ESS0_0_fa7bdeee:

    # hcm "（ああそうか……もう見限っちまったのか、自分と自分の周囲を）"
    hcm "（Ha, shunaqami... o‘zidan va atrofidagilardan umidini uzib bo‘libdi-da.）"

# game/scripts/game/OG_N032ESS0.rpy:645
translate uzbek OG_N032ESS0_0_70306f55:

    # voice "audio/log/N032ESS0_RUM022.ogg"
    # tur "「惨めなのは、嫌だけど……」"
    voice "audio/log/N032ESS0_RUM022.ogg"
    tur "「Xorlanishni xohlamayman, lekin...」"

# game/scripts/game/OG_N032ESS0.rpy:649
translate uzbek OG_N032ESS0_0_29d592d6:

    # hcm "（それでも世界は厳然とそこにある。カーストの序列に従い、振舞うことを強要される、残酷でくそったれのゴミみたいな、変わることない子どもの王国が……）"
    hcm "（Shunday bo‘lsa ham, dunyo o‘z joyida turibdi. Tabaqalanish tartibiga bo‘ysunishga majbur qiladigan, shafqatsiz va jirkanch, o‘zgarmas bolalar qirolligi...）"

# game/scripts/game/OG_N032ESS0.rpy:651
