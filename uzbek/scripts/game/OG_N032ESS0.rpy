    tur "「............」"

# game/scripts/game/OG_N032ESS0.rpy:561
translate uzbek OG_N032ESS0_0_74be225c:

    # hcm "（……雪ノ下がちゃんと謝罪するなんて初めて見たんじゃねぇか？　由比ケ浜まで言葉を失ってるし……）"
    hcm "（...Yukinoshitaning uzr so‘raganini birinchi marta ko‘rishim emasmi? Hatto Yuigahama ham lol qoldi...）"

# game/scripts/game/OG_N032ESS0.rpy:571
translate uzbek OG_N032ESS0_0_3a17d888:

    # voice "audio/log/N032ESS0_HCM024.ogg"
    # hcm "「……あー、まぁ、ほらあれだ。なら撮っておくか？　俺の写真なんてスーパーレアだぞ。普通なら課金対象だぞ」"
    voice "audio/log/N032ESS0_HCM024.ogg"
    hcm "「...A, mayli, anavi. Unda rasmga olib qo‘yasanmi? Mening rasmim juda noyob narsa. Odatda buning uchun pul to‘lash kerak.」"

# game/scripts/game/OG_N032ESS0.rpy:581
translate uzbek OG_N032ESS0_0_51258da0:

    # voice "audio/log/N032ESS0_RUM017.ogg"
