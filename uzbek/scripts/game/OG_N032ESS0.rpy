    # voice "audio/log/N032ESS0_HCM028.ogg"
    # hcm "「惨めなのは、嫌か」"
    voice "audio/log/N032ESS0_HCM028.ogg"
    hcm "「Xorlanishni xohlamaysanmi?」"

# game/scripts/game/OG_N032ESS0.rpy:665
