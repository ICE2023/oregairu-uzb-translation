# game/scripts/game/OG_A021YUK0.rpy:18
translate uzbek OG_A021YUK0_0_d0638e5d:

    # voice "audio/log/A021YUK0_HCM000.ogg"
    # hcm "「よう」"
    voice "audio/log/A021YUK0_HCM000.ogg"
    hcm "「Salom.」"

# game/scripts/game/OG_A021YUK0.rpy:28
translate uzbek OG_A021YUK0_0_06502dc4:

    # voice "audio/log/A021YUK0_YUK000.ogg"
    # yuk "「……あらこんにちは」"
    voice "audio/log/A021YUK0_YUK000.ogg"
    yuk "「...Voy, salom.」"

# game/scripts/game/OG_A021YUK0.rpy:32
translate uzbek OG_A021YUK0_0_09c7a1be:

    # voice "audio/log/A021YUK0_HCM001.ogg"
    # hcm "「でかける用事ってのはこれか？　なんか昨日もここで見かけた気がするけど、いつもいんのか？」"
    voice "audio/log/A021YUK0_HCM001.ogg"
    hcm "「Ishim bor deganing shumi? Kecha ham shu yerda ko‘rgandek edim, doim shu yerdamisan?」"

# game/scripts/game/OG_A021YUK0.rpy:36
translate uzbek OG_A021YUK0_0_16bcede0:

    # voice "audio/log/A021YUK0_YUK001.ogg"
    # yuk "「そういうあなたこそ、私をつけまわしてでもいるのかしら？」"
    voice "audio/log/A021YUK0_YUK001.ogg"
    yuk "「O‘zing-chi, meni poylab yuribsanmi deyman?」"

# game/scripts/game/OG_A021YUK0.rpy:40
translate uzbek OG_A021YUK0_0_9d1a7c9e:

    # voice "audio/log/A021YUK0_HCM002.ogg"
    # hcm "「そんなわけあるか。予備校の帰りに立ち寄るにはちょうどいいんだよ、この本屋」"
    voice "audio/log/A021YUK0_HCM002.ogg"
    hcm "「Bo‘lmagan gap. Tayyorlov kursidan qaytishda kirib o‘tishga qulay-da bu kitob do‘koni.」"

# game/scripts/game/OG_A021YUK0.rpy:44
translate uzbek OG_A021YUK0_0_d7bdc137:

    # voice "audio/log/A021YUK0_YUK002.ogg"
    # yuk "「でしょうね」"
    voice "audio/log/A021YUK0_YUK002.ogg"
    yuk "「Shundaydir.」"

# game/scripts/game/OG_A021YUK0.rpy:48
translate uzbek OG_A021YUK0_0_c7abfbc9:

    # voice "audio/log/A021YUK0_HCM003.ogg"
    # hcm "「なんだそれ？」"
    voice "audio/log/A021YUK0_HCM003.ogg"
    hcm "「Bu nimasi?」"

# game/scripts/game/OG_A021YUK0.rpy:58
translate uzbek OG_A021YUK0_0_e0bad89a:

    # voice "audio/log/A021YUK0_YUK003.ogg"
    # yuk "「あなた、気付いていなかったの？　私も川崎さんと同じ予備校に通っているのよ」"
    voice "audio/log/A021YUK0_YUK003.ogg"
    yuk "「Nahotki sezmagan bo‘lsang? Men ham Kavasaki bilan bitta kursga qatnayman-ku.」"

# game/scripts/game/OG_A021YUK0.rpy:62
translate uzbek OG_A021YUK0_0_5a5e639a:

    # voice "audio/log/A021YUK0_HCM004.ogg"
    # hcm "「あー、川崎な！　そうだ川崎だよ！　川崎！！　ていうか俺も川崎と同じ予備校なんですけど？」"
    voice "audio/log/A021YUK0_HCM004.ogg"
    hcm "「Ha-a, Kavasaki-ya! To‘g‘ri, Kavasaki! Kavasaki!! Aytgancha, men ham Kavasaki bilan bitta kursdaman-ku?」"

# game/scripts/game/OG_A021YUK0.rpy:66
translate uzbek OG_A021YUK0_0_3befddfd:

    # voice "audio/log/A021YUK0_YUK004.ogg"
    # yuk "「……あなた、もしかして彼女のこと忘れてたの？　クラスメイトでしょう」"
    voice "audio/log/A021YUK0_YUK004.ogg"
    yuk "「...Sen, mabodo uning kimligini unutib qo‘ymadingmi? Axir sinfdoshing-ku.」"

# game/scripts/game/OG_A021YUK0.rpy:70
translate uzbek OG_A021YUK0_0_0bbd2981:

    # voice "audio/log/A021YUK0_HCM005.ogg"
    # hcm "「人の名前って覚えるの大変だよなー、普段から人付き合いしてねーと特に。あとあれな興味ないとさらに」"
    voice "audio/log/A021YUK0_HCM005.ogg"
    hcm "「Odamlarning ismini eslab qolish qiyin-da, ayniqsa odamlar bilan kam muloqot qilsang. Qolaversa, qiziqmasang undan ham qiyin.」"

# game/scripts/game/OG_A021YUK0.rpy:80
translate uzbek OG_A021YUK0_0_07d4e3f4:

    # voice "audio/log/A021YUK0_YUK005.ogg"
    # yuk "「呆れて言葉も出ないわ……」"
    voice "audio/log/A021YUK0_YUK005.ogg"
    yuk "「Hayratdan so‘z ham topolmay qoldim...」"

# game/scripts/game/OG_A021YUK0.rpy:84
translate uzbek OG_A021YUK0_0_65a876c7:

    # voice "audio/log/A021YUK0_HCM006.ogg"
    # hcm "「つーか、お前もあの予備校に通ってんだな。そのわりに全然見かけねー気がするけど」"
    voice "audio/log/A021YUK0_HCM006.ogg"
    hcm "「Xullas, sen ham o‘sha kursga qatnarkan-san-da. Lekin negadir seni u yerda umuman ko‘rmagan ekanman.」"

# game/scripts/game/OG_A021YUK0.rpy:94
translate uzbek OG_A021YUK0_0_89d04511:

    # voice "audio/log/A021YUK0_YUK006.ogg"
    # yuk "「クラスが違うからよ。私が受講しているのは国立最難関コースだもの」"
    voice "audio/log/A021YUK0_YUK006.ogg"
    yuk "「Chunki sinflarimiz boshqa. Men eng qiyin davlat oliygohlari kursida o‘qiyman.」"

# game/scripts/game/OG_A021YUK0.rpy:98
translate uzbek OG_A021YUK0_0_7b3f15d0:

    # voice "audio/log/A021YUK0_HCM007.ogg"
    # hcm "「そうか。大変だな５教科７科目もやんのは」"
    voice "audio/log/A021YUK0_HCM007.ogg"
    hcm "「Shunaqami. 5 ta fan, 7 ta yo‘nalishda o‘qish qiyin bo‘lsa kerak.」"

# game/scripts/game/OG_A021YUK0.rpy:102
translate uzbek OG_A021YUK0_0_e3204b52:

    # voice "audio/log/A021YUK0_YUK007.ogg"
    # yuk "「私はまだ文理選択を決めてないから受講している科目はもっと多いけれど」"
    voice "audio/log/A021YUK0_YUK007.ogg"
    yuk "「Men hali gumanitar yoki aniq fanlarni tanlaganim yo‘q, shuning uchun fanlarim bundan ham ko‘proq.」"

# game/scripts/game/OG_A021YUK0.rpy:106
translate uzbek OG_A021YUK0_0_42f6f75d:

    # voice "audio/log/A021YUK0_HCM008.ogg"
    # hcm "「理系だとさらにキツいだろ。数学が２つあるのが意味分からんし、あれ。２回も数学やるとかマスマティクスというよりマゾヒスティックだよな」"
    voice "audio/log/A021YUK0_HCM008.ogg"
    hcm "「Aniq fanlar undan ham qiyin bo‘lsa kerak. Ikkita matematika borligiga tushunmayman o‘zi. Ikki marta matematika o‘qish — bu matematika emas, mazoxizmning o‘zginasi.」"

# game/scripts/game/OG_A021YUK0.rpy:110
translate uzbek OG_A021YUK0_0_abf2451b:

    # voice "audio/log/A021YUK0_YUK008.ogg"
    # yuk "「似てるようで全然似てないわね……。私は特に苦労していないけれど、数学９点で最下位だとそう感じるのでしょうね」"
    voice "audio/log/A021YUK0_YUK008.ogg"
    yuk "「O‘xshash eshitiladi-yu, lekin umuman boshqa narsa... Men qiynalmayman-u, lekin matematikadan 9 ball olib, eng oxirgi o‘rinda bo‘lgan odamga shunday tuyulsa kerak-da.」"

# game/scripts/game/OG_A021YUK0.rpy:114
translate uzbek OG_A021YUK0_0_a04445c8:

    # voice "audio/log/A021YUK0_HCM009.ogg"
    # hcm "「俺の成績のことはいいだろ。別に数学使わないから、捨ててるんだよ」"
    voice "audio/log/A021YUK0_HCM009.ogg"
    hcm "「Baholarimni gapirmasak ham bo‘lar. Baribir matematika kerak emas menga, shuning uchun tashlab qo‘yganman.」"

# game/scripts/game/OG_A021YUK0.rpy:127
translate uzbek OG_A021YUK0_0_b9b9d736:

    # voice "audio/log/A021YUK0_YUK009.ogg"
    # yuk "「マイナス方向にだけは思い切りがいいのよね……。けれど、逆に考えれば私立文系で数学が選択できれば手持ちの武器が増えるという考え方もできるのではなくて？」"
    voice "audio/log/A021YUK0_YUK009.ogg"
    yuk "「Faqat salbiy tomonga kelganda qat'iyatsan-da... Lekin boshqacha o‘ylab ko‘r, xususiy gumanitar oliygohlar uchun matematikani tanlasang, imkoniyatlaring ko‘payadi-ku?」"

# game/scripts/game/OG_A021YUK0.rpy:131
translate uzbek OG_A021YUK0_0_e4819bb1:

    # voice "audio/log/A021YUK0_HCM010.ogg"
    # hcm "「いい、いらん。できないものを人並みにするより、できることを最大限伸ばしたほうが効率良い」"
    voice "audio/log/A021YUK0_HCM010.ogg"
    hcm "「Kerak emas. Eplayolmaydigan narsamni o‘rtacha darajaga olib chiqqandan ko‘ra, qo‘limdan keladiganini maksimal darajada rivojlantirish samaraliroq.」"

# game/scripts/game/OG_A021YUK0.rpy:135
translate uzbek OG_A021YUK0_0_b72c94f5:

    # voice "audio/log/A021YUK0_YUK010.ogg"
    # yuk "「なかなかよくできた言い訳ね、……といいたいところだけれど、あなたの場合、得意教科はそれなりだからたちが悪いわ」"
    voice "audio/log/A021YUK0_YUK010.ogg"
    yuk "「Bahonani qoyillatibsiz... demoqchi edim-u, lekin sening holatingda, yaxshi biladigan fanlaring chindan ham yaxshi, shuning uchun ham gapirish qiyin.」"

# game/scripts/game/OG_A021YUK0.rpy:139
translate uzbek OG_A021YUK0_0_9f97924a:

    # voice "audio/log/A021YUK0_HCM011.ogg"
    # hcm "「いや俺より成績上のお前に言われても……」"
    voice "audio/log/A021YUK0_HCM011.ogg"
    hcm "「Mendan ko‘ra baholari yuqori bo‘lgan odamdan buni eshitish...」"

# game/scripts/game/OG_A021YUK0.rpy:152
translate uzbek OG_A021YUK0_0_15a07c6c:

    # voice "audio/log/A021YUK0_YUK011.ogg"
    # yuk "「……。あなたももっと――、いえ、なんでもないわ」"
    voice "audio/log/A021YUK0_YUK011.ogg"
    yuk "「...Sen ham ko‘proq... yo‘q, hech narsa.」"

# game/scripts/game/OG_A021YUK0.rpy:156
translate uzbek OG_A021YUK0_0_156c7de4:

    # voice "audio/log/A021YUK0_HCM012.ogg"
    # hcm "「まぁ、なんでもいいけど、あれか。長期休暇に帰省してなかったのは講義が多いからか」"
    voice "audio/log/A021YUK0_HCM012.ogg"
    hcm "「Mayli, nima bo‘lsa ham... Ta’tilda uyga ketmaganing sababi darslar ko‘pligidanmi?」"

# game/scripts/game/OG_A021YUK0.rpy:160
translate uzbek OG_A021YUK0_0_e038336e:

    # voice "audio/log/A021YUK0_YUK012.ogg"
    # yuk "「ええ、実家から通うよりは近くて楽だもの。サブレを預かることもできたし、結果的には良かったわ」"
    voice "audio/log/A021YUK0_YUK012.ogg"
    yuk "「Ha, ota-onamning uyidan qatnagandan ko‘ra bu yer yaqin va qulayroq. Ustiga-ustak Soblni ham qarab turish imkoni bo‘ldi, xullas hammasi yaxshilikka.」"

# game/scripts/game/OG_A021YUK0.rpy:164
translate uzbek OG_A021YUK0_0_a53c0dfa:

    # voice "audio/log/A021YUK0_HCM013.ogg"
    # hcm "「……その調子なら、大丈夫そうだな」"
    voice "audio/log/A021YUK0_HCM013.ogg"
    hcm "「...Kayfiyatingga qaraganda, hammasi joyida ko‘rinadi.」"

# game/scripts/game/OG_A021YUK0.rpy:168
translate uzbek OG_A021YUK0_0_3f19a651:

    # voice "audio/log/A021YUK0_YUK013.ogg"
    # yuk "「なにをもって大丈夫と言っているのかは知らないけれど、私とサブレのことなら心配無用よ。エアコンもちゃんと利かせて出てきたし、お水も用意したわ」"
    voice "audio/log/A021YUK0_YUK013.ogg"
    yuk "「Nimani nazarda tutib joyida deyayotganingni bilmadim-u, lekin men va Sobl uchun xavotirga o‘rin yo‘q. Konditsionerni yoqib, suvini ham tayyorlab chiqqanman.」"

# game/scripts/game/OG_A021YUK0.rpy:172
translate uzbek OG_A021YUK0_0_caa7eccf:

    # voice "audio/log/A021YUK0_HCM014.ogg"
    # hcm "「そっか、ご苦労さん」"
    voice "audio/log/A021YUK0_HCM014.ogg"
    hcm "「Tushunarli, hormang.」"

# game/scripts/game/OG_A021YUK0.rpy:176
translate uzbek OG_A021YUK0_0_9323373a:

    # voice "audio/log/A021YUK0_YUK014.ogg"
    # yuk "「あなたに言われる筋合いはないわ、由比ケ浜さんのためだもの。そろそろ午後の授業だから行くわね、ではさようなら」"
    voice "audio/log/A021YUK0_YUK014.ogg"
    yuk "「Sizdan bu gapni eshitishim shart emas, bularning bari Yuigahama uchun. Tushdan keyingi darslar boshlanyapti, men ketdim. Xayr.」"

# game/scripts/game/OG_A021YUK0.rpy:180
translate uzbek OG_A021YUK0_0_615367a8:

    # voice "audio/log/A021YUK0_HCM015.ogg"
    # hcm "「おう、じゃあな」"
    voice "audio/log/A021YUK0_HCM015.ogg"
    hcm "「Ha, ko‘rishguncha.」"