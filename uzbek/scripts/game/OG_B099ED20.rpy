# game/scripts/game/OG_B099ED20.rpy:18
translate uzbek OG_B099ED20_0_6b3474b3:

    # narrator "つつがなく終わる始業式、つつがなく終わるホームルーム。"
    narrator "O‘quv yili boshlanishiga bag‘ishlangan marosim va sinf soati tinchgina o‘tdi."

# game/scripts/game/OG_B099ED20.rpy:28
