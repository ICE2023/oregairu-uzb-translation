    # voice "audio/log/D072ESS3_HCM004.ogg"
    # hcm "「……え？　あ、行くの？」"
    voice "audio/log/D072ESS3_HCM004.ogg"
    hcm "...E? A, ketasanmi?"

# game/scripts/game/OG_D072ESS3.rpy:78
