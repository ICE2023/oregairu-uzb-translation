    voice "audio/log/D072ESS3_HCM005.ogg"
    hcm "A... xo'p."

# game/scripts/game/OG_D072ESS3.rpy:94
translate uzbek OG_D072ESS3_0_ac3ba68c:

    # hcm "（……やべえ。なんだったんだ今のは。まああれだ、日射病の一種とかそういうアレだったと言うことにしておこう）"
    hcm "(...G'alati bo'ldi. Hozir nima bo'ldi o'zi. Mayli, quyosh urishi oqibati deb hisoblay qolamiz.)"

# game/scripts/game/OG_D072ESS3.rpy:96
translate uzbek OG_D072ESS3_0_4e22595d:

    # hcm "（……しかし、大志探しはだりいなあ。この暑さだし人混みだしやっぱ面倒くせえ……ま、仕方ねぇな……）"
    hcm "(...Lekin, Taishini qidirishga erinyapman. Issiq, odam ko'p, baribir noqulay... mayli, iloj yo'q...)"