# game/scripts/game/OG_D072ESS3.rpy:50
translate uzbek OG_D072ESS3_0_d170dbc5:

    # voice "audio/log/D072ESS3_KAW003.ogg"
    # kaw "「っていうか……普段あんたその、なんつーか無愛想だから、珍しいなって、その……それだけ！　ご、誤解しないでよ？」"
    voice "audio/log/D072ESS3_KAW003.ogg"
    kaw "Nima desam ekan... odatda kamgap bo'lganing uchun, bugungi holating g'alati tuyuldi... sh-shuning uchun! No-noto'g'ri tushunma?"

# game/scripts/game/OG_D072ESS3.rpy:54
translate uzbek OG_D072ESS3_0_efcb6fe7:

    # voice "audio/log/D072ESS3_HCM003.ogg"
    # hcm "「いや、そんな照れられても……こっちが困るわ」"
    voice "audio/log/D072ESS3_HCM003.ogg"
    hcm "Yo'q, bunchalik uyalishing... meni ham noqulay ahvolga solib qo'yadi."

# game/scripts/game/OG_D072ESS3.rpy:64
translate uzbek OG_D072ESS3_0_bb23574a:

    # voice "audio/log/D072ESS3_KAW004.ogg"
    # kaw "「なっ！　ててて、照れてないし！　…………バカ」"
    voice "audio/log/D072ESS3_KAW004.ogg"
    kaw "Nima! U-u-uyalganim yo'q! ........Jinni."

# game/scripts/game/OG_D072ESS3.rpy:68
translate uzbek OG_D072ESS3_0_70e1a332:

    # hcm "（やべ、どうしたらいいんだこの雰囲気……プールだし、川崎は水着だし、うっかりすると俺うっかり八幡ですよ？……まぁ、うっかりしねぇけど）"
    hcm "(Obbo, nima qilsam ekan bu vaziyatda... Hovuzdamiz, Kawasaki suzish kiyimida, sal bo'lmasa... xayol ketib qoladi. ...Yo'q, xayol surmayman.)"

# game/scripts/game/OG_D072ESS3.rpy:70
translate uzbek OG_D072ESS3_0_0027ed40:

    # voice "audio/log/D072ESS3_KAW005.ogg"
    # kaw "「……え、えっと。そ、そろそろ大志探しに行って、いい？」"
    voice "audio/log/D072ESS3_KAW005.ogg"
    kaw "...E, haligi. E-endi Taishini qidirgani borsam... maylimi?"

# game/scripts/game/OG_D072ESS3.rpy:74
