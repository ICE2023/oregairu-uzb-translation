# game/scripts/game/OG_TALK_TOT2.rpy:318
translate uzbek OG_TALK_TOT2_8_2d1733db:

    # hcm "「あれ、俺今声出てた？」"
    hcm "「Iye, ovozim chiqib ketdimi?」"

# game/scripts/game/OG_TALK_TOT2.rpy:320
translate uzbek OG_TALK_TOT2_8_64f1b979:

    # voice "audio/log/TOT2B3_TOT002.ogg"
    # tot mca02 "「……すごい出てたけど……うーん、『これ』か。うん、そうかもしれないね」"
    voice "audio/log/TOT2B3_TOT002.ogg"
    tot mca02 "「...Juda baland chiqdi... Hmm, “bu”mi. Ha, shunday bo‘lishi mumkin.」"

# game/scripts/game/OG_TALK_TOT2.rpy:324
translate uzbek OG_TALK_TOT2_8_4054368d:

    # hcm "「そ、そうか！」"
    hcm "「Shu-shunaqami!」"

# game/scripts/game/OG_TALK_TOT2.rpy:326
translate uzbek OG_TALK_TOT2_8_7f184660:

    # voice "audio/log/TOT2B3_TOT003.ogg"
    # tot "「もし女の子とふたりだったら、ね」"
    voice "audio/log/TOT2B3_TOT003.ogg"
    tot "「Agar qiz bola bilan yolg‘iz bo‘lganingda.」"

# game/scripts/game/OG_TALK_TOT2.rpy:330
translate uzbek OG_TALK_TOT2_8_0b7e99b9:

    # hcm "「そ、そうか……」"
    hcm "「Shu-shunaqami...」"

# game/scripts/game/OG_TALK_TOT2.rpy:332
translate uzbek OG_TALK_TOT2_8_b59fc16b:

    # voice "audio/log/TOT2B3_TOT004.ogg"
    # tot "「けど……そっか、八幡もやっぱり、そういうこと考えるんだね」"
    voice "audio/log/TOT2B3_TOT004.ogg"
    tot "「Lekin... tushunarli, Hachiman ham shunaqa narsalarni o‘ylab turar ekan-da.」"

# game/scripts/game/OG_TALK_TOT2.rpy:336
translate uzbek OG_TALK_TOT2_8_dddb2391:

    # hcm "「えっ？　あ、いや、まぁ、なんだ……ん？、俺『も』ってことは……戸塚も？」"
    hcm "「E? A, yo‘q, endi... Hm? Men “ham” deganingga qaraganda... Totsuka hammi?」"

# game/scripts/game/OG_TALK_TOT2.rpy:338
translate uzbek OG_TALK_TOT2_8_5423d9be:

    # voice "audio/log/TOT2B3_TOT005.ogg"
    # tot mca01 "「どうかなぁ……したことないからよくわかんないけど……フフッ、でも、誰かとこういう話したことないから、うれしいな。八幡相手だと、話しやすいしね」"
    voice "audio/log/TOT2B3_TOT005.ogg"
    tot mca01 "「Bilmasam... hech qilib ko‘rmaganman, tushunmayman... Fufu, lekin hech kim bilan bunday mavzuda gaplashmagan edim, xursandman. Hachiman bilan gaplashish oson-da.」"

# game/scripts/game/OG_TALK_TOT2.rpy:342
translate uzbek OG_TALK_TOT2_8_90867d73:

    # hcm "「そ、そうか……！」"
    hcm "「Shu-shunaqami...!」"

# game/scripts/game/OG_TALK_TOT2.rpy:350
translate uzbek OG_TALK_TOT2_9_d6cfb389:

    # hcm "「将来のこととか、考えてるか？」"
    hcm "「Kelajak haqida o‘ylab ko‘rganmisan?」"

# game/scripts/game/OG_TALK_TOT2.rpy:352
translate uzbek OG_TALK_TOT2_9_b84be378:

    # voice "audio/log/TOT2B4_TOT000.ogg"
    # tot mca02 "「そうだね……もう遅いくらいだよね」"
    voice "audio/log/TOT2B4_TOT000.ogg"
    tot mca02 "「To‘g‘ri... biroz kech ham qoldim-ov.」"

# game/scripts/game/OG_TALK_TOT2.rpy:356
translate uzbek OG_TALK_TOT2_9_366e1ca4:

    # hcm "「いや、遅いなんてことはないだろ。タイムマシンでもない限り過去には戻れないんだから、いつだって今このときが一番早いんだ」"
    hcm "「Yo‘q, kech emas. Vaqt mashinasi bo‘lmasa o‘tmishga qaytib bo‘lmaydi, shuning uchun hozirgi vaqt eng erta vaqt hisoblanadi.」"

# game/scripts/game/OG_TALK_TOT2.rpy:358
translate uzbek OG_TALK_TOT2_9_ff84f79b:

    # hcm "「だから、過去をくやむよりは、現時点でなにができるかを考える方がいい……と思うぞ」"
    hcm "「Shuning uchun o‘tmishga afsuslangandan ko‘ra, hozir nima qila olishing haqida o‘ylagan ma’qul... deb o‘ylayman.」"

# game/scripts/game/OG_TALK_TOT2.rpy:360
translate uzbek OG_TALK_TOT2_9_64ec9f77:

    # voice "audio/log/TOT2B4_TOT001.ogg"
    # tot "「……へぇ、なんか、とても前向きな考え方だね！」"
    voice "audio/log/TOT2B4_TOT001.ogg"
    tot "「...Hee, juda ijobiy fikrlar ekan!」"

# game/scripts/game/OG_TALK_TOT2.rpy:364
translate uzbek OG_TALK_TOT2_9_bc86f595:

    # hcm "「へっ？　……なんか聞きなれない褒め言葉をもらった気がするんだけど」"
    hcm "「E? ...Menga aytilmaydigan maqtov eshitdim shekilli.」"

# game/scripts/game/OG_TALK_TOT2.rpy:366
translate uzbek OG_TALK_TOT2_9_2477efd8:

    # voice "audio/log/TOT2B4_TOT002.ogg"
    # tot mca01 "「八幡は前向きだねって言ったんだよ？」"
    voice "audio/log/TOT2B4_TOT002.ogg"
    tot mca01 "「Hachiman ijobiy fikrlaydi dedim-ku?」"

# game/scripts/game/OG_TALK_TOT2.rpy:370
translate uzbek OG_TALK_TOT2_9_a10cebf1:

    # hcm "「……まぁ、目標見据えてるってのは、前向きになるもんだからな」"
    hcm "「...Ha, endi, maqsad sari intilish insonga ijobiy kuch beradi-da.」"

# game/scripts/game/OG_TALK_TOT2.rpy:372
translate uzbek OG_TALK_TOT2_9_38b859c6:

    # voice "audio/log/TOT2B4_TOT003.ogg"
    # tot "「八幡の目標ってなんなの？」"
    voice "audio/log/TOT2B4_TOT003.ogg"
    tot "「Sening maqsading nima?」"

