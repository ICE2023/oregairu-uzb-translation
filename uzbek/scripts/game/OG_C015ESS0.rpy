# game/scripts/game/OG_C015ESS0.rpy:90
translate uzbek OG_C015ESS0_0_a3377bcc:

    # voice "audio/log/C015ESS0_KOM005.ogg"
    # kom "「だって、どう考えてもおかしいよ。捻デレが売りのお兄ちゃんが、戸塚さんの前だと素直になってただのデレデレさんだもん」"
    voice "audio/log/C015ESS0_KOM005.ogg"
