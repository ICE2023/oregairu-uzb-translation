# game/scripts/game/OG_D099ED11.rpy:54
translate uzbek OG_D099ED11_0_ce96560f:

    # voice "audio/log/D099ED11_HCM003.ogg"
    # hcm "「あのあと、大志と話したりしなかったのかよ」"
    voice "audio/log/D099ED11_HCM003.ogg"
