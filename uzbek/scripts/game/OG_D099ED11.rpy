    voice "audio/log/D099ED11_KAW004.ogg"
    kaw "...Sen ham gap eshitdingmi?"

# game/scripts/game/OG_D099ED11.rpy:76
translate uzbek OG_D099ED11_0_d3ef5395:

    # voice "audio/log/D099ED11_HCM005.ogg"
    # hcm "「ああ。問い詰められて、全部白状させられた」"
    voice "audio/log/D099ED11_HCM005.ogg"
    hcm "Ha. Rosa tergadi, oxiri bor gapni aytishga majbur bo'ldim."

# game/scripts/game/OG_D099ED11.rpy:90
translate uzbek OG_D099ED11_0_a54ab877:

    # voice "audio/log/D099ED11_KOM000.ogg"
