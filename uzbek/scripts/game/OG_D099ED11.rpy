    voice "audio/log/D099ED11_KAW003.ogg"
    kaw "...Gaplashdim."

# game/scripts/game/OG_D099ED11.rpy:62
translate uzbek OG_D099ED11_0_16222bca:

    # voice "audio/log/D099ED11_HCM004.ogg"
    # hcm "「そうか。俺も小町と話した」"
    voice "audio/log/D099ED11_HCM004.ogg"
    hcm "Shunaqami. Men ham Komachi bilan gaplashdim."

# game/scripts/game/OG_D099ED11.rpy:72
translate uzbek OG_D099ED11_0_1687ccd9:

    # voice "audio/log/D099ED11_KAW004.ogg"
