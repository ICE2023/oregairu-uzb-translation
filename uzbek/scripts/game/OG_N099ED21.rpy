# game/scripts/game/OG_N099ED21.rpy:120
translate uzbek OG_N099ED21_0_b794f8d5:

    # voice "audio/log/N099ED21_HCM003.ogg"
    # hcm "「くつろいでんじゃねえよ。んで、用件はなんだ。遊びに来たとか言うんじゃねえぞ。お前と遊ぶキャパシティは俺にはないからな」"
    voice "audio/log/N099ED21_HCM003.ogg"
    hcm "「Rohatlanib o‘tirma. Xo‘sh, nima ishing bor? O‘ynagani keldim dema. Sen bilan o‘ynaydigan toqatim yo‘q.」"

# game/scripts/game/OG_N099ED21.rpy:130
translate uzbek OG_N099ED21_0_904e0334:

    # voice "audio/log/N099ED21_ZAI005.ogg"
    # zai "「用件はこれだ。……八幡、これを読んでくれ」"
    voice "audio/log/N099ED21_ZAI005.ogg"
    zai "「Ishim mana bu. ...Hachiman, buni o‘qib chiq.」"

# game/scripts/game/OG_N099ED21.rpy:144
translate uzbek OG_N099ED21_0_d720e385:

    # voice "audio/log/N099ED21_HCM004.ogg"
