# game/scripts/game/OG_B023ESS0.rpy:390
translate uzbek OG_B023ESS0_4_a8934c8b:

    # hcm "（言ってみりゃ、こういうイベントごとは淑女の社交場のようなもんだ。連れてる男子はひとつのステイタスになるかもしれない）"
    hcm "（Bunaqa tadbirlar xonimlarning uchrashuv joyiga o‘xshaydi. Yoningdagi yigit ham obro‘ belgisi bo‘lishi mumkin.）"

# game/scripts/game/OG_B023ESS0.rpy:392
translate uzbek OG_B023ESS0_4_328f90e0:

    # hcm "（それこそ、持ってるバッグや身に着けてる服のブランドでその人間の価値が測られるのに似ている）"
    hcm "（Bu xuddi sumka yoki kiyimingning qaysi brenddan ekanligiga qarab baholanishga o‘xshaydi.）"

# game/scripts/game/OG_B023ESS0.rpy:394
translate uzbek OG_B023ESS0_4_4aa31d6a:

    # hcm "（そして、相模っていったか……あいつはそういうものの見方をする人間なんだろう。レッテルを貼って、カースト分けをして、誰かを見下して安心したいんだ）"
    hcm "（Keyin, Sagami dedimi... U shunaqa qarashdagi odam bo‘lsa kerak. Odamlarga tamg‘a bosib, tabaqalarga ajratib, kimnidir pastga urib o‘zini xotirjam his qilmoqchi.）"

# game/scripts/game/OG_B023ESS0.rpy:396
translate uzbek OG_B023ESS0_4_f3ea7aca:

    # hcm "（あの蔑んだ目、あれは明らかに俺と、そして俺を連れている由比ケ浜を見下した目だった）"
    hcm "（Uning nafrat to‘la ko‘zlari, menga va men bilan yurgan Yuigahamaga past nazar bilan qaragani aniq edi.）"

# game/scripts/game/OG_B023ESS0.rpy:398
translate uzbek OG_B023ESS0_4_0789fbeb:

    # voice "audio/log/b023ess0_X19000.ogg"
    # X19 "「やきそばひとつ、お待ちっ！」"
    voice "audio/log/b023ess0_X19000.ogg"
    X19 "「Mana, bitta yakisoba!」"

# game/scripts/game/OG_B023ESS0.rpy:402
translate uzbek OG_B023ESS0_4_8a05f32d:

    # hcm "（たとえばこれが俺でなく、葉山なら周囲の反応は全然違っただろう。それこそ今夜のヒーローインタビューに由比ケ浜が呼ばれちゃう）"
    hcm "（Agar mening o‘rnimda Hayama bo‘lganida, atrofdagilarning munosabati butunlay boshqacha bo‘lardi. Yuigahama kechaning qahramoni sifatida intervyuga chaqirilardi.）"

# game/scripts/game/OG_B023ESS0.rpy:404
translate uzbek OG_B023ESS0_4_c1f7de71:

    # hcm "（けど、俺じゃあ軍法会議で欠席裁判レベルだ）"
    hcm "（Meni esa harbiy sudga chaqirmasdan turib hukm qilib yuborishadi.）"

# game/scripts/game/OG_B023ESS0.rpy:412
translate uzbek OG_B023ESS0_4_67718b30:

    # hcm "（俺はいくら笑われてもいいんだけど、一緒にいて由比ケ浜が笑われるのは可哀想だ）"
    hcm "（Mening ustimdan qancha kulishsa ham roziman, lekin yonimda turgan Yuigahamaning kulgiga qolishiga chidolmayman.）"

# game/scripts/game/OG_B023ESS0.rpy:414
translate uzbek OG_B023ESS0_4_8689996f:

    # voice "audio/log/b023ess0_X19001.ogg"
    # X19 "「はい、タコ焼きにラムネ１本、８００円ねー」"
    voice "audio/log/b023ess0_X19001.ogg"
    X19 "「Mana, takoyaki va bitta limonad, 800 iyena bo‘ladi.」"

# game/scripts/game/OG_B023ESS0.rpy:418
translate uzbek OG_B023ESS0_4_52bbb9b4:

    # voice "audio/log/B023ESS0_HCM016.ogg"
    # hcm "「どうも……って、あれ？」"
    voice "audio/log/B023ESS0_HCM016.ogg"
    hcm "「Rahmat... iye?」"

# game/scripts/game/OG_B023ESS0.rpy:424
translate uzbek OG_B023ESS0_4_103e6bd4:

    # hcm "（おいおい、グルグル考えながら歩いてるうちに、全部買い物済んじまったぞ。まだ話してるのか、あいつ）"
    hcm "（Iye, xayol surib yurib, hamma narsani xarid qilib bo‘libman-ku. U hali ham gaplashyaptimi?）"

# game/scripts/game/OG_B023ESS0.rpy:426
translate uzbek OG_B023ESS0_4_04c85557:

    # hcm "（仕方ない、電話してみるか……）"
    hcm "（Iloj yo‘q, qo‘ng‘iroq qilib ko‘raman...）"

# game/scripts/game/OG_B023ESS0.rpy:432
translate uzbek OG_B023ESS0_4_f3273b6e:

    # voice "audio/log/B023ESS0_HCM017.ogg"
    # hcm "「……電波悪いな」"
    voice "audio/log/B023ESS0_HCM017.ogg"
    hcm "「...Aloqa yomon-ku.」"

# game/scripts/game/OG_B023ESS0.rpy:443
translate uzbek OG_B023ESS0_5_dc99b671:

    # hcm "（これだけ混雑してれば仕方ないか……クソ、これだから人ゴミは嫌いんだよ）"
    hcm "（Shuncha odam bo‘lgandan keyin iloj yo‘q-da... La'nat, shuning uchun ham tiqilinch joylarni yomon ko‘raman.）"

# game/scripts/game/OG_B023ESS0.rpy:449
translate uzbek OG_B023ESS0_6_d303c1d7:

    # hcm "（さっきもなかなか通じなかったしな……人は増えるばっかりだし、当然といえば当然か。これはヤバイな）"
    hcm "（Boyayam yaxshi tushmagandi... Odamlar ko‘payib boryapti, bu tabiiy hol. Vaziyat chatoq.）"

# game/scripts/game/OG_B023ESS0.rpy:455
translate uzbek OG_B023ESS0_7_fe033c38:

    # hcm "（しつこくかけてみるしかないか……）"
    hcm "（Qayta-qayta qilib ko‘rishdan boshqa iloj yo‘q...）"

# game/scripts/game/OG_B023ESS0.rpy:459
translate uzbek OG_B023ESS0_7_6015e1a5:

    # hcm "（……お、由比ケ浜から着信！）"
    hcm "（...O, Yuigahamadan qo‘ng‘iroq!）"

# game/scripts/game/OG_B023ESS0.rpy:465
translate uzbek OG_B023ESS0_7_60690b81:

    # voice "audio/log/B023ESS0_HCM018.ogg"
    # hcm "「もしもし」"
    voice "audio/log/B023ESS0_HCM018.ogg"
    hcm "「Alo.」"

# game/scripts/game/OG_B023ESS0.rpy:469
translate uzbek OG_B023ESS0_7_6a9b943b:

    # voice "audio/log/B023ESS0_YUI025.ogg"
    # yui "「ヒッキー！　よかった、やっと繋がった〜。何回かけても回線が混みあってたから、もうダメかと思った」"
    voice "audio/log/B023ESS0_YUI025.ogg"
    yui "「Hikki! Xayriyat, zo‘rg‘a tushdim~. Qancha harakat qilsam ham tusholmay, endi bo‘lmadi shekilli deb o‘ylagandim.」"

# game/scripts/game/OG_B023ESS0.rpy:473
translate uzbek OG_B023ESS0_7_a9206d52:

    # voice "audio/log/B023ESS0_HCM019.ogg"
    # hcm "「相模との話は済んだのか？」"
    voice "audio/log/B023ESS0_HCM019.ogg"
    hcm "「Sagami bilan gaplashib bo‘ldingmi?」"

# game/scripts/game/OG_B023ESS0.rpy:477
translate uzbek OG_B023ESS0_7_53bd57f7:

    # voice "audio/log/B023ESS0_YUI026.ogg"
    # yui "「うん、とっくに。ずーっとヒッキーに電話かけてたんだよ？」"
    voice "audio/log/B023ESS0_YUI026.ogg"
    yui "「Ha, allaqachon. Boyadan beri Hikkiga qo‘ng‘iroq qilayotgandim.」"

# game/scripts/game/OG_B023ESS0.rpy:481
translate uzbek OG_B023ESS0_7_fb453364:

    # voice "audio/log/B023ESS0_HCM020.ogg"
    # hcm "「そっか、悪い。今どこにいるんだ？　買い物はもう済ませちまったぞ」"
    voice "audio/log/B023ESS0_HCM020.ogg"
    hcm "「Shunaqami, uzr. Hozir qayerdasan? Men xaridlarni qilib bo‘ldim.」"

# game/scripts/game/OG_B023ESS0.rpy:485
translate uzbek OG_B023ESS0_7_de0f3052:

    # voice "audio/log/B023ESS0_YUI027.ogg"
    # yui "「うそ、ごめん！　あたしがそっちに行くよ、いまどこ？」"
    voice "audio/log/B023ESS0_YUI027.ogg"
    yui "「Yo‘g‘-e, kechir! O‘zim boraman, qayerdasan?」"

# game/scripts/game/OG_B023ESS0.rpy:489
translate uzbek OG_B023ESS0_7_96e9361e:

    # voice "audio/log/B023ESS0_HCM021.ogg"
    # hcm "「えーと、タコ焼き屋の前だな。射的とカキ氷に挟まれてる屋台」"
    voice "audio/log/B023ESS0_HCM021.ogg"
    hcm "「Takoyaki do‘konining oldidaman. Tir bilan muzqaymoq do‘konining o‘rtasidagi joyda.」"

# game/scripts/game/OG_B023ESS0.rpy:493
translate uzbek OG_B023ESS0_7_8e764466:

    # voice "audio/log/B023ESS0_YUI028.ogg"
    # yui "「わかった！」"
    voice "audio/log/B023ESS0_YUI028.ogg"
    yui "「Tushundim!」"

# game/scripts/game/OG_B023ESS0.rpy:507
translate uzbek OG_B023ESS0_7_b9923235:

    # voice "audio/log/B023ESS0_YUI029.ogg"
    # yui "「ヒッキー、ほんとにごめんっ！」"
    voice "audio/log/B023ESS0_YUI029.ogg"
    yui "「Hikki, rostdan ham uzr!」"

# game/scripts/game/OG_B023ESS0.rpy:511
translate uzbek OG_B023ESS0_7_d8bf35c9:

    # voice "audio/log/B023ESS0_HCM022.ogg"
    # hcm "「あー、いや……俺も無断で遠くまで行ったのがいけなかったしな。まぁ、相模とゆっくり話せてよかったんじゃないか？」"
    voice "audio/log/B023ESS0_HCM022.ogg"
    hcm "「A, yo‘q... Men ham aytmasdan uzoqlashib ketib to‘g‘ri qilmadim. Sagami bilan bemalol gaplashib olganing yaxshi bo‘ldi, to‘g‘rimi?」"

# game/scripts/game/OG_B023ESS0.rpy:521
translate uzbek OG_B023ESS0_7_ea770153:

    # voice "audio/log/B023ESS0_YUI030.ogg"
    # yui "「え？　あー……うん、まぁ」"
    voice "audio/log/B023ESS0_YUI030.ogg"
    yui "「E? A... ha, shunday.」"

# game/scripts/game/OG_B023ESS0.rpy:525
translate uzbek OG_B023ESS0_7_d8d507f8:

    # voice "audio/log/B023ESS0_HCM023.ogg"
    # hcm "「悪かったよ」"
    voice "audio/log/B023ESS0_HCM023.ogg"
    hcm "「Kechirasan.」"

# game/scripts/game/OG_B023ESS0.rpy:535
translate uzbek OG_B023ESS0_7_9fda5579:

    # voice "audio/log/B023ESS0_YUI031.ogg"
    # yui "「……？　なんでヒッキーが謝るの？」"
    voice "audio/log/B023ESS0_YUI031.ogg"
    yui "「...? Nega Hikki uzr so‘rayapti?」"

# game/scripts/game/OG_B023ESS0.rpy:539
translate uzbek OG_B023ESS0_7_c648da0b:

    # voice "audio/log/B023ESS0_HCM024.ogg"
    # hcm "「いやまぁ、その、なんだ。一緒にいるのが俺じゃあな……。相模の顔を知ってりゃ、声をかけられる前に離れて歩くとか、対策できたんだが」"
    voice "audio/log/B023ESS0_HCM024.ogg"
    hcm "「Endi, nima desam ekan... Yoningda men bo‘lganim uchun... Sagamini taniganimda, u gapirmasidan oldin uzoqroq yurardim, biror chora ko‘rardim.」"

# game/scripts/game/OG_B023ESS0.rpy:549
translate uzbek OG_B023ESS0_7_ce0b736a:

    # voice "audio/log/B023ESS0_YUI032.ogg"
    # yui "「ど、どうしてそんなこと言うの？　あたしそんなこと思ってないよ？」"
    voice "audio/log/B023ESS0_YUI032.ogg"
    yui "「Nega bunday deysan? Men unaqa deb o‘ylamayman-ku?」"

# game/scripts/game/OG_B023ESS0.rpy:553
translate uzbek OG_B023ESS0_7_b7d4ab06:

    # voice "audio/log/B023ESS0_HCM025.ogg"
    # hcm "「お前は思わないだろ、知ってるよ。けど、相模はたぶんそうじゃない。あの手の連中のことはわかってるし、ああいう扱いも慣れてる。気にすんな」"
    voice "audio/log/B023ESS0_HCM025.ogg"
    hcm "「Sen unaqa o‘ylamasligingni bilaman. Lekin Sagami boshqacha fikrda. Bunaqa toifadagilarni yaxshi bilaman, bunday muomalaga o‘rganib ham qolganman. E’tibor berma.」"

# game/scripts/game/OG_B023ESS0.rpy:557
translate uzbek OG_B023ESS0_7_750cfd3e:

    # voice "audio/log/B023ESS0_YUI033.ogg"
    # yui "「そんなこと言わないでよ」"
    voice "audio/log/B023ESS0_YUI033.ogg"
    yui "「Unday dema.」"

# game/scripts/game/OG_B023ESS0.rpy:569
translate uzbek OG_B023ESS0_7_49b86b2f:

    # voice "audio/log/B023ESS0_YUI034.ogg"
    # yui "「そ、それにあたし、ヒッキーのこと――」"
    voice "audio/log/B023ESS0_YUI034.ogg"
    yui "「Qo-qolaversa, men Hikkini——」"

# game/scripts/game/OG_B023ESS0.rpy:581
translate uzbek OG_B023ESS0_7_35514c07:

    # voice "audio/log/B023ESS0_YUI035.ogg"
    # yui "「……！」"
    voice "audio/log/B023ESS0_YUI035.ogg"
    yui "「...!」"

# game/scripts/game/OG_B023ESS0.rpy:585
translate uzbek OG_B023ESS0_7_17723692:

    # voice "audio/log/B023ESS0_HCM026.ogg"
    # hcm "「……でけー音だな」"
    voice "audio/log/B023ESS0_HCM026.ogg"
    hcm "「...Juda baland ovoz-a.」"

# game/scripts/game/OG_B023ESS0.rpy:589
translate uzbek OG_B023ESS0_7_ae60ae85:

    # voice "audio/log/B023ESS0_YUI036.ogg"
    # yui "「え？　あ、う、うん……花火、始まっちゃったね」"
    voice "audio/log/B023ESS0_YUI036.ogg"
    yui "「E? A, ha... mushakbozlik boshlanib ketibdi.」"

# game/scripts/game/OG_B023ESS0.rpy:593
translate uzbek OG_B023ESS0_7_6313702e:

    # voice "audio/log/B023ESS0_HCM027.ogg"
    # hcm "「そうだな。ここで立って見てるわけにもいかねぇし……会場の方に移動するか」"
    voice "audio/log/B023ESS0_HCM027.ogg"
    hcm "「To‘g‘ri. Bu yerda tik turib tomosha qilolmaymiz... tomoshagoh tomon yuramizmi?」"

# game/scripts/game/OG_B023ESS0.rpy:603
translate uzbek OG_B023ESS0_7_119e37fa:

    # voice "audio/log/B023ESS0_YUI037.ogg"
    # yui "「……うん」"
    voice "audio/log/B023ESS0_YUI037.ogg"
    yui "「...Xo‘p.」"

# game/scripts/game/OG_B023ESS0.rpy:607
translate uzbek OG_B023ESS0_7_16fb0e75:

    # voice "audio/log/B023ESS0_HCM028.ogg"
    # hcm "「よし、行くか」"
    voice "audio/log/B023ESS0_HCM028.ogg"
    hcm "「Bo‘pti, ketdik.」"

