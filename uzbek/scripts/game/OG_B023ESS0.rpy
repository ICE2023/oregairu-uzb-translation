    # voice "audio/log/B023ESS0_HCM029.ogg"
    # hcm "「あん？」"
    voice "audio/log/B023ESS0_HCM029.ogg"
    hcm "「A?」"

# game/scripts/game/OG_B023ESS0.rpy:631
translate uzbek OG_B023ESS0_7_21594712:

    # voice "audio/log/B023ESS0_YUI039.ogg"
    # yui "「……なんでもない」"
    voice "audio/log/B023ESS0_YUI039.ogg"
    yui "「...Hech narsa.」"

# game/scripts/game/OG_B023ESS0.rpy:635
translate uzbek OG_B023ESS0_7_38692163:

    # voice "audio/log/B023ESS0_HCM030.ogg"
    # hcm "「なんだよ、さっきから……ほら、早くしないと花火終わっちまうぞ」"
    voice "audio/log/B023ESS0_HCM030.ogg"
    hcm "「Boyadan beri nima deyapsan o‘zi... Qani, tezroq bo‘lmasang, mushakbozlik tugab qoladi.」"

# game/scripts/game/OG_B023ESS0.rpy:647
translate uzbek OG_B023ESS0_7_1fd59802:

    # voice "audio/log/B023ESS0_YUI040.ogg"
    # yui "「あっ、すみませーん。ちょっと失礼しまーす」"
    voice "audio/log/B023ESS0_YUI040.ogg"
    yui "「A, kechirasizlar. O‘tib olsak maylimi?」"

# game/scripts/game/OG_B023ESS0.rpy:657
translate uzbek OG_B023ESS0_7_18fe9851:

    # voice "audio/log/B023ESS0_YUI041.ogg"
    # yui "「ふぅ、人が多いなぁ……真っ直ぐ歩くだけで精一杯だね」"
    voice "audio/log/B023ESS0_YUI041.ogg"
    yui "「Uf-f, odam juda ko‘p-a... To‘g‘ri yurishning o‘zi qiyin bo‘lyapti.」"

# game/scripts/game/OG_B023ESS0.rpy:661
translate uzbek OG_B023ESS0_7_9e7e3723:

    # voice "audio/log/B023ESS0_HCM031.ogg"
    # hcm "「すげぇ混みようだな」"
    voice "audio/log/B023ESS0_HCM031.ogg"
    hcm "「Tiqilinch-ku.」"

# game/scripts/game/OG_B023ESS0.rpy:665
translate uzbek OG_B023ESS0_7_3121f38b:

    # voice "audio/log/B023ESS0_YUI042.ogg"
    # yui "「うん……どこかに座れる場所でもあるといいんだけど」"
    voice "audio/log/B023ESS0_YUI042.ogg"
    yui "「Ha... Qayerdadir o‘tiradigan joy bo‘lsa yaxshi bo‘lardi.」"

# game/scripts/game/OG_B023ESS0.rpy:669
translate uzbek OG_B023ESS0_7_cb018c5c:

    # voice "audio/log/B023ESS0_HCM032.ogg"
    # hcm "「座るって言ってもお前、浴衣で地べたってわけにもいかねぇだろ。こんなに混むって知ってたらビニールシートくらい用意してきたんだけどな」"
    voice "audio/log/B023ESS0_HCM032.ogg"
    hcm "「O‘tiramiz deysan-u, lekin yukatada yerga o‘tirib bo‘lmaydi-ku. Shunchalik tiqilinch bo‘lishini bilganimda, o‘zim bilan to‘shama olib kelardim.」"

# game/scripts/game/OG_B023ESS0.rpy:679
translate uzbek OG_B023ESS0_7_5f06b60d:

    # voice "audio/log/B023ESS0_YUI043.ogg"
    # yui "「むー、なんかあたしが悪いみたい……ごめん、言っておけばよかったね」"
    voice "audio/log/B023ESS0_YUI043.ogg"
    yui "「Mmm, xuddi men aybdordekman... uzr, aytib qo‘yishim kerak edi.」"

# game/scripts/game/OG_B023ESS0.rpy:683
translate uzbek OG_B023ESS0_7_e6b01a0b:

    # voice "audio/log/B023ESS0_HCM033.ogg"
    # hcm "「別に……俺もそこまで頭が回らなかっただけの話だ。さっさと適当な場所見つけようぜ」"
    voice "audio/log/B023ESS0_HCM033.ogg"
    hcm "「Hechqisi yo‘q... Men ham bu haqda o‘ylamabman, xolos. Tezroq biror joy topaylik.」"

# game/scripts/game/OG_B023ESS0.rpy:693
translate uzbek OG_B023ESS0_7_e945d9ba:

    # voice "audio/log/B023ESS0_YUI044.ogg"
    # yui "「あっ、ヒッキー、あっち空いてるみたいだよ！」"
    voice "audio/log/B023ESS0_YUI044.ogg"
    yui "「A, Hikki, anavi yer bo‘shganga o‘xshaydi!」"

# game/scripts/game/OG_B023ESS0.rpy:697
translate uzbek OG_B023ESS0_7_16d0b93d:

    # voice "audio/log/B023ESS0_HCM034.ogg"
    # hcm "「……いや、あそこは止めておこう」"
    voice "audio/log/B023ESS0_HCM034.ogg"
    hcm "「...Yo‘q, u yerga bormaymiz.」"

# game/scripts/game/OG_B023ESS0.rpy:707
translate uzbek OG_B023ESS0_7_fd687b36:

    # voice "audio/log/B023ESS0_YUI045.ogg"
    # yui "「えっ、なんで？」"
    voice "audio/log/B023ESS0_YUI045.ogg"
    yui "「E, nega?」"

# game/scripts/game/OG_B023ESS0.rpy:711
translate uzbek OG_B023ESS0_7_509816ef:

    # hcm "（さすがにトイレの前はな……俺だけなら構わないけど、由比ケ浜にはキツいだろ）"
    hcm "（Hojatxona oldi to‘g‘ri kelmaydi-da... O‘zim bo‘lsam mayli edi-yu, Yuigahamaga noqulay bo‘ladi.）"

# game/scripts/game/OG_B023ESS0.rpy:713
translate uzbek OG_B023ESS0_7_ac94af14:

    # voice "audio/log/B023ESS0_HCM035.ogg"
    # hcm "「とにかく、他をあたろうぜ」"
    voice "audio/log/B023ESS0_HCM035.ogg"
    hcm "「Xullas, boshqa joy qaraylik.」"

# game/scripts/game/OG_B023ESS0.rpy:717
translate uzbek OG_B023ESS0_7_31389d13:

    # hcm "（ベンチにも縁石にもつけいる隙はなし……か。みんな考えてることは同じだな。せめて新聞紙のひとつでもあれば座れるんだが）"
    hcm "（O‘rindiqlar ham, yo‘l chetidagi toshlar ham band... hamma bir xil narsani o‘ylagan ekan. Hech bo‘lmasa biror gazeta bo‘lganda, o‘tirish mumkin edi.）"

# game/scripts/game/OG_B023ESS0.rpy:719
translate uzbek OG_B023ESS0_7_ee998305:

    # hcm "（行けども行けども人だらけ。どこまで歩けばいいんだ、いったい）"
    hcm "（Yurganim sari odam ko‘payib ketyapti. Qayergacha yurishimiz kerak o‘zi?）"

# game/scripts/game/OG_B023ESS0.rpy:721
translate uzbek OG_B023ESS0_7_04e911f9:

    # voice "audio/log/B023ESS0_HCM036.ogg"
    # hcm "「由比ケ浜、どうする？　もっと先まで行ってみるか？」"
    voice "audio/log/B023ESS0_HCM036.ogg"
    hcm "「Yuigahama, nima qilamiz? Yana oldinroqqa yurib ko‘ramizmi?」"

# game/scripts/game/OG_B023ESS0.rpy:725
translate uzbek OG_B023ESS0_7_170b4ea2:

    # voice "audio/log/B023ESS0_HCM037.ogg"
    # hcm "「……由比ケ浜？」"
    voice "audio/log/B023ESS0_HCM037.ogg"
    hcm "「...Yuigahama?」"

# game/scripts/game/OG_B023ESS0.rpy:731
translate uzbek OG_B023ESS0_7_1c46230b:

    # hcm "（……いない？　やべぇ、はぐれたか）"
    hcm "（...Yo‘qmi? Yomon bo‘ldi-ku, adashib qoldim shekilli.）"

# game/scripts/game/OG_B023ESS0.rpy:733
translate uzbek OG_B023ESS0_7_659949ba:

    # voice "audio/log/B023ESS0_HCM038.ogg"
    # hcm "「おい、由比ケ浜！　由比ケ浜！？　……くそっ」"
    voice "audio/log/B023ESS0_HCM038.ogg"
    hcm "「Hoy, Yuigahama! Yuigahama!? ...La'nat.」"

# game/scripts/game/OG_B023ESS0.rpy:743
translate uzbek OG_B023ESS0_7_fcdff9c3:

    # hcm "（こっちに戻ってきてるかと思ったけど、いないか……）"
    hcm "（Bu tomonga qaytib kelgandir deb o‘ylagandim, yo‘q ekan...）"

# game/scripts/game/OG_B023ESS0.rpy:745
translate uzbek OG_B023ESS0_7_946c22f9:

    # hcm "（携帯は全然通じねぇし、どうすればいいんだ。このままじゃ冗談なしに花火終わっちまうぞ）"
    hcm "（Telefon ham ishlamayapti, nima qilsam ekan. Bu ketishda mushakbozlik rostdan ham tugab qoladi-ku.）"

# game/scripts/game/OG_B023ESS0.rpy:749
translate uzbek OG_B023ESS0_7_c06d719b:

    # voice "audio/log/b023ess0_X25000.ogg"
    # X25 "「おい、前見て歩けよ」"
    voice "audio/log/b023ess0_X25000.ogg"
    X25 "「Hoy, ko‘zingga qarab yurmaysanmi?」"

# game/scripts/game/OG_B023ESS0.rpy:753
translate uzbek OG_B023ESS0_7_ed23d073:

    # voice "audio/log/B023ESS0_HCM039.ogg"
    # hcm "「……すんません」"
    voice "audio/log/B023ESS0_HCM039.ogg"
    hcm "「...Kechirasiz.」"

# game/scripts/game/OG_B023ESS0.rpy:759
translate uzbek OG_B023ESS0_7_5aabf8b2:

    # hcm "（……これだからやっぱり、人ごみは嫌いだ）"
    hcm "（...Shuning uchun ham tiqilinch joylarni yomon ko‘raman-da.）"

