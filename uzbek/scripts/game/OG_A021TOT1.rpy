    voice "audio/log/A021TOT1_TOT002.ogg"
    tot "「Sobl deganing kim?」"

# game/scripts/game/OG_A021TOT1.rpy:38
translate uzbek OG_A021TOT1_0_5a049285:

    # voice "audio/log/A021TOT1_HCM002.ogg"
    # hcm "「由比ケ浜んとこの犬なんだけど、昨日までうちで預かってたんだ。あいつ今、家族旅行中でさ」"
    voice "audio/log/A021TOT1_HCM002.ogg"
    hcm "「Yuigahamaning iti. Kechagacha bizning uyda edi. U hozir oilaviy sayohatda-da.」"

# game/scripts/game/OG_A021TOT1.rpy:48
translate uzbek OG_A021TOT1_0_21d9d9fd:

    # voice "audio/log/A021TOT1_TOT003.ogg"
    # tot "「へぇ〜、きっとかわいいんだろうね」"
    voice "audio/log/A021TOT1_TOT003.ogg"
    tot "「E-e, yoqimtoy bo‘lsa kerak-a.」"

# game/scripts/game/OG_A021TOT1.rpy:52
translate uzbek OG_A021TOT1_0_63111e2c:

    # hcm "（お前の方がな……じゃなくて）"
