# game/scripts/game/OG_A021TOT1.rpy:54
translate uzbek OG_A021TOT1_0_fe179c18:

    # voice "audio/log/A021TOT1_HCM003.ogg"
    # hcm "「そうだな、あれはもうそういうふうにデザインされた生きものって感じだな。小町がメロメロになってた」"
    voice "audio/log/A021TOT1_HCM003.ogg"
    hcm "「Ha, xuddi shirin qilib yaratilgan jonzotga o‘xshaydi. Komachi uni rosa yaxshi ko‘rib qolgandi.」"

# game/scripts/game/OG_A021TOT1.rpy:58
translate uzbek OG_A021TOT1_0_5d40d717:

    # voice "audio/log/A021TOT1_TOT004.ogg"
    # tot "「そうなんだー。一度見てみたいなぁ」"
    voice "audio/log/A021TOT1_TOT004.ogg"
    tot "「Shunaqami. Bir ko‘rgim kelyapti.」"

# game/scripts/game/OG_A021TOT1.rpy:62
translate uzbek OG_A021TOT1_0_bc8020bc:

