# game/scripts/game/OG_A052ESS0.rpy:56
translate uzbek OG_A052ESS0_0_d4796411:

    # voice "audio/log/A052ESS0_KOM003.ogg"
    # kom "「どの映画を見るか相談して決める、以外のところの全部が、まっっっったくダメです！」"
    voice "audio/log/A052ESS0_KOM003.ogg"
    kom "「Qaysi kinoni ko‘rishni maslahatlashganingizdan tashqari, hamma-hammasi, u-mu-man o‘xshamadi!」"

# game/scripts/game/OG_A052ESS0.rpy:60
