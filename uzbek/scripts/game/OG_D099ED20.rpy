    voice "audio/log/D099ED20_TAI006.ogg"
    tai "Yo'g'e... tasodifan men tomonga uchib keldi. Unda, aka, otyapman."

# game/scripts/game/OG_D099ED20.rpy:266
translate uzbek OG_D099ED20_0_f371a9d1:

    # voice "audio/log/D099ED20_HCM011.ogg"
    # hcm "「……ちょっと待て。お前な、さっきから当然のように呼んでるが、お兄さんって呼んだら殺すと何度言わせる」"
    voice "audio/log/D099ED20_HCM011.ogg"
    hcm "...Shoshmay tur. Boyadan beri tabiiydek chaqiryapsan-u, aka desang o'ldiraman deb necha marta aytishim kerak."

# game/scripts/game/OG_D099ED20.rpy:270
translate uzbek OG_D099ED20_0_ae9b6a6e:

    # voice "audio/log/D099ED20_KAW006.ogg"
    # kaw "「ちょっとあんた。あんたがシスコンなのはわかったけど、うちの大志にこれ以上喧嘩売ったらただじゃおかないよ？」"
    voice "audio/log/D099ED20_KAW006.ogg"
    kaw "Hoy sen. Siskonligingni tushundik, lekin mening Taishimga yana biror gap aytsang, o'zingdan ko'r."

# game/scripts/game/OG_D099ED20.rpy:274
translate uzbek OG_D099ED20_0_9f03c965:

    # voice "audio/log/D099ED20_HCM012.ogg"
    # hcm "「お前なあ、自分が今なにしゃべってるのかわかってんのか？　今まさに自分はブラコンですって主張してるようなもんだぞ」"
    voice "audio/log/D099ED20_HCM012.ogg"
    hcm "Sen o'zing nima deyayotganingni tushunyapsanmi? Hozirgina men ukamni yaxshi ko'raman deb jar solding-ku."

# game/scripts/game/OG_D099ED20.rpy:286
translate uzbek OG_D099ED20_0_683c52eb:

    # voice "audio/log/D099ED20_KAW007.ogg"
    # kaw "「なっ！　あんたさ、海に沈められたいの？」"
    voice "audio/log/D099ED20_KAW007.ogg"
    kaw "Nima! Seni dengizga cho'ktirib yuboraymi?"

# game/scripts/game/OG_D099ED20.rpy:290
translate uzbek OG_D099ED20_0_5873ae2b:

    # voice "audio/log/D099ED20_HCM013.ogg"
    # hcm "「その発言は似合いすぎるからやめろ……」"
    voice "audio/log/D099ED20_HCM013.ogg"
    hcm "Bu gap senga juda yarashyapti, qo'ysang-chi..."

# game/scripts/game/OG_D099ED20.rpy:294
