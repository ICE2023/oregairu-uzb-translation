    # voice "audio/log/D099ED20_KAW005.ogg"
    # kaw "「……ま、あんたたちがそう言うなら」"
    voice "audio/log/D099ED20_KAW005.ogg"
    kaw "...Mayli, shunday deyayotgan bo'lsanglar."

# game/scripts/game/OG_D099ED20.rpy:218
