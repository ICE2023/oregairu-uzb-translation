# game/scripts/game/OG_TALK_HIR1.rpy:160
translate uzbek OG_TALK_HIR1_5_8304beb3:

    # voice "audio/log/HIR1A5_HIR002.ogg"
    # hir "「放課後、進路相談に来る生徒がいきなり黙り込むことがあるから、君もその口かと」"
    voice "audio/log/HIR1A5_HIR002.ogg"
    hir "「Darsdan keyin kelajak haqida maslahatlashgani kelgan o‘quvchilar ham birdan jim bo‘lib qolishadi, sen ham shulardanmisan deb o‘yladim.」"

# game/scripts/game/OG_TALK_HIR1.rpy:164
translate uzbek OG_TALK_HIR1_5_f96d71da:

    # hcm "「いや、俺の進路はもう決定済みですから」"
    hcm "「Yo‘q, mening kelajagim allaqachon aniq.」"

# game/scripts/game/OG_TALK_HIR1.rpy:166
