# game/scripts/game/OG_N024YUK2.rpy:62
translate uzbek OG_N024YUK2_0_ae42a10f:

    # voice "audio/log/N024YUK2_YUK004.ogg"
    # yuk "「鈴虫の鳴き声は外国人には雑音にしか聞こえないらしいわ。興味の範疇外にあるときはどんな言葉も雑音でしかないということね」"
    voice "audio/log/N024YUK2_YUK004.ogg"
    yuk "Eshitishimcha, chet elliklar uchun chigirtkaning ovozi shunchaki shovqin ekan. Ya'ni, qiziqmagan narsang haqidagi har qanday so'z ham shunchaki shovqin bo'lib tuyular ekan-da."

# game/scripts/game/OG_N024YUK2.rpy:66
translate uzbek OG_N024YUK2_0_66afd91b:

    # voice "audio/log/N024YUK2_HCM002.ogg"
    # hcm "「雑学教えてくれてどうも。邪魔して悪かったよ」"
    voice "audio/log/N024YUK2_HCM002.ogg"
    hcm "Foydali ma'lumot uchun rahmat. Xalaqit berganim uchun uzr."

# game/scripts/game/OG_N024YUK2.rpy:76
translate uzbek OG_N024YUK2_0_84538970:

    # voice "audio/log/N024YUK2_YUK005.ogg"
    # yuk "「……皮肉を介する理解度が高いのだけは褒めてあげるわ」"
    voice "audio/log/N024YUK2_YUK005.ogg"
    yuk "...Kinoyani yaxshi tushunishingni maqtagim keldi."

# game/scripts/game/OG_N024YUK2.rpy:80
translate uzbek OG_N024YUK2_0_09009896:

    # voice "audio/log/N024YUK2_HCM003.ogg"
    # hcm "「ぼっちは人の言葉の裏を読む力に長けてるからな。何より、言われ慣れてる」"
    voice "audio/log/N024YUK2_HCM003.ogg"
    hcm "Yolg'iz odamlar so'zning tagidagi ma'noni tez ilg'ashga usta bo'lishadi. Eng muhimi, bunaqa gaplarga o'rganib qolganman."

# game/scripts/game/OG_N024YUK2.rpy:84
translate uzbek OG_N024YUK2_0_02fc0643:

    # voice "audio/log/N024YUK2_YUK006.ogg"
    # yuk "「その反省を次に生かさないところは詰ってあげるわ、鈴虫」"
    voice "audio/log/N024YUK2_YUK006.ogg"
    yuk "Xulosa chiqarmasligingni esa tanqid qilaman, chigirtka."

# game/scripts/game/OG_N024YUK2.rpy:88
