# game/scripts/game/OG_A031ESS1.rpy:20
translate uzbek OG_A031ESS1_0_c0a82313:

    # voice "audio/log/A031ESS1_YUK000.ogg"
    # yuk "「別に構わないわ。なるべく早めにことを進めたかったのは、こちらの希望でもあるのだから。それより」"
    voice "audio/log/A031ESS1_YUK000.ogg"
    yuk "「Hechqisi yo‘q. Tezroq hal qilishni o‘zim ham xohlayotgandim. Undan ko‘ra...」"

# game/scripts/game/OG_A031ESS1.rpy:34
translate uzbek OG_A031ESS1_0_a1f11fb9:

    # voice "audio/log/A031ESS1_YUK001.ogg"
    # yuk "「なぜ、比企谷くんまで同席しているのかしら」"
    voice "audio/log/A031ESS1_YUK001.ogg"
    yuk "「Nega Hikigaya ham shu yerda?」"

# game/scripts/game/OG_A031ESS1.rpy:48
translate uzbek OG_A031ESS1_0_df2c84ff:

    # voice "audio/log/A031ESS1_KOM001.ogg"
    # kom "「あはは……ま、まぁ、兄にも関係のある話なので……」"
    voice "audio/log/A031ESS1_KOM001.ogg"
    kom "「A-ha-ha... bu gapning akamga ham aloqasi bor-da...」"

# game/scripts/game/OG_A031ESS1.rpy:52
translate uzbek OG_A031ESS1_0_a9d66f68:

    # voice "audio/log/A031ESS1_HCM000.ogg"
    # hcm "「え！？　そうなの！？　超初耳なんだけど」"
    voice "audio/log/A031ESS1_HCM000.ogg"
    hcm "「E!? Shunaqami!? Birinchi marta eshitishim!」"

# game/scripts/game/OG_A031ESS1.rpy:62
translate uzbek OG_A031ESS1_0_ce46e398:

    # voice "audio/log/A031ESS1_KOM002.ogg"
    # kom "「うん、今言ったからね」"
    voice "audio/log/A031ESS1_KOM002.ogg"
    kom "「Ha, hozir aytdim-ku.」"

# game/scripts/game/OG_A031ESS1.rpy:72
translate uzbek OG_A031ESS1_0_11d9df4a:

    # voice "audio/log/A031ESS1_YUK002.ogg"
    # yuk "「それで？　小町さんのたってのお願いといいうのはなんなのかしら？」"
    voice "audio/log/A031ESS1_YUK002.ogg"
    yuk "「Xo‘sh? Komachining shoshilinch iltimosi nima ekan?」"

# game/scripts/game/OG_A031ESS1.rpy:82
translate uzbek OG_A031ESS1_0_0fe99288:

    # voice "audio/log/A031ESS1_KOM003.ogg"
    # kom "「あっ、はい。実はですね――」"
    voice "audio/log/A031ESS1_KOM003.ogg"
    kom "「A, ha. Gap shundaki——」"

# game/scripts/game/OG_A031ESS1.rpy:94
translate uzbek OG_A031ESS1_0_33fe681c:

    # voice "audio/log/A031ESS1_KOM004.ogg"
    # kom "「このあたりで兄を更生させたいんです！」"
    voice "audio/log/A031ESS1_KOM004.ogg"
    kom "「Fursatdan foydalanib, akamni tarbiyalab olmoqchiman!」"

# game/scripts/game/OG_A031ESS1.rpy:98
translate uzbek OG_A031ESS1_0_86140c93:

    # voice "audio/log/A031ESS1_YUK003.ogg"
    # yuk "「無理ね」"
    voice "audio/log/A031ESS1_YUK003.ogg"
    yuk "「Iloji yo‘q.」"

# game/scripts/game/OG_A031ESS1.rpy:108
translate uzbek OG_A031ESS1_0_59b52068:

    # voice "audio/log/A031ESS1_KOM005.ogg"
    # kom "「わ、即答だー」"
    voice "audio/log/A031ESS1_KOM005.ogg"
    kom "「Voy, darrov javob berdilar-a.」"

# game/scripts/game/OG_A031ESS1.rpy:114
translate uzbek OG_A031ESS1_0_bb1b6f5c:

    # voice "audio/log/A031ESS1_HCM001.ogg"
    # hcm "「待て。そもそも俺は更生する必要がないし、変わらなきゃいけないって言う強迫観念がもう同調圧力にな――」"
    voice "audio/log/A031ESS1_HCM001.ogg"
    hcm "「To‘xtanglar. Avvalo men tarbiyaga muhtoj emasman, o‘zgarishim kerak degan gaplaring bilan bosim o‘tka——」"

# game/scripts/game/OG_A031ESS1.rpy:124
translate uzbek OG_A031ESS1_0_5ac5a84e:

    # voice "audio/log/A031ESS1_KOM006.ogg"
    # kom "「うん、お兄ちゃん今そういうのはいいからしばらく黙ってて、お願い」"
    voice "audio/log/A031ESS1_KOM006.ogg"
    kom "「Akajon, hozir bunaqa gaplaringiz kerak emas, iltimos, biroz jim turing.」"

# game/scripts/game/OG_A031ESS1.rpy:128
translate uzbek OG_A031ESS1_0_bb4b4020:

    # voice "audio/log/A031ESS1_HCM002.ogg"
    # hcm "「む……」"
    voice "audio/log/A031ESS1_HCM002.ogg"
    hcm "「Hm...」"

# game/scripts/game/OG_A031ESS1.rpy:138
translate uzbek OG_A031ESS1_0_03a4f8d2:

    # voice "audio/log/A031ESS1_KOM007.ogg"
    # kom "「あの、確かに無理とおっしゃるのもわかるんですけど、一応話だけでも聞いてもらえませんか？」"
    voice "audio/log/A031ESS1_KOM007.ogg"
    kom "「To‘g‘ri, iloji yo‘q deganingizni tushunaman-u, lekin gapimni oxirigacha eshitsangiz bo‘ladimi?」"

# game/scripts/game/OG_A031ESS1.rpy:142
translate uzbek OG_A031ESS1_0_65f2b28d:

    # voice "audio/log/A031ESS1_YUK004.ogg"
    # yuk "「ええ構わないわ、どうぞ続けて？」"
    voice "audio/log/A031ESS1_YUK004.ogg"
    yuk "「Mayli, davom etaver.」"

# game/scripts/game/OG_A031ESS1.rpy:146
translate uzbek OG_A031ESS1_0_e9a7ff0e:

    # voice "audio/log/A031ESS1_KOM008.ogg"
    # kom "「すみませんです……そう、それでですね、更生の第一歩としてまずはマトモな友達作りが必要だと小町的には思うんですけど、どうでしょうか？」"
    voice "audio/log/A031ESS1_KOM008.ogg"
    kom "「Rahmat... Xullas, tarbiyaning birinchi qadami sifatida, avvalo tuzukroq do‘st orttirish kerak, deb o‘ylayman. Nima deysiz?」"

# game/scripts/game/OG_A031ESS1.rpy:150
translate uzbek OG_A031ESS1_0_bf7fb436:

    # voice "audio/log/A031ESS1_YUK005.ogg"
    # yuk "「……小町さん、そもそもマトモな友達作りという点に関してハードルが高すぎるわ。その、……いろいろと問題があるし。申し訳ないのだけれど、無理よ」"
    voice "audio/log/A031ESS1_YUK005.ogg"
    yuk "「...Komachi, tuzukroq do‘st orttirishning o‘zi juda qiyin masala. Chunki... muammolar ko‘p. Kechirasiz-u, buning iloji yo‘q.」"

# game/scripts/game/OG_A031ESS1.rpy:160
translate uzbek OG_A031ESS1_0_77134937:

    # voice "audio/log/A031ESS1_KOM009.ogg"
    # kom "「あはは、またバッサリだなー！」"
    voice "audio/log/A031ESS1_KOM009.ogg"
    kom "「A-ha-ha, yana rad etdilar-ku!」"

# game/scripts/game/OG_A031ESS1.rpy:164
translate uzbek OG_A031ESS1_0_c7de15e5:

    # voice "audio/log/A031ESS1_HCM003.ogg"
    # hcm "「つーか、友達とか作ってどうすんだよ？　スーパーのタイムセール並みになにかお得なことでもあるのか？」"
    voice "audio/log/A031ESS1_HCM003.ogg"
    hcm "「O‘zi do‘st orttirib nima qilaman? Supermarketdagi chegirmalarga o‘xshab biror foydasi bormi?」"

# game/scripts/game/OG_A031ESS1.rpy:176
translate uzbek OG_A031ESS1_0_9d39435d:

    # voice "audio/log/A031ESS1_KOM010.ogg"
    # kom "「あ、あるよ！」"
    voice "audio/log/A031ESS1_KOM010.ogg"
    kom "「Bo-bor albatta!」"

# game/scripts/game/OG_A031ESS1.rpy:180
translate uzbek OG_A031ESS1_0_b7144dc2:

    # voice "audio/log/A031ESS1_HCM004.ogg"
    # hcm "「たとえば？」"
    voice "audio/log/A031ESS1_HCM004.ogg"
    hcm "「Masalan?」"

# game/scripts/game/OG_A031ESS1.rpy:190
translate uzbek OG_A031ESS1_0_33c436f3:

    # voice "audio/log/A031ESS1_KOM011.ogg"
    # kom "「休んだら宿題見せてくれるし、ノート見せてくれるし、休みの日には遊びに出かけたり……楽しいこといっぱいだよ！」"
    voice "audio/log/A031ESS1_KOM011.ogg"
    kom "「Dars qoldirsang, vazifalarni, daftarni berib turadi, dam olish kunlari birga aylanishga chiqasan... xullas, mazza qilasan!」"

# game/scripts/game/OG_A031ESS1.rpy:194
translate uzbek OG_A031ESS1_0_3e588119:

    # voice "audio/log/A031ESS1_HCM005.ogg"
    # hcm "「……それは友達じゃなくて宿題の回答や授業ノートが欲しいだけだろ。自分でやったほうが早いし、身になる。休みの日ったってそれぞれ予定があるだろ？」"
    voice "audio/log/A031ESS1_HCM005.ogg"
    hcm "「...Bu do‘stlik emas, shunchaki uyga vazifaning javobi yoki daftarni ko‘chirish-ku. O‘zi bajargani tezroq va foydaliroq bo‘ladi. Dam olish kunlari esa hammaning o‘z rejasi bor.」"

# game/scripts/game/OG_A031ESS1.rpy:204
translate uzbek OG_A031ESS1_0_08574208:

    # voice "audio/log/A031ESS1_HCM006.ogg"
    # hcm "「俺は休みの日は家でゆっくりしたい派だし、仮に誘われても断るのも心苦しいだろうが。逆に誘って無理して付き合わせるのも悪いしな」"
    voice "audio/log/A031ESS1_HCM006.ogg"
    hcm "「Men dam olish kunlari uyda maza qilib yotishni xohlayman, agar chaqirishsa rad etish ham noqulay. Aksincha, o‘zim taklif qilib, majburlab olib yurish ham yaxshi emas.」"

# game/scripts/game/OG_A031ESS1.rpy:214
translate uzbek OG_A031ESS1_0_5b10232c:

    # voice "audio/log/A031ESS1_HCM007.ogg"
    # hcm "「だいたい、利用価値の有無や気遣いによって成立するもんを友達とは呼ばん」"
    voice "audio/log/A031ESS1_HCM007.ogg"
    hcm "「Umuman olganda, foyda-ziyon yoki majburiyat yuzasidan bo‘lgan munosabatni do‘stlik deb bo‘lmaydi.」"

# game/scripts/game/OG_A031ESS1.rpy:224
translate uzbek OG_A031ESS1_0_6f498693:

    # voice "audio/log/A031ESS1_KOM012.ogg"
    # kom "「う……、度重なる正論……」"
    voice "audio/log/A031ESS1_KOM012.ogg"
    kom "「Ihh... rosa falsafa so‘qdingiz-ku...」"

# game/scripts/game/OG_A031ESS1.rpy:228
translate uzbek OG_A031ESS1_0_3c4686c7:

    # voice "audio/log/A031ESS1_HCM008.ogg"
    # hcm "「その点、ぼっちなら、時間がありあまってるから勉強くらいしかやることなくて成績あがるし」"
    voice "audio/log/A031ESS1_HCM008.ogg"
    hcm "「Shu jihatdan, yolg‘iz bo‘lsang, vaqting ko‘p bo‘lgani uchun faqat o‘qish bilan shug‘ullanasan va baholaring yaxshilanadi.」"

# game/scripts/game/OG_A031ESS1.rpy:232
translate uzbek OG_A031ESS1_0_e926f9b9:

    # voice "audio/log/A031ESS1_HCM009.ogg"
    # hcm "「誰かに合わせる必要もねーから好きなこと好きなだけ出来るし、人間関係の気苦労やしがらみとも無縁だから時として大胆な行動だってできるぞ」"
    voice "audio/log/A031ESS1_HCM009.ogg"
    hcm "「Birovga moslashish shart emas, xohlagan ishingni xohlagancha qilasan, odamlar bilan munosabatdagi boshog‘riq va g‘iybatlardan uzoq bo‘lasan, ba’zida hatto dadil ishlarga ham qo‘l urishing mumkin.」"

# game/scripts/game/OG_A031ESS1.rpy:236
translate uzbek OG_A031ESS1_0_3adcd25a:

    # voice "audio/log/A031ESS1_HCM010.ogg"
    # hcm "「知ってるか？　人生っていうのは限りがあるんだ。その中で最大限自分のしたいことをする、これこそが自由、これこそが生きるということだ」"
    voice "audio/log/A031ESS1_HCM010.ogg"
    hcm "「Bilasanmi? Umr qisqa. Shu vaqt ichida o‘zing xohlagan ishni qilish — mana shu haqiqiy erkinlik, mana shu haqiqiy hayot.」"

# game/scripts/game/OG_A031ESS1.rpy:240
translate uzbek OG_A031ESS1_0_0249ddc8:

    # voice "audio/log/A031ESS1_HCM011.ogg"
    # hcm "「つまり、結論として、ぼっちこそが最高に豊かな人生を送れる最強の勝ち組ライフスタイル！　ってことだよ」"
    voice "audio/log/A031ESS1_HCM011.ogg"
    hcm "「Xullas, xulosa shuki, yolg‘izlik — eng mazmunli hayot kechirishga imkon beradigan, g‘oliblarga xos eng zo‘r hayot tarzi!」"

# game/scripts/game/OG_A031ESS1.rpy:252
translate uzbek OG_A031ESS1_0_c924d5d2:

    # voice "audio/log/A031ESS1_YUK006.ogg"
    # yuk "「あなたって、本当に……なんていうのかしら、呆れるくらい下方向にだけ前向きなのね」"
    voice "audio/log/A031ESS1_YUK006.ogg"
    yuk "「Sen rostdan ham... nima desam ekan, faqat pastga qarab intilishda tengsiz ekansan.」"

# game/scripts/game/OG_A031ESS1.rpy:256
translate uzbek OG_A031ESS1_0_73ae4c90:

    # voice "audio/log/A031ESS1_HCM012.ogg"
    # hcm "「よせよせ、照れるだろ」"
    voice "audio/log/A031ESS1_HCM012.ogg"
    hcm "「Qo‘ysang-chi, xijolat qilyapsan.」"

# game/scripts/game/OG_A031ESS1.rpy:268
translate uzbek OG_A031ESS1_0_b84db0ab:

    # voice "audio/log/A031ESS1_KOM013.ogg"
    # kom "「たぶん褒められてないよ、お兄ちゃん！」"
    voice "audio/log/A031ESS1_KOM013.ogg"
    kom "「Sizni maqtashmadi, akajon!」"

# game/scripts/game/OG_A031ESS1.rpy:278
translate uzbek OG_A031ESS1_0_b3e2fa16:

    # voice "audio/log/A031ESS1_KOM014.ogg"
    # kom "「そ、それにほら、そういうところも更生していかないとだよ！」"
    voice "audio/log/A031ESS1_KOM014.ogg"
    kom "「Qo-qolaversa, mana shu odatlaringizni ham to‘g‘rilashingiz kerak!」"

# game/scripts/game/OG_A031ESS1.rpy:282
translate uzbek OG_A031ESS1_0_aa58ccf0:

    # voice "audio/log/A031ESS1_HCM013.ogg"
    # hcm "「いやだから更生いらないし。そもそも、お前の言う『友達』ってなんなんだよ、どこからどこまでが友達なの？」"
    voice "audio/log/A031ESS1_HCM013.ogg"
    hcm "「Aytdim-ku, menga tarbiya kerak emas deb. Umuman, sen aytayotgan “do‘st” o‘zi nima, kimlargacha do‘st hisoblanadi?」"

# game/scripts/game/OG_A031ESS1.rpy:296
translate uzbek OG_A031ESS1_0_04eee5ea:

    # voice "audio/log/A031ESS1_YUK007.ogg"
    # yuk "「……そうね、まずは友達の定義から必要かもしれないわね」"
    voice "audio/log/A031ESS1_YUK007.ogg"
    yuk "「...To‘g‘ri, avval do‘stlik nima ekanligini aniqlashtirib olish kerak.」"

# game/scripts/game/OG_A031ESS1.rpy:306
translate uzbek OG_A031ESS1_0_9e3b8dd6:

    # voice "audio/log/A031ESS1_KOM015.ogg"
    # kom "「えっ、そ、そういうものですか？」"
    voice "audio/log/A031ESS1_KOM015.ogg"
    kom "「E, shu-shunaqami?」"

# game/scripts/game/OG_A031ESS1.rpy:310
translate uzbek OG_A031ESS1_0_3d81ad4e:

    # voice "audio/log/A031ESS1_YUK008.ogg"
    # yuk "「少なくとも、比企谷くんには必要だと思うわ。でなければ、万が一奇跡的になにかの間違いで友達ができたとしても、本人が友達と認識できないもの」"
    voice "audio/log/A031ESS1_YUK008.ogg"
    yuk "「Hech bo‘lmaganda Hikigaya uchun bu zarur. Bo‘lmasa, mabodo mo‘jiza ro‘y berib, tasodifan do‘st orttirsa ham, o‘zi buni tushunmay qolishi mumkin.」"

# game/scripts/game/OG_A031ESS1.rpy:320
translate uzbek OG_A031ESS1_0_4bc6407e:

    # voice "audio/log/A031ESS1_KOM016.ogg"
    # kom "「た、確かに。けど、友達の定義って改めて言われると……雪乃さんはどう思います？」"
    voice "audio/log/A031ESS1_KOM016.ogg"
    kom "「To‘-to‘g‘ri. Lekin, do‘stlikning ta’rifi deganda... Yukino opa, siz nima deb o‘ylaysiz?」"

# game/scripts/game/OG_A031ESS1.rpy:330
translate uzbek OG_A031ESS1_0_d4404bb8:

    # voice "audio/log/A031ESS1_YUK009.ogg"
    # yuk "「……」"
    voice "audio/log/A031ESS1_YUK009.ogg"
    yuk "「...」"

# game/scripts/game/OG_A031ESS1.rpy:340
translate uzbek OG_A031ESS1_0_9aecab54:

    # voice "audio/log/A031ESS1_KOM017.ogg"
    # kom "「……雪乃さん？」"
    voice "audio/log/A031ESS1_KOM017.ogg"
    kom "「...Yukino opa?」"

# game/scripts/game/OG_A031ESS1.rpy:350
translate uzbek OG_A031ESS1_0_59db4507:

    # voice "audio/log/A031ESS1_YUK010.ogg"
    # yuk "「そ、そうね……でもまずは、本人がどう定義しているかを明らかにするところからではないかしら？」"
    voice "audio/log/A031ESS1_YUK010.ogg"
    yuk "「Ha, ha... lekin avval uning o‘zi buni qanday tushunishini bilib olishimiz kerak emasmi?」"

# game/scripts/game/OG_A031ESS1.rpy:362
translate uzbek OG_A031ESS1_0_21c00cef:

    # voice "audio/log/A031ESS1_HCM014.ogg"
    # hcm "「いや、わかってたら俺、問題提起してないでしょ。わからんっつーの」"
    voice "audio/log/A031ESS1_HCM014.ogg"
    hcm "「Agar bilganimda, bu masalani ko‘tarmasdim-ku. Bilmayman deyapman.」"

# game/scripts/game/OG_A031ESS1.rpy:366
translate uzbek OG_A031ESS1_0_cc3f5460:

    # voice "audio/log/A031ESS1_YUK011.ogg"
    # yuk "「抽象的に考えるから余計迷走することもあるわ。そういうときには、具体的なところからアプローチしてみるのも手よ」"
    voice "audio/log/A031ESS1_YUK011.ogg"
    yuk "「Mavhum o‘ylash yanada chalg‘itishi mumkin. Bunday paytda aniq misollar orqali yondashish kerak.」"

# game/scripts/game/OG_A031ESS1.rpy:376
translate uzbek OG_A031ESS1_0_e0d57ce6:

    # voice "audio/log/A031ESS1_KOM018.ogg"
    # kom "「具体的？」"
    voice "audio/log/A031ESS1_KOM018.ogg"
    kom "「Aniq misollar?」"

# game/scripts/game/OG_A031ESS1.rpy:386
translate uzbek OG_A031ESS1_0_acb02821:

    # voice "audio/log/A031ESS1_YUK012.ogg"
    # yuk "「この場合、どういう行動をすれば友達と呼べるのか、ということかしらね」"
    voice "audio/log/A031ESS1_YUK012.ogg"
    yuk "「Bu holatda, qanday harakat qilsa do‘st deb atash mumkinligi haqida gap ketyapti.」"

# game/scripts/game/OG_A031ESS1.rpy:390
translate uzbek OG_A031ESS1_0_1ea0166c:

    # voice "audio/log/A031ESS1_HCM015.ogg"
    # hcm "「なんか因果関係逆転してねぇか、それ」"
    voice "audio/log/A031ESS1_HCM015.ogg"
    hcm "「Bunda sabab-oqibat teskari bo‘lib qolmayaptimi?」"

# game/scripts/game/OG_A031ESS1.rpy:400
translate uzbek OG_A031ESS1_0_fd9a3cbd:

    # voice "audio/log/A031ESS1_YUK013.ogg"
    # yuk "「技術の習得はまず模倣から入るものよ。武道だって型を学ぶじゃない」"
    voice "audio/log/A031ESS1_YUK013.ogg"
    yuk "「Hunar o‘rganish taqliddan boshlanadi. Jang san’atida ham avval harakatlarni o‘rganishadi-ku.」"

# game/scripts/game/OG_A031ESS1.rpy:413
translate uzbek OG_A031ESS1_0_cc627403:

    # voice "audio/log/A031ESS1_KOM019.ogg"
    # kom "「なるほど！　どうなのお兄ちゃん？　友達ってどういうことすれば友達だと思う？」"
    voice "audio/log/A031ESS1_KOM019.ogg"
    kom "「Tushunarli! Xo‘sh, akajon? Nima qilsa do‘st bo‘ladi deb o‘ylaysiz?」"

# game/scripts/game/OG_A031ESS1.rpy:417
translate uzbek OG_A031ESS1_0_bee1b761:

