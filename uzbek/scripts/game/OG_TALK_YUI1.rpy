# game/scripts/game/OG_TALK_YUI1.rpy:170
translate uzbek OG_TALK_YUI1_6_de2d52e6:

    # hcm "「あの三浦と正面切って対立するとは……さすがに雪ノ下だな」"
    hcm "「Miura bilan ochiqchasiga tortishish... Yukinoshitaga tan berdim.」"

# game/scripts/game/OG_TALK_YUI1.rpy:172
translate uzbek OG_TALK_YUI1_6_0a50c35f:

    # voice "audio/log/YUI1B1_YUI000.ogg"
    # yui "「うん……でも、ゆきのんの言ってることって正しいと思うんだよね。優美子もそれはわかってると思う。けど、素直に受け入れられないっていうか……」"
    voice "audio/log/YUI1B1_YUI000.ogg"
    yui "「Ha... lekin Yukinonning gapi to‘g‘ri deb o‘ylayman. Yumiko ham buni biladi. Faqat tan olgisi kelmayapti-da...」"

# game/scripts/game/OG_TALK_YUI1.rpy:176
translate uzbek OG_TALK_YUI1_6_d3c08642:

    # hcm "「まぁな。人は図星を指されると、つい反論したくなるもんだ」"
    hcm "「Shunday. Odamning aybini yuziga solsang, o‘zini oqlashga harakat qiladi-da.」"

# game/scripts/game/OG_TALK_YUI1.rpy:178
translate uzbek OG_TALK_YUI1_6_fa3d908a:

    # voice "audio/log/YUI1B1_YUI001.ogg"
    # yui "「ゆきのんを見てるとハラハラすることもあるけど、すごいなぁっていつも思うんだ。他人に自分の考えを左右されたりしないし」"
    voice "audio/log/YUI1B1_YUI001.ogg"
    yui "「Yukinonni ko‘rib xavotir olsam ham, unga qoyil qolaman. Boshqalarning gapiga qarab fikrini o‘zgartirmaydi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:182
translate uzbek OG_TALK_YUI1_6_4c1e09ad:

    # voice "audio/log/YUI1B1_YUI002.ogg"
    # yui "「そういう強さは、あたしにはないから」"
    voice "audio/log/YUI1B1_YUI002.ogg"
    yui "「Menda bunaqa kuch yo‘q.」"

# game/scripts/game/OG_TALK_YUI1.rpy:186
translate uzbek OG_TALK_YUI1_6_19ae9e9f:

    # hcm "「……俺からしたら、お前も充分すごいけどな」"
    hcm "「...Mening nazarimda, sen ham ancha kuchlisan.」"

# game/scripts/game/OG_TALK_YUI1.rpy:188
translate uzbek OG_TALK_YUI1_6_b8e07a75:

    # voice "audio/log/YUI1B1_YUI003.ogg"
    # yui mca07 "「へっ？」"
    voice "audio/log/YUI1B1_YUI003.ogg"
    yui "「E?」"

# game/scripts/game/OG_TALK_YUI1.rpy:192
translate uzbek OG_TALK_YUI1_6_c875fd73:

    # hcm "「雪ノ下の辛辣な意見も、ぬるっと素直に受け入れたり、するっとかわしたりしてるだろ。それって割とレアなスキルなんじゃねぇの」"
    hcm "「Yukinoshitaning achchiq gaplarini ham to‘g‘ri qabul qilding, ba’zilarini esa e’tiborsiz qoldirding. Bu ham kamdan-kam uchraydigan qobiliyat.」"

# game/scripts/game/OG_TALK_YUI1.rpy:194
translate uzbek OG_TALK_YUI1_6_598c5bb5:

    # voice "audio/log/YUI1B1_YUI004.ogg"
    # yui "「もしかして……ほめてる？」"
    voice "audio/log/YUI1B1_YUI004.ogg"
    yui "「Meni maqtadingmi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:198
translate uzbek OG_TALK_YUI1_6_19d36c75:

    # hcm "「別に。事実を言ったまでだ」"
    hcm "「Yo‘q. Shunchaki haqiqatni aytdim.」"

# game/scripts/game/OG_TALK_YUI1.rpy:200
translate uzbek OG_TALK_YUI1_6_38d08e66:

    # voice "audio/log/YUI1B1_YUI005.ogg"
    # yui mca00 "「そっか……へへっ、でもいいや。なんかうれしい」"
    voice "audio/log/YUI1B1_YUI005.ogg"
    yui "「Shunaqami... xexe, mayli. Baribir xursandman.」"

# game/scripts/game/OG_TALK_YUI1.rpy:216
translate uzbek OG_TALK_YUI1_7_330d57d8:

    # hcm "「星、キレイだな……見てると吸い込まれそうだ」"
    hcm "「Yulduzlar chiroyli-a... Qarasang, o‘ziga tortib ketadigandek.」"

# game/scripts/game/OG_TALK_YUI1.rpy:218
translate uzbek OG_TALK_YUI1_7_0705a251:

    # voice "audio/log/YUI1B2_YUI000.ogg"
    # yui mca08 "「え？……うんっ！？」"
    voice "audio/log/YUI1B2_YUI000.ogg"
    yui "「E? ...Ha!?」"

# game/scripts/game/OG_TALK_YUI1.rpy:222
translate uzbek OG_TALK_YUI1_7_0781e362:

    # hcm "「え、何その驚愕のリアクション」"
    hcm "「E, buncha hayron qolmasang.」"

# game/scripts/game/OG_TALK_YUI1.rpy:224
translate uzbek OG_TALK_YUI1_7_844fdc73:

    # voice "audio/log/YUI1B2_YUI001.ogg"
    # yui mca04 "「あっ、ご、ごめん……ヒッキーが超似合わないこと言ったからビックリして……」"
    voice "audio/log/YUI1B2_YUI001.ogg"
    yui "「A, u-uzr... Hikki o‘ziga yarashmaydigan gap gapirgani uchun hayron qoldim...」"

# game/scripts/game/OG_TALK_YUI1.rpy:228
translate uzbek OG_TALK_YUI1_7_3e5f303c:

    # hcm "「失礼な。俺は文学少年なんだよ。星空の下独りでいると言葉が内側に治まってくもんでな。だからまぁ、ぼっちってのは結構ロマンチストなんだよ」"
    hcm "「Odobsizlik qilma. Men adabiyotni sevaman. Yulduzli osmon ostida yolg‘iz qolganda so‘zlar ich-ichimdan otilib chiqadi. Shuning uchun yolg‘iz odamlar romantik bo‘lishadi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:230
translate uzbek OG_TALK_YUI1_7_3822028d:

    # voice "audio/log/YUI1B2_YUI002.ogg"
    # yui "「えー？　絶対違うよー。ヒッキーはアゴイスト？　とかいうのでしょ？」"
    voice "audio/log/YUI1B2_YUI002.ogg"
    yui "「E-e? Unday emas-ku. Hikki “agoist” (jag‘dor)ku?」"

# game/scripts/game/OG_TALK_YUI1.rpy:234
translate uzbek OG_TALK_YUI1_7_6cdd82a8:

    # hcm "「おい、別に俺シャクれてねぇだろ。エゴイストって言いたいの、それ？」"
    hcm "「Hoy, mening jag‘im chiqib ketmagan. Egoist demoqchimisan?」"

# game/scripts/game/OG_TALK_YUI1.rpy:236
translate uzbek OG_TALK_YUI1_7_0b003686:

    # voice "audio/log/YUI1B2_YUI003.ogg"
    # yui mca00 "「そ、それだ！」"
    voice "audio/log/YUI1B2_YUI003.ogg"
    yui "「Ha, o‘sha!」"

# game/scripts/game/OG_TALK_YUI1.rpy:240
translate uzbek OG_TALK_YUI1_7_a7c4369f:

    # hcm "「……」"
    hcm "「...」"

# game/scripts/game/OG_TALK_YUI1.rpy:242
translate uzbek OG_TALK_YUI1_7_201230fe:

    # voice "audio/log/YUI1B2_YUI004.ogg"
    # yui mca04 "「うっ、何その反応。で、でも、ヒッキーだってさっき間違えてたし！」"
    voice "audio/log/YUI1B2_YUI004.ogg"
    yui "「U, nega unday qarayapsan. Le-lekin, Hikki ham boya adashdi-ku!」"

# game/scripts/game/OG_TALK_YUI1.rpy:246
translate uzbek OG_TALK_YUI1_7_77927220:

    # hcm "「はぁ、何を？」"
    hcm "「A, nimada?」"

# game/scripts/game/OG_TALK_YUI1.rpy:248
translate uzbek OG_TALK_YUI1_7_b35a4ff6:

    # voice "audio/log/YUI1B2_YUI005.ogg"
    # yui mca00 "「……今は独りじゃなくて、二人きりだよ」"
    voice "audio/log/YUI1B2_YUI005.ogg"
    yui "「...Hozir yolg‘iz emassan, ikkimiz birgamiz.」"

# game/scripts/game/OG_TALK_YUI1.rpy:252
translate uzbek OG_TALK_YUI1_7_4063e526:

    # hcm "「……まぁ、これで３人いたら怖いよな」"
    hcm "「...To‘g‘ri, uch kishi bo‘lganda qo‘rqinchli bo‘lardi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:254
translate uzbek OG_TALK_YUI1_7_726c8648:

    # voice "audio/log/YUI1B2_YUI006.ogg"
    # yui mca08 "「な、なんでそういうこと言うの！？　怖くなってくるじゃん！」"
    voice "audio/log/YUI1B2_YUI006.ogg"
    yui "「Nega unday deyapsan!? Qo‘rqib ketyapman-ku!」"

# game/scripts/game/OG_TALK_YUI1.rpy:270
translate uzbek OG_TALK_YUI1_8_8b75f012:

    # hcm "「三浦の方は、もう落ち着いてるんだよな？」"
    hcm "「Miura o‘ziga keldimi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:272
translate uzbek OG_TALK_YUI1_8_b3827c48:

    # voice "audio/log/YUI1B3_YUI000.ogg"
    # yui "「うん。姫菜も側についてるし、大丈夫だと思う」"
    voice "audio/log/YUI1B3_YUI000.ogg"
    yui "「Ha. Hina ham yonida, xavotir olmasa ham bo‘ladi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:276
translate uzbek OG_TALK_YUI1_8_868479eb:

    # hcm "「三浦の泣き顔か……想像つかねぇんだけど」"
    hcm "「Miuraning yig‘laganini... tasavvur qilolmayman.」"

# game/scripts/game/OG_TALK_YUI1.rpy:278
translate uzbek OG_TALK_YUI1_8_4b1a2dbf:

    # voice "audio/log/YUI1B3_YUI001.ogg"
    # yui mca05 "「そう、かな？　優美子は確かに強引だし強気だけど……女の子だし、泣くこともあるよ」"
    voice "audio/log/YUI1B3_YUI001.ogg"
    yui "「Shunaqamikin? Yumiko qaysar va o‘ziga ishongan qiz, lekin... u ham qiz bola-ku, yig‘lashi mumkin.」"

# game/scripts/game/OG_TALK_YUI1.rpy:282
translate uzbek OG_TALK_YUI1_8_e22aaa7b:

    # hcm "「鬼の目にも涙、か」"
    hcm "「Iblisning ko‘ziga yosh kelibdi-da.」"

# game/scripts/game/OG_TALK_YUI1.rpy:284
translate uzbek OG_TALK_YUI1_8_7fab3d54:

    # voice "audio/log/YUI1B3_YUI002.ogg"
    # yui "「そーいうこと言わないのっ！　でも……ごめんね、ヒッキーにグチっぽく話しちゃって」"
    voice "audio/log/YUI1B3_YUI002.ogg"
    yui "「Unday demagin! Lekin... uzr, Hikki, senga nolib qo‘ydim.」"

# game/scripts/game/OG_TALK_YUI1.rpy:288
translate uzbek OG_TALK_YUI1_8_d6c2119b:

    # hcm "「いや、それは別にいいんだけどな」"
    hcm "「Yo‘q, hechqisi yo‘q.」"

# game/scripts/game/OG_TALK_YUI1.rpy:296
translate uzbek OG_TALK_YUI1_9_e12c0ced:

    # hcm "「三浦をなぐさめるなら、葉山でも連れてくるのがてっとり早いんじゃねぇの？」"
    hcm "「Miurani yupatish uchun Hayamani olib kelish kerak emasmi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:298
translate uzbek OG_TALK_YUI1_9_ef290e9e:

    # voice "audio/log/YUI1B4_YUI000.ogg"
    # yui "「それもちょっと考えたんだけどね……優美子、プライド高いから、隼人くんに素直に泣きつくのは難しいかも」"
    voice "audio/log/YUI1B4_YUI000.ogg"
    yui "「Buni ham o‘ylab ko‘rgandim... lekin Yumiko juda mag‘rur, Hayato-kunning oldida yig‘lay olmasa kerak.」"

# game/scripts/game/OG_TALK_YUI1.rpy:302
translate uzbek OG_TALK_YUI1_9_6f12c515:

    # hcm "「そういうもんか」"
    hcm "「Shunaqami.」"

# game/scripts/game/OG_TALK_YUI1.rpy:304
translate uzbek OG_TALK_YUI1_9_ecde019e:

    # voice "audio/log/YUI1B4_YUI001.ogg"
    # yui mca07 "「そういうもんだよ……あ、そうだ！　ヒッキー、部屋に帰ったらさりげなく隼人くんにこのこと話してもらえないかな？」"
    voice "audio/log/YUI1B4_YUI001.ogg"
    yui "「Shunaqa... A, aytgancha! Hikki, xonaga qaytganingda Hayato-kunga sekin aytib qo‘ya olasanmi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:308
translate uzbek OG_TALK_YUI1_9_72ed1e8b:

    # hcm "「なんで俺が。それは無理だ」"
    hcm "「Nega endi. Iloji yo‘q.」"

# game/scripts/game/OG_TALK_YUI1.rpy:310
translate uzbek OG_TALK_YUI1_9_7511d1db:

    # voice "audio/log/YUI1B4_YUI002.ogg"
    # yui "「どうして」"
    voice "audio/log/YUI1B4_YUI002.ogg"
    yui "「Nega?」"

# game/scripts/game/OG_TALK_YUI1.rpy:314
translate uzbek OG_TALK_YUI1_9_8ae18ff9:

    # hcm "「俺が葉山と会話するわけないだろ」"
    hcm "「Men Hayama bilan gaplashmayman-ku.」"

# game/scripts/game/OG_TALK_YUI1.rpy:316
translate uzbek OG_TALK_YUI1_9_cebd6044:

    # voice "audio/log/YUI1B4_YUI003.ogg"
    # yui "「そ、そうだった……」"
    voice "audio/log/YUI1B4_YUI003.ogg"
    yui "「Ha, to‘g‘ri...」"

# game/scripts/game/OG_TALK_YUI1.rpy:320
translate uzbek OG_TALK_YUI1_9_3fae9ee0:

    # hcm "「そうでなくても、俺に伝言ゲームさせると、ひどいことになるぞ。たぶん、雪ノ下を悪く言うことでしか三浦をかばえないからな、俺は」"
    hcm "「Unday bo‘lmasa ham, men gap tashisam yomon bo‘ladi. Miurani himoya qilish uchun Yukinoshitani yomonlashimga to‘g‘ri keladi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:322
translate uzbek OG_TALK_YUI1_9_4bd34a9c:

    # hcm "「俺の性格の悪さを甘く見るな」"
    hcm "「Mening yomon xarakterimni unutma.」"

# game/scripts/game/OG_TALK_YUI1.rpy:324
translate uzbek OG_TALK_YUI1_9_10758f77:

    # voice "audio/log/YUI1B4_YUI004.ogg"
    # yui mca04 "「ヒッキーはそんなことしな……いや、するかも。それも結構楽しそうに……」"
    voice "audio/log/YUI1B4_YUI004.ogg"
    yui mca04 "「Hikki unday qilmaydi... yo‘q, qilishi mumkin. O‘zi ham mazza qilib...」"

# game/scripts/game/OG_TALK_YUI1.rpy:328
translate uzbek OG_TALK_YUI1_9_24aa7279:

    # hcm "「だろ？」"
    hcm "「To‘g‘rimi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:330
translate uzbek OG_TALK_YUI1_9_bf12ec26:

    # voice "audio/log/YUI1B4_YUI005.ogg"
    # yui mca00 "「……本当に性格悪いなぁ、もう。やっぱ、さっきのお願いナシ！　忘れて？」"
    voice "audio/log/YUI1B4_YUI005.ogg"
    yui mca00 "「...Rostdan ham yomonsan. Bo‘ldi, boyagi iltimosim bekor! Unutib yubor.」"

# game/scripts/game/OG_TALK_YUI1.rpy:340
translate uzbek OG_TALK_YUI1_10_b1b749f4:

    # hcm "「しかし、お前も大変そうだな」"
    hcm "「Lekin senga ham qiyin ekan.」"

# game/scripts/game/OG_TALK_YUI1.rpy:342
translate uzbek OG_TALK_YUI1_10_e383bf24:

    # voice "audio/log/YUI1B5_YUI000.ogg"
    # yui mca07 "「うん……でも、あたし、小さい頃からこういうポジション多かったから、女の子同士ってこんな感じだなって思ってるし」"
    voice "audio/log/YUI1B5_YUI000.ogg"
    yui "「Ha... lekin yoshligimdan shunday edim, qizlar o‘rtasida shunaqa bo‘lishi odatiy hol deb o‘ylayman.」"

# game/scripts/game/OG_TALK_YUI1.rpy:346
translate uzbek OG_TALK_YUI1_10_11c52305:

    # hcm "「ふーん、そういうもんかねぇ」"
    hcm "「Xmm, shunaqamikin.」"

# game/scripts/game/OG_TALK_YUI1.rpy:348
translate uzbek OG_TALK_YUI1_10_c743a085:

    # hcm "「誰が泣いただ泣かされただ、誰とグループになるだのなんだの……つくづく女に生まれなくて良かったぜ」"
    hcm "「Kimdir yig‘ladi, kimdir yig‘latdi, kim kim bilan guruh bo‘ladi... qiz bola bo‘lib tug‘ilmaganimga shukur.」"

# game/scripts/game/OG_TALK_YUI1.rpy:350
translate uzbek OG_TALK_YUI1_10_1268e228:

    # voice "audio/log/YUI1B5_YUI001.ogg"
    # yui "「男子にはわかんないかもね。男同士ってもっとカラっとしてるっていうか」"
    voice "audio/log/YUI1B5_YUI001.ogg"
    yui "「O‘g‘il bolalar buni tushunmasa kerak. O‘g‘il bolalar o‘rtasida munosabatlar ochiqroq bo‘ladi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:354
translate uzbek OG_TALK_YUI1_10_93ee6e24:

    # hcm "「いや、んなこたぁねぇよ。男子だって仲が良いとか悪いとか、グループだ派閥だってあるぜ。陰口やいじめもある。」"
    hcm "「Yo‘q, unday emas. O‘g‘il bolalar orasida ham yaxshi va yomon munosabatlar, guruhlar, g‘iybat va bezorilik bor.」"

# game/scripts/game/OG_TALK_YUI1.rpy:356
translate uzbek OG_TALK_YUI1_10_4e5ad865:

    # voice "audio/log/YUI1B5_YUI002.ogg"
    # yui "「そ、そうなの？」"
    voice "audio/log/YUI1B5_YUI002.ogg"
    yui "「Shu-shunaqami?」"

# game/scripts/game/OG_TALK_YUI1.rpy:360
translate uzbek OG_TALK_YUI1_10_add62bf2:

    # hcm "「ああ。変わんねぇよ、男子も女子も。群れてパワーゲームすんのが人間の習性なんだろうな、アホくさ」"
    hcm "「Ha. O‘g‘il bolalar ham, qizlar ham bir xil. Odamlar guruh bo‘lib kuch sinashishni yaxshi ko‘rishadi, bema’nilik.」"

# game/scripts/game/OG_TALK_YUI1.rpy:362
translate uzbek OG_TALK_YUI1_10_20a311b2:

    # voice "audio/log/YUI1B5_YUI003.ogg"
    # yui mca06 "「あ、あはは……アホくさい、かぁ。結構頑張ってたりするんだけどなぁ……」"
    voice "audio/log/YUI1B5_YUI003.ogg"
    yui "「A, axaxa... Bema’nilik de. Men ancha harakat qilaman-u, lekin...」"

# game/scripts/game/OG_TALK_YUI1.rpy:366
translate uzbek OG_TALK_YUI1_10_2d0bc3e7:

    # hcm "「……悪い。俺はそういうの頑張れないから、それはわからん」"
    hcm "「...Uzr. Men bunday narsalarga harakat qilolmayman, shuning uchun tushunmayman.」"

# game/scripts/game/OG_TALK_YUI1.rpy:368
translate uzbek OG_TALK_YUI1_10_a786adfd:

    # voice "audio/log/YUI1B5_YUI004.ogg"
    # yui "「……そっか。あたしも、わかんなくなってきちゃうな……」"
    voice "audio/log/YUI1B5_YUI004.ogg"
    yui "「...Shunaqami. Men ham tushunmay qolyapman...」"

# game/scripts/game/OG_TALK_YUI1.rpy:384
translate uzbek OG_TALK_YUI1_11_b58c7c15:

    # hcm "「えー……以上を持ちまして、本日の営業を終了させていただきます」"
    hcm "「E... Shu bilan bugungi ish kunimiz o‘z nihoyasiga yetdi.」"

# game/scripts/game/OG_TALK_YUI1.rpy:386
translate uzbek OG_TALK_YUI1_11_211ae28b:

    # voice "audio/log/YUI1B6_YUI000.ogg"
    # yui mca08 "「営業！？　営業だったんだ！？」"
    voice "audio/log/YUI1B6_YUI000.ogg"
    yui mca08 "「Ish!? Bu ishmidi!?」"

# game/scripts/game/OG_TALK_YUI1.rpy:390
translate uzbek OG_TALK_YUI1_11_54af2cbd:

    # hcm "「……とりあえず、言いたいことは察してくれ」"
    hcm "「...Xullas, nima demoqchi ekanimni tushunib ol.」"

# game/scripts/game/OG_TALK_YUI1.rpy:392
translate uzbek OG_TALK_YUI1_11_eff6d4d2:

    # voice "audio/log/YUI1B6_YUI001.ogg"
    # yui mca06 "「はぁ……あたしとこれ以上話す気はない、っていうことだよね？」"
    voice "audio/log/YUI1B6_YUI001.ogg"
    yui mca06 "「Uf... Men bilan boshqa gaplashishni xohlamaysan, shundaymi?」"

# game/scripts/game/OG_TALK_YUI1.rpy:396
translate uzbek OG_TALK_YUI1_11_657d3853:

    # hcm "「要約するとそうなるな。珍しく正解だ。やるじゃん」"
    hcm "「Qisqasi shunday. To‘g‘ri topding. Qoyil.」"

# game/scripts/game/OG_TALK_YUI1.rpy:398
translate uzbek OG_TALK_YUI1_11_96886fd4:

    # voice "audio/log/YUI1B6_YUI002.ogg"
    # yui "「全っ然うれしくないんだけど……」"
    voice "audio/log/YUI1B6_YUI002.ogg"
    yui "「Umuman xursand bo‘lmadim...」"

