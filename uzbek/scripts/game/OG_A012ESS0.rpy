# game/scripts/game/OG_A012ESS0.rpy:26
translate uzbek OG_A012ESS0_0_fd11c044:

    # voice "audio/log/A012ESS0_HCM001.ogg"
    # hcm "「そりゃそうか、俺んちじゃロクに食ってなかったもんな。よかったなーサブレ、のびのびできて」"
    voice "audio/log/A012ESS0_HCM001.ogg"
    hcm "“Shunaqa-da, biznikida tuzukroq yemagandi ham. Yaxshi bo‘ldi, Sable, mazza qilyapsan.”"

# game/scripts/game/OG_A012ESS0.rpy:30
translate uzbek OG_A012ESS0_0_d5fbd536:

    # voice "audio/log/A012ESS0_YUK001.ogg"
    # yuk "「……だと、いいのだけれど」"
    voice "audio/log/A012ESS0_YUK001.ogg"
    yuk "“...Shunday bo‘lsa yaxshi edi.”"

# game/scripts/game/OG_A012ESS0.rpy:40
translate uzbek OG_A012ESS0_0_44b6427d:

    # voice "audio/log/A012ESS0_HCM002.ogg"
    # hcm "「ん？」"
    voice "audio/log/A012ESS0_HCM002.ogg"
    hcm "“Hm?”"

# game/scripts/game/OG_A012ESS0.rpy:52
translate uzbek OG_A012ESS0_0_6c4e562d:

    # voice "audio/log/A012ESS0_YUK002.ogg"
    # yuk "「今はまだ、あなたがいるからサブレも安心なのでしょうけど……帰ったあとは、どうなることか」"
    voice "audio/log/A012ESS0_YUK002.ogg"
    yuk "“Hozircha sen borliging uchun Sable xotirjamdir, lekin... ketganingdan keyin nima bo‘larkin.”"

# game/scripts/game/OG_A012ESS0.rpy:56
translate uzbek OG_A012ESS0_0_cc580d90:

    # voice "audio/log/A012ESS0_HCM003.ogg"
    # hcm "「そう言われてもなぁ。小町でも泊まらせるか？」"
    voice "audio/log/A012ESS0_HCM003.ogg"
    hcm "“Shunaqa desang ham... Komachini olib qolasanmi unda?”"

# game/scripts/game/OG_A012ESS0.rpy:60
translate uzbek OG_A012ESS0_0_133a72c4:

    # voice "audio/log/A012ESS0_YUK003.ogg"
    # yuk "「受験生なのでしょう、そんなことさせられないわ」"
    voice "audio/log/A012ESS0_YUK003.ogg"
    yuk "“U imtihonga tayyorlanyapti-ku, uni ovora qilolmayman.”"

# game/scripts/game/OG_A012ESS0.rpy:64
translate uzbek OG_A012ESS0_0_f0eb7763:

    # voice "audio/log/A012ESS0_HCM004.ogg"
    # hcm "「お気遣いどうも。まぁ、案外喜んで来そうな気もするけど」"
    voice "audio/log/A012ESS0_HCM004.ogg"
    hcm "“G‘amxo‘rliging uchun rahmat. Lekin, menimcha, jon deb kelgan bo‘lardi.”"

# game/scripts/game/OG_A012ESS0.rpy:68
translate uzbek OG_A012ESS0_0_5b763880:

    # voice "audio/log/A012ESS0_YUK004.ogg"
    # yuk "「そうね。もしかしたら、小町さんも先住動物に気を遣っているかもしれないし。たまには解放してあげたほうがいいかもしれないわ」"
    voice "audio/log/A012ESS0_YUK004.ogg"
    yuk "“To‘g‘ri. Balki Komachi-san ham uyidagi jonivorga rioya qilayotgandir. Ba’zan uni ham ozod qilib turish kerakdir.”"

# game/scripts/game/OG_A012ESS0.rpy:72
translate uzbek OG_A012ESS0_0_a8b00def:

    # voice "audio/log/A012ESS0_HCM005.ogg"
    # hcm "「いや動物て。俺の人権どこ行ったんだよ。まぁ、餌も食べてるし、心配することはないと思うけどまだ不安か？」"
    voice "audio/log/A012ESS0_HCM005.ogg"
    hcm "“Iye, jonivor emish. Mening insonlik huquqim qayerda qoldi? Mayli, ovqatini yeyapti-ku, xavotir oladigan joyi yo‘q, lekin hali ham bezovtamisan?”"

# game/scripts/game/OG_A012ESS0.rpy:82
translate uzbek OG_A012ESS0_0_8bf0074d:

    # voice "audio/log/A012ESS0_YUK005.ogg"
    # yuk "「散歩中もそうだったけれど、この家に戻ってからも、やはり私を敬遠しているような気がするもの」"
    voice "audio/log/A012ESS0_YUK005.ogg"
    yuk "“Sayr paytida ham shunday edi, uyga qaytganda ham baribir mendan o‘zini olib qochayotgandek tuyulyapti.”"

# game/scripts/game/OG_A012ESS0.rpy:86
translate uzbek OG_A012ESS0_0_f7c93e09:

    # voice "audio/log/A012ESS0_HCM006.ogg"
    # hcm "「そりゃーあれだ、お前がサブレを敬遠してっからだよ。さっきも言ったろ？　そういうの結構伝わるもんだって」"
    voice "audio/log/A012ESS0_HCM006.ogg"
    hcm "“Bu aniq, o‘zing Sabledan qochayotganing uchun-da. Boya aytdim-ku? Bunaqa narsalar tez seziladi deb.”"

# game/scripts/game/OG_A012ESS0.rpy:90
translate uzbek OG_A012ESS0_0_92c59488:

    # voice "audio/log/A012ESS0_YUK006.ogg"
    # yuk "「……」"
    voice "audio/log/A012ESS0_YUK006.ogg"
    yuk "“......”"

# game/scripts/game/OG_A012ESS0.rpy:94
translate uzbek OG_A012ESS0_0_beea04f6:

    # voice "audio/log/A012ESS0_HCM007.ogg"
    # hcm "「まぁでも、見ろよ。喜んでエサ食ってるじゃねーか。あいつだって本気で嫌なら、この家に入ることすら嫌がってるって」"
    voice "audio/log/A012ESS0_HCM007.ogg"
    hcm "“Lekin qara. Mazza qilib ovqat yeyapti-ku. Agar rostan yomon ko‘rganda, bu uyga kirishni ham xohlamasdi.”"

# game/scripts/game/OG_A012ESS0.rpy:98
translate uzbek OG_A012ESS0_0_a73c7778:

    # voice "audio/log/A012ESS0_YUK007.ogg"
    # yuk "「けれど、私が不慣れなせいで、サブレに余計なプレッシャーを与えないかしら？」"
    voice "audio/log/A012ESS0_YUK007.ogg"
    yuk "“Lekin, tajribasizligim sababli Sablega ortiqcha bosim o‘tkazib qo‘ymasmikinman?”"

# game/scripts/game/OG_A012ESS0.rpy:102
translate uzbek OG_A012ESS0_0_e6d00061:

    # voice "audio/log/A012ESS0_HCM008.ogg"
    # hcm "「エサと水とトイレの世話と夕方の散歩だけしときゃ、あとは構ってやらんでも大丈夫だろ。それが延々と続くってんなら問題だけど、今回はそうじゃねーし」"
    voice "audio/log/A012ESS0_HCM008.ogg"
    hcm "“Ovqati, suvi, hojatxonasi va kechki sayridan xabar olib tursang bo‘ldi, ortiqcha e’tibor bermasang ham bo‘laveradi. Agar bu uzoq davom etganda muammo bo‘lardi, lekin hozir unaqa emas-ku.”"

# game/scripts/game/OG_A012ESS0.rpy:112
translate uzbek OG_A012ESS0_0_08a4d71d:

    # voice "audio/log/A012ESS0_YUK008.ogg"
    # yuk "「そんな雑な扱いでいいのかしら……」"
    voice "audio/log/A012ESS0_YUK008.ogg"
    yuk "“Shunchalik e’tiborsiz qoldirsam bo‘laveradimi...”"

# game/scripts/game/OG_A012ESS0.rpy:116
translate uzbek OG_A012ESS0_0_d3156d91:

    # voice "audio/log/A012ESS0_HCM009.ogg"
    # hcm "「いいんじゃねぇの。それくらいのほうがお互い気楽なことだってある。無理して築く関係なんてすぐに破綻するし、破綻する関係ならその程度のもんだろ」"
    voice "audio/log/A012ESS0_HCM009.ogg"
    hcm "“Bo‘laveradi. Shunchaligi ikkala tomon uchun ham qulayroq bo‘lishi mumkin. Zo‘rma-zo‘raki qurilgan munosabat tez buziladi, agar buzilsa, demak shunga arziydigan narsa ekan.”"

# game/scripts/game/OG_A012ESS0.rpy:120
translate uzbek OG_A012ESS0_0_3d98f091:

    # voice "audio/log/A012ESS0_YUK009.ogg"
    # yuk "「……そう、なのかしら」"
    voice "audio/log/A012ESS0_YUK009.ogg"
    yuk "“...Shunaqamikin.”"

# game/scripts/game/OG_A012ESS0.rpy:124
translate uzbek OG_A012ESS0_0_85d919b3:

    # voice "audio/log/A012ESS0_HCM010.ogg"
    # hcm "「そうだろ。あんまりベタベタされても逆にストレス溜まるだろうし」"
    voice "audio/log/A012ESS0_HCM010.ogg"
    hcm "“Shunaqa. Haddan tashqari erkalasa ham, aksincha stress bo‘ladi.”"

# game/scripts/game/OG_A012ESS0.rpy:128
translate uzbek OG_A012ESS0_0_31d7a0c7:

    # voice "audio/log/A012ESS0_HCM011.ogg"
    # hcm "「仲良くしようって気合い入れて構えられると、そのやる気がキモいとか怖いとかいろいろ言われるんだよ、陰で。ソースは俺」"
    voice "audio/log/A012ESS0_HCM011.ogg"
    hcm "“Chiqishib ketishga harakat qilib, jon-jahdi bilan yopishsa, bu g‘ayrati g‘alati yoki qo‘rqinchli deb har xil gaplar aytishadi ortidan. Manba — men o‘zim.”"

# game/scripts/game/OG_A012ESS0.rpy:138
translate uzbek OG_A012ESS0_0_6a4d1f13:

    # voice "audio/log/A012ESS0_YUK010.ogg"
    # yuk "「……まったく参考にならない貴重な体験談をありがとう。すこしはまともなことを言うのかと真面目に聞いて損したわ」"
    voice "audio/log/A012ESS0_YUK010.ogg"
    yuk "“...Umuman foydasi tegmaydigan qimmatli tajribang uchun rahmat. Biror tuzukroq gap aytarmikan deb jiddiy eshitib, vaqtimni ketkazibman.”"

# game/scripts/game/OG_A012ESS0.rpy:142
translate uzbek OG_A012ESS0_0_cd7e90e8:

    # voice "audio/log/A012ESS0_HCM012.ogg"
    # hcm "「ま、仲良くなれないにしてもうまくやれよ」"
    voice "audio/log/A012ESS0_HCM012.ogg"
    hcm "“Mayli, do‘stlasholmasang ham, til topishib tur.”"

# game/scripts/game/OG_A012ESS0.rpy:152
translate uzbek OG_A012ESS0_0_27232500:

    # voice "audio/log/A012ESS0_YUK011.ogg"
    # yuk "「それは、平塚先生の受け売り？」"
    voice "audio/log/A012ESS0_YUK011.ogg"
    yuk "“Bu Hiratsuka-senseyning gapi-ku?”"

# game/scripts/game/OG_A012ESS0.rpy:156
translate uzbek OG_A012ESS0_0_9a4f96c2:

    # voice "audio/log/A012ESS0_HCM013.ogg"
    # hcm "「まぁな」"
    voice "audio/log/A012ESS0_HCM013.ogg"
    hcm "“Shunaqa desa ham bo‘ladi.”"

# game/scripts/game/OG_A012ESS0.rpy:160
translate uzbek OG_A012ESS0_0_e2aadf4e:

    # voice "audio/log/A012ESS0_YUK012.ogg"
    # yuk "「……動物相手ならうまくやれるあなたが、人間相手にうまくやれない理由を説明できたなら、きっと私もサブレとうまくやれそうな気がするわ」"
    voice "audio/log/A012ESS0_YUK012.ogg"
    yuk "“...Hayvonlar bilan til topisha oladigan sen, odamlar bilan nega chiqisha olmasligingni tushuntirib bera olsang, men ham Sable bilan kelisha olaman deb o‘ylayman.”"

# game/scripts/game/OG_A012ESS0.rpy:164
translate uzbek OG_A012ESS0_0_e3be7ea7:

    # voice "audio/log/A012ESS0_HCM014.ogg"
    # hcm "「人間相手がヘタクソなのはお前だっていっしょだと思うぞ……」"
    voice "audio/log/A012ESS0_HCM014.ogg"
    hcm "“Odamlar bilan muomala qilishda sen ham mendan qolishmaysan-ov...”"

# game/scripts/game/OG_A012ESS0.rpy:168
translate uzbek OG_A012ESS0_0_e9edac39:

    # voice "audio/log/A012ESS0_YUK013.ogg"
    # yuk "「冗談よ」"
    voice "audio/log/A012ESS0_YUK013.ogg"
    yuk "“Hazillashdim.”"

# game/scripts/game/OG_A012ESS0.rpy:172
translate uzbek OG_A012ESS0_0_0693a5a8:

    # voice "audio/log/A012ESS0_HCM015.ogg"
    # hcm "「じゃあそろそろ帰るわ」"
    voice "audio/log/A012ESS0_HCM015.ogg"
    hcm "“Bo‘pti, men endi ketay.”"

# game/scripts/game/OG_A012ESS0.rpy:182
translate uzbek OG_A012ESS0_0_cb1d68cd:

    # voice "audio/log/A012ESS0_YUK014.ogg"
    # yuk "「……一応、お礼を言っておくわね」"
    voice "audio/log/A012ESS0_YUK014.ogg"
    yuk "“...Nima bo‘lganda ham, rahmat aytib qo‘yay.”"

# game/scripts/game/OG_A012ESS0.rpy:186
translate uzbek OG_A012ESS0_0_d2f5c3dc:

    # voice "audio/log/A012ESS0_HCM016.ogg"
    # hcm "「礼をいうのはこっちだよ。……じゃあな、サブレ」"
    voice "audio/log/A012ESS0_HCM016.ogg"
    hcm "“Aslida men rahmat aytishim kerak. ...Xayr, Sable.”"

# game/scripts/game/OG_A012ESS0.rpy:190
translate uzbek OG_A012ESS0_0_56076d06:

    # voice "audio/log/a012ess0_X01000.ogg"
    # X01 "「ひゃん！」"
    voice "audio/log/a012ess0_X01000.ogg"
    X01 "“Vov!”"

# game/scripts/game/OG_A012ESS0.rpy:202
translate uzbek OG_A012ESS0_0_be19e041:

    # hcm "（……ま、冗談飛ばせる程度には緊張もほぐれたみてーだな……あんま冗談になってなかったけどね！）"
    hcm "（...Mayli, hazillashadigan darajada o‘zini erkin tutyapti... garchi unchalik hazilga o‘xshamagan bo‘lsa ham!）"