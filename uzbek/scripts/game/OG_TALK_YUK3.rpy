# game/scripts/game/OG_TALK_YUK3.rpy:332
translate uzbek OG_TALK_YUK3_7_d2619d32:

    # hcm "「いやめっちゃあくびしてたし、今。まぁ、気持ちはわかる。身近に生き物いると気になって寝つけないときはあるもんな」"
    hcm "Hozirgina esnading-ku. Mayli, tushunaman. Yoningda jonivor bo'lsa, uxlash qiyin bo'ladi."

# game/scripts/game/OG_TALK_YUK3.rpy:334
translate uzbek OG_TALK_YUK3_7_921394da:

    # voice "audio/log/YUK3B2_YUK002.ogg"
    # yuk "「そういうわけでも……」"
    voice "audio/log/YUK3B2_YUK002.ogg"
    yuk "Unday emas..."

# game/scripts/game/OG_TALK_YUK3.rpy:338
translate uzbek OG_TALK_YUK3_7_583d68f7:

    # hcm "「そうかぁ？　小型犬なんかだと特にそういう傾向ありそうなもんだが」"
    hcm "Shunaqami? Kichkina kuchuklar odatda tinchimas bo'lishadi-ku."

# game/scripts/game/OG_TALK_YUK3.rpy:340
translate uzbek OG_TALK_YUK3_7_34cd01d0:

    # voice "audio/log/YUK3B2_YUK003.ogg"
    # yuk "「確かに、そういう点がないでもないけれど……」"
    voice "audio/log/YUK3B2_YUK003.ogg"
    yuk "To'g'ri, unchalik tinch emas, lekin..."

# game/scripts/game/OG_TALK_YUK3.rpy:344
translate uzbek OG_TALK_YUK3_7_d8067b9a:

    # hcm "「だろ？　サブレ、結構落ち着きないもんな」"
    hcm "Aytmadimmi? Sable ancha sho'x."

# game/scripts/game/OG_TALK_YUK3.rpy:346
translate uzbek OG_TALK_YUK3_7_82e09008:

    # voice "audio/log/YUK3B2_YUK004.ogg"
    # yuk "「それはそういうものだと理解しているからいいのだけれど」"
    voice "audio/log/YUK3B2_YUK004.ogg"
    yuk "Tabiati shundayligini tushunganim uchun bu muammo emas."

# game/scripts/game/OG_TALK_YUK3.rpy:350
translate uzbek OG_TALK_YUK3_7_94022a6e:

    # hcm "「けれど？」"
    hcm "Lekin?"

# game/scripts/game/OG_TALK_YUK3.rpy:352
translate uzbek OG_TALK_YUK3_7_9b9a6c63:

    # voice "audio/log/YUK3B2_YUK005.ogg"
    # yuk mra00 "「……あの、自分の尻尾をひたすら追い続けている姿が気になってしまって」"
    voice "audio/log/YUK3B2_YUK005.ogg"
    yuk mra00 "...Uning o'z dumini tinimsiz quvayotganini ko'rib g'alati bo'lib ketdim."

# game/scripts/game/OG_TALK_YUK3.rpy:356
translate uzbek OG_TALK_YUK3_7_bfacbfde:

    # hcm "「……あー、あの行動な」"
    hcm "...Ha, o'sha odatimi."

# game/scripts/game/OG_TALK_YUK3.rpy:358
translate uzbek OG_TALK_YUK3_7_3d0eb2b8:

    # voice "audio/log/YUK3B2_YUK006.ogg"
    # yuk "「……それが普段の由比ケ浜さんの空回り振りと重なってなんだかやるせなくて」"
    voice "audio/log/YUK3B2_YUK006.ogg"
    yuk "...Bu Yuigahama-sanning bekorchi harakatlarini eslatib yuborgani uchun kayfiyatimni buzdi."

# game/scripts/game/OG_TALK_YUK3.rpy:362
translate uzbek OG_TALK_YUK3_7_5a657b1e:

    # hcm "「それは考えすぎだと思うが……否定してやれねぇんだよなぁ」"
    hcm "Mence, bu ortiqcha o'ylash... lekin buni inkor ham qilolmayman-da."

# game/scripts/game/OG_TALK_YUK3.rpy:376
translate uzbek OG_TALK_YUK3_8_94dce423:

    # hcm "「そういや、夏休み中って奉仕部の活動もうねぇの？」"
    hcm "Aytgancha, yozgi ta'tilda Volontyorlik klubining faoliyati bo'lmaydimi?"

# game/scripts/game/OG_TALK_YUK3.rpy:378
translate uzbek OG_TALK_YUK3_8_97768326:

    # voice "audio/log/YUK3B3_YUK000.ogg"
    # yuk mra00 "「そうね、今のところは」"
    voice "audio/log/YUK3B3_YUK000.ogg"
    yuk mra00 "Ha, hozircha shunday."

# game/scripts/game/OG_TALK_YUK3.rpy:382
translate uzbek OG_TALK_YUK3_8_82bec34a:

    # hcm "「『今のところは』ってことは、なにかしら急に予定が入ることもあるのか？」"
    hcm "'Hozircha' deganing, to'satdan biror reja chiqib qolishi mumkin deganimi?"

# game/scripts/game/OG_TALK_YUK3.rpy:384
translate uzbek OG_TALK_YUK3_8_745e96a1:

    # voice "audio/log/YUK3B3_YUK001.ogg"
    # yuk "「普段どおり、平塚先生の気分次第というところかしら。もっとも、夏休みの間はその可能性も低いでしょうけれど」"
    voice "audio/log/YUK3B3_YUK001.ogg"
    yuk "Odatdagidek, Hiratsuka-sensening kayfiyatiga bog'liq. Lekin yozgi ta'tilda buning ehtimoli past."

# game/scripts/game/OG_TALK_YUK3.rpy:388
translate uzbek OG_TALK_YUK3_8_161c1105:

    # hcm "「飛び込みで依頼が来ることは？」"
    hcm "To'satdan so'rov kelib qolishi-chi?"

# game/scripts/game/OG_TALK_YUK3.rpy:390
translate uzbek OG_TALK_YUK3_8_d91eac6d:

    # voice "audio/log/YUK3B3_YUK002.ogg"
    # yuk "「誰がどう飛び込んでくるのかしら？」"
    voice "audio/log/YUK3B3_YUK002.ogg"
    yuk "Kim va qanday qilib kelishi mumkin?"

# game/scripts/game/OG_TALK_YUK3.rpy:394
translate uzbek OG_TALK_YUK3_8_9f280cac:

    # hcm "「どうって……あー、そういうことか」"
    hcm "Qanday qilib... ha, tushunarli."

# game/scripts/game/OG_TALK_YUK3.rpy:396
translate uzbek OG_TALK_YUK3_8_2bb61868:

    # voice "audio/log/YUK3B3_YUK003.ogg"
    # yuk mra05 "「ちょっと待ちなさい。今なにを納得したのかしら？」"
    voice "audio/log/YUK3B3_YUK003.ogg"
    yuk mra05 "Shoshma. Hozir nimani tushunding?"

# game/scripts/game/OG_TALK_YUK3.rpy:400
translate uzbek OG_TALK_YUK3_8_9a79f97b:

    # hcm "「だって、依頼するにはお前をつかまえるか連絡先を知らなきゃなんねーわけだし」"
    hcm "Axir, so'rov berish uchun seni topishlari yoki telefon raqamingni bilishlari kerak-ku."

# game/scripts/game/OG_TALK_YUK3.rpy:402
translate uzbek OG_TALK_YUK3_8_bb4befba:

    # voice "audio/log/YUK3B3_YUK004.ogg"
    # yuk "「だから？」"
    voice "audio/log/YUK3B3_YUK004.ogg"
    yuk "Xo'sh?"

# game/scripts/game/OG_TALK_YUK3.rpy:406
translate uzbek OG_TALK_YUK3_8_fc551e93:

    # hcm "「夏休み中にお前をつかまえられる機会なんてねぇし、連絡先知ってるヤツなんていな……なんで怒ってんだよ」"
    hcm "Yozgi ta'tilda seni topishning iloji yo'q, raqamingni esa hech kim bilmaydi... nega jahling chiqyapti?"

# game/scripts/game/OG_TALK_YUK3.rpy:408
translate uzbek OG_TALK_YUK3_8_508cd373:

    # voice "audio/log/YUK3B3_YUK005.ogg"
    # yuk mra02 "「怒ってなどいないわ。ただ、ずいぶんと知ったふうなことを言ってくれると思っただけよ」"
    voice "audio/log/YUK3B3_YUK005.ogg"
    yuk mra02 "Jahlim chiqqani yo'q. Shunchaki, o'zingcha hamma narsani bilag'ondek gapirishing g'alati tuyuldi."

# game/scripts/game/OG_TALK_YUK3.rpy:412
translate uzbek OG_TALK_YUK3_8_823463a0:

    # hcm "「知ってるに決まってるだろ。俺とお前で電話帳登録数に差があるとは思えん。ソースは俺」"
    hcm "Bilishim aniq-ku. Sening telefoningdagi kontaktlar soni menikidan ko'p bo'lmasa kerak. Men bunga aminman."

# game/scripts/game/OG_TALK_YUK3.rpy:414
translate uzbek OG_TALK_YUK3_8_90bcc96d:

    # voice "audio/log/YUK3B3_YUK006.ogg"
    # yuk "「……そうだったわね。発言を撤回するわ」"
    voice "audio/log/YUK3B3_YUK006.ogg"
    yuk "...To'g'ri. Gapimni qaytarib olaman."

# game/scripts/game/OG_TALK_YUK3.rpy:418
translate uzbek OG_TALK_YUK3_8_c2acee74:

    # hcm "「……素直なのはいいけど、なんでそう哀れみに満ちた目で見るのかな？　それ間違ってるよね？」"
    hcm "...Rostgo'yliging yaxshi-yu, lekin nega menga bunchalik achinib qarayapsan? Bu noto'g'ri-ku?"

# game/scripts/game/OG_TALK_YUK3.rpy:432
translate uzbek OG_TALK_YUK3_9_674cd10f:

    # hcm "「うちのカマクラは、おかげさんでのびのびやってるよ」"
    hcm "Bizning Kamakura sening sharofating bilan mazza qilib yashayapti."

# game/scripts/game/OG_TALK_YUK3.rpy:434
translate uzbek OG_TALK_YUK3_9_5ab57e91:

    # voice "audio/log/YUK3B4_YUK000.ogg"
    # yuk mra00 "「そう。のびのび……」"
    voice "audio/log/YUK3B4_YUK000.ogg"
    yuk mra00 "Shunaqami. Mazza qilib..."

# game/scripts/game/OG_TALK_YUK3.rpy:438
translate uzbek OG_TALK_YUK3_9_b9f3fff0:

    # hcm "「……ああ、のびのび」"
    hcm "...Ha, mazza qilib."

# game/scripts/game/OG_TALK_YUK3.rpy:440
translate uzbek OG_TALK_YUK3_9_bfc6eb2c:

    # voice "audio/log/YUK3B4_YUK001.ogg"
    # yuk "「……のびのび」"
    voice "audio/log/YUK3B4_YUK001.ogg"
    yuk "...Mazza qilib."

# game/scripts/game/OG_TALK_YUK3.rpy:444
translate uzbek OG_TALK_YUK3_9_1fd113fd:

    # hcm "「………」"
    hcm "........."

# game/scripts/game/OG_TALK_YUK3.rpy:446
translate uzbek OG_TALK_YUK3_9_8e7d4ed0:

    # voice "audio/log/YUK3B4_YUK002.ogg"
    # yuk "「………」"
    voice "audio/log/YUK3B4_YUK002.ogg"
    yuk "........."

# game/scripts/game/OG_TALK_YUK3.rpy:450
translate uzbek OG_TALK_YUK3_9_e57cde14:

    # hcm "（これはもっと猫の様子を話したほうがいいんだろうな。そろそろ雪ノ下検定３級くらいならとれそうな気がする今日この頃です）"
    hcm "(Mushuk haqida ko'proq gapirib berishim kerak shekilli. Bugungi kunda Yukinoshita haqidagi bilimim 3-darajaga yetib qoldi-yov.)"

# game/scripts/game/OG_TALK_YUK3.rpy:452
translate uzbek OG_TALK_YUK3_9_ca2d4822:

    # hcm "「ああ、のびのびしてるな。丸まってるなーと思ったら寝返り打ってうなーっと伸びるんだ」"
    hcm "Ha, mazza qilib yashayapti. Yumaloq bo'lib yotgan bo'lsa, bir ag'darilib, cho'zilib oladi."

# game/scripts/game/OG_TALK_YUK3.rpy:454
translate uzbek OG_TALK_YUK3_9_9ff12a0f:

    # hcm "「普段の１．５倍くらいの長さになって、名前呼ぶと気まぐれにしっぽで床タンッて叩いて……あの、なんでみるみる顔を曇らせてるんですか」"
    hcm "Odatdagidan bir yarim baravar uzun bo'lib ketadi, ismini chaqirsang, dumini polga urib qo'yadi... Iye, nega yuzing buncha tundlashib ketdi?"

# game/scripts/game/OG_TALK_YUK3.rpy:456
translate uzbek OG_TALK_YUK3_9_2e5b27ac:

    # voice "audio/log/YUK3B4_YUK003.ogg"
    # yuk mra02 "「……そう自慢げに話されても」"
    voice "audio/log/YUK3B4_YUK003.ogg"
    yuk mra02 "...Bunchalik maqtanib gapirmasang ham bo'lardi."

# game/scripts/game/OG_TALK_YUK3.rpy:460
translate uzbek OG_TALK_YUK3_9_56b12b86:

    # hcm "「話し方ひとつで不機嫌になるとか、どんだけ気難しいんだよお前は」"
    hcm "Gapirish ohangiga qarab ham kayfiyating buziladimi, buncha injiqsan?"

# game/scripts/game/OG_TALK_YUK3.rpy:474
translate uzbek OG_TALK_YUK3_10_f14eb44a:

    # hcm "「そういやペット同伴可な宿とか、考えなかったのかな、由比ケ浜ん家は」"
    hcm "Aytgancha, Yuigahamalar oilasi hayvonlar bilan borish mumkin bo'lgan mehmonxonani o'ylab ko'rishmaganmikin?"

# game/scripts/game/OG_TALK_YUK3.rpy:476
translate uzbek OG_TALK_YUK3_10_eb12a127:

    # voice "audio/log/YUK3B5_YUK000.ogg"
    # yuk mra00 "「ペットを飼っていない私でも、『ペット可』という条件を加えるだけで、旅先の選択肢がどれだけ減るか想像がつくわ」"
    voice "audio/log/YUK3B5_YUK000.ogg"
    yuk mra00 "Uy hayvoni bo'lmagan men ham 'hayvonlar bilan mumkin' degan shart qo'shilsa, tanlov imkoniyati qanchalik kamayib ketishini tasavvur qila olaman."

# game/scripts/game/OG_TALK_YUK3.rpy:480
translate uzbek OG_TALK_YUK3_10_db491055:

    # hcm "「それはそうだけど」"
    hcm "Bu ham to'g'ri."

# game/scripts/game/OG_TALK_YUK3.rpy:482
translate uzbek OG_TALK_YUK3_10_2c9291ba:

    # voice "audio/log/YUK3B5_YUK001.ogg"
    # yuk "「それに、もし旅先でサブレが病気にでもなったら、人間よりもきっといろいろと大変なのでしょう？」"
    voice "audio/log/YUK3B5_YUK001.ogg"
    yuk "Qolaversa, sayohat paytida Sable kasal bo'lib qolsa, odamlardan ko'ra qiyinroq bo'ladi, shunday emasmi?"

# game/scripts/game/OG_TALK_YUK3.rpy:486
translate uzbek OG_TALK_YUK3_10_fb68257c:

    # hcm "「ああ、そういう事もあるか」"
    hcm "Ha, bunday bo'lishi ham mumkin."

# game/scripts/game/OG_TALK_YUK3.rpy:488
translate uzbek OG_TALK_YUK3_10_3ac65d0f:

    # voice "audio/log/YUK3B5_YUK002.ogg"
    # yuk mra07 "「ペットを飼っているにしては不見識ね」"
    voice "audio/log/YUK3B5_YUK002.ogg"
    yuk mra07 "Uy hayvoni bor odam uchun juda befarq ekansan."

# game/scripts/game/OG_TALK_YUK3.rpy:492
translate uzbek OG_TALK_YUK3_10_a4c301a1:

    # hcm "「まぁな。飼われることに関しちゃ自信あるんだけどな……」"
    hcm "To'g'ri. Boqimanda bo'lish bo'yicha o'zimga ishonaman-u..."

# game/scripts/game/OG_TALK_YUK3.rpy:494
translate uzbek OG_TALK_YUK3_10_14fef336:

    # voice "audio/log/YUK3B5_YUK003.ogg"
    # yuk mra04 "「……はぁ」"
    voice "audio/log/YUK3B5_YUK003.ogg"
    yuk mra04 "...Eh."

# game/scripts/game/OG_TALK_YUK3.rpy:512
translate uzbek OG_TALK_YUK3_11_2a9e70cd:

    # hcm "「っつーか、別に手伝いに来なくてもよかったんじゃねーの？　お前ひとりでも全然大丈夫そうじゃねーか」"
    hcm "O'zi yordamga kelishim shart emasmidi deyman? O'zing ham eplaydigan ko'rinasan."

# game/scripts/game/OG_TALK_YUK3.rpy:514
translate uzbek OG_TALK_YUK3_11_9d61fdb0:

    # voice "audio/log/YUK3B6_YUK000.ogg"
    # yuk mra00 "「……そう、かもしれないわね」"
    voice "audio/log/YUK3B6_YUK000.ogg"
    yuk mra00 "...Balki shundaydir."

# game/scripts/game/OG_TALK_YUK3.rpy:518
translate uzbek OG_TALK_YUK3_11_b6228c86:

    # hcm "「いやー、楽でよかった、仕事しなくていいっていうのは素敵だなー」"
    hcm "Yaxshi bo'ldi, ishlamasdan dam olishga nima yetsin."

# game/scripts/game/OG_TALK_YUK3.rpy:520
translate uzbek OG_TALK_YUK3_11_53c87768:

    # voice "audio/log/YUK3B6_YUK001.ogg"
    # yuk mra06 "「…………」"
    voice "audio/log/YUK3B6_YUK001.ogg"
    yuk mra06 "........"

# game/scripts/game/OG_TALK_YUK3.rpy:524
translate uzbek OG_TALK_YUK3_11_dd70b9dc:

    # hcm "「〜〜♪」"
    hcm "~~♪"

# game/scripts/game/OG_TALK_YUK3.rpy:526
translate uzbek OG_TALK_YUK3_11_e0b5fd8d:

    # voice "audio/log/YUK3B6_YUK002.ogg"
    # yuk mra07 "「……」"
    voice "audio/log/YUK3B6_YUK002.ogg"
    yuk mra07 "..."

# game/scripts/game/OG_TALK_YUK3.rpy:530
translate uzbek OG_TALK_YUK3_11_a9da99e5:

    # hcm "「……ん？　どうした」"
    hcm "...Hm? Nima bo'ldi."

# game/scripts/game/OG_TALK_YUK3.rpy:532
translate uzbek OG_TALK_YUK3_11_315f3339:

    # voice "audio/log/YUK3B6_YUK003.ogg"
    # yuk "「…………」"
    voice "audio/log/YUK3B6_YUK003.ogg"
    yuk "........"

