# game/scripts/game/OG_D052ESS2.rpy:27
translate uzbek OG_D052ESS2_0_0c4359e5:

    # voice "audio/log/D052ESS2_TAI000.ogg"
    # tai "「な、なんすか？」"
    voice "audio/log/D052ESS2_TAI000.ogg"
    tai "N-nima ekan?"

# game/scripts/game/OG_D052ESS2.rpy:31
translate uzbek OG_D052ESS2_0_2686f6b3:

    # voice "audio/log/D052ESS2_HCM001.ogg"
    # hcm "「うちには少々変わり者の美人国語教師がいる。アラサーだが相当な美女でナイスバディだ……その人とうまくやれ」"
    voice "audio/log/D052ESS2_HCM001.ogg"
    hcm "Bizda biroz g'alati, lekin go'zal ona tili o'qituvchisi bor. O'ttiz yoshlardadir, ammo juda chiroyli va qomati ham kelishgan... O'sha bilan til topishib ketishing kerak."

# game/scripts/game/OG_D052ESS2.rpy:43
translate uzbek OG_D052ESS2_0_d097a433:

    # voice "audio/log/D052ESS2_TAI001.ogg"
    # tai "「ナイスバディの美女とうまくやるんすか！！　ぐ、具体的には？」"
    voice "audio/log/D052ESS2_TAI001.ogg"
    tai "Qomati kelishgan go'zal ayol bilan til topishishim kerakmi!! A, aniqrog'i nima qilishim kerak?"

# game/scripts/game/OG_D052ESS2.rpy:47
translate uzbek OG_D052ESS2_0_cb82a606:

    # voice "audio/log/D052ESS2_HCM002.ogg"
    # hcm "「コビを売れ、ほめたたえろ、少年漫画の話を振れ。とにかく逆らうな。アレに睨まれると高校生活は一転してバイオレンスに支配された闇世界となる」"
    voice "audio/log/D052ESS2_HCM002.ogg"
    hcm "Yaxshi ko'rinishga harakat qil, maqta, bolalar mangasi haqida gaplash. Eng asosiysi, gap qaytarma. Uning g'azabiga uchrasang, maktab hayoting bir zumda zo'ravonlik hukmronlik qiladigan zulmat dunyosiga aylanadi."

# game/scripts/game/OG_D052ESS2.rpy:57
translate uzbek OG_D052ESS2_0_85e44508:

    # voice "audio/log/D052ESS2_TAI002.ogg"
    # tai "「こ、怖い人なんすか？」"
    voice "audio/log/D052ESS2_TAI002.ogg"
    tai "Q-qo'rqinchli odammi?"

# game/scripts/game/OG_D052ESS2.rpy:61
translate uzbek OG_D052ESS2_0_cc4d5f1d:

    # voice "audio/log/D052ESS2_HCM003.ogg"
    # hcm "「ああ、怖い、いろんな意味で。しかし美女だぞ。アラサーだが」"
    voice "audio/log/D052ESS2_HCM003.ogg"
    hcm "Ha, qo'rqinchli, har xil ma'noda. Lekin go'zal. O'ttiz yoshlarda bo'lsa ham."

# game/scripts/game/OG_D052ESS2.rpy:71
translate uzbek OG_D052ESS2_0_d533e610:

    # voice "audio/log/D052ESS2_TAI003.ogg"
    # tai "「俺、俄然やる気出てきたっす！」"
    voice "audio/log/D052ESS2_TAI003.ogg"
    tai "Men yanada qiziqib qoldim!"

# game/scripts/game/OG_D052ESS2.rpy:81
translate uzbek OG_D052ESS2_0_10989ed3:

    # voice "audio/log/D052ESS2_KAW000.ogg"
    # kaw "「……あんたがさっきから話してるのってもしかして平塚先生のこと？」"
    voice "audio/log/D052ESS2_KAW000.ogg"
    kaw "...Sen hozir Hiratsuka-sensey haqida gapiryapsanmi?"

# game/scripts/game/OG_D052ESS2.rpy:85
translate uzbek OG_D052ESS2_0_eb481743:

    # voice "audio/log/D052ESS2_HCM004.ogg"
    # hcm "「うちに他に該当する教師いねえだろ」"
    voice "audio/log/D052ESS2_HCM004.ogg"
    hcm "Bizda bundan boshqa bunday ta'rifga mos keladigan o'qituvchi yo'q-ku."

# game/scripts/game/OG_D052ESS2.rpy:95
translate uzbek OG_D052ESS2_0_b7b3b7f8:

    # voice "audio/log/D052ESS2_KAW001.ogg"
    # kaw "「よくもまあそう好き勝手なこと言えるね……」"
    voice "audio/log/D052ESS2_KAW001.ogg"
    kaw "Qanday qilib og'zingga kelganni gapira olasan-a..."

# game/scripts/game/OG_D052ESS2.rpy:105
