    # kaw mca09 "「……なんであんたにそんなこといきなり話さなきゃなんないのさ」"
    voice "audio/log/KAW1B4_KAW001.ogg"
    kaw mca09 "「...Nega senga bunday narsalarni aytishim kerak?」"

# game/scripts/game/OG_TALK_KAW1.rpy:327
translate uzbek OG_TALK_KAW1_9_49c7223f:

    # hcm "「個人情報保護法だってあるもんな、悪かったよ。もう聞かん」"
    hcm "「Shaxsiy ma’lumotlarni himoya qilish qonuni bor-a, uzr. Boshqa so‘ramayman.」"

# game/scripts/game/OG_TALK_KAW1.rpy:329
translate uzbek OG_TALK_KAW1_9_9e199cbe:

    # voice "audio/log/KAW1B4_KAW002.ogg"
    # kaw mca10 "「…………芸とか」"
    voice "audio/log/KAW1B4_KAW002.ogg"
    kaw mca10 "「............Tikuvchilik.」"

# game/scripts/game/OG_TALK_KAW1.rpy:333
translate uzbek OG_TALK_KAW1_9_2b10d465:

    # hcm "「……は？　武芸？　似合う似合う、そんな感じ」"
    hcm "「...A? Jang san’atimi? Yarashadi, yarashadi, senga mos.」"

# game/scripts/game/OG_TALK_KAW1.rpy:335
translate uzbek OG_TALK_KAW1_9_38e80bde:

    # voice "audio/log/KAW1B4_KAW003.ogg"
    # kaw mca08 "「武芸じゃないよ、どう聞いたらそう聞こえんのさ！？」"
    voice "audio/log/KAW1B4_KAW003.ogg"
    kaw mca08 "「Jang san’ati emas! Qanday qilib shunaqa eshitding!?」"

# game/scripts/game/OG_TALK_KAW1.rpy:339
translate uzbek OG_TALK_KAW1_9_e35ac4c3:

    # hcm "「じゃなんだよ。もっぺんいってくれ」"
    hcm "「Unda nima? Yana bir marta aytib yubor.」"

# game/scripts/game/OG_TALK_KAW1.rpy:341
translate uzbek OG_TALK_KAW1_9_3e9d2a26:

    # voice "audio/log/KAW1B4_KAW004.ogg"
    # kaw mca05 "「……もういい」"
    voice "audio/log/KAW1B4_KAW004.ogg"
    kaw mca05 "「...Bo‘ldi, kerak emas.」"

# game/scripts/game/OG_TALK_KAW1.rpy:351
translate uzbek OG_TALK_KAW1_10_4c7d2bde:

    # hcm "「しかしお前、弟と仲いいな」"
    hcm "「Lekin sen ukang bilan inoq ekansan.」"

# game/scripts/game/OG_TALK_KAW1.rpy:353
translate uzbek OG_TALK_KAW1_10_3792b880:

    # voice "audio/log/KAW1B5_KAW000.ogg"
    # kaw mca07 "「……あんたに言われたくないけど」"
    voice "audio/log/KAW1B5_KAW000.ogg"
    kaw mca07 "「...Buni sen aytishing g‘alati.」"

# game/scripts/game/OG_TALK_KAW1.rpy:357
translate uzbek OG_TALK_KAW1_10_2ec8f408:

    # hcm "「なんだよ。うちは兄妹仲は普通だぞ」"
    hcm "「Nima deganing bu. Bizning oilada aka-singil munosabatlari oddiy.」"

# game/scripts/game/OG_TALK_KAW1.rpy:359
translate uzbek OG_TALK_KAW1_10_e5f2a686:

    # voice "audio/log/KAW1B5_KAW001.ogg"
    # kaw mca05 "「うちだって普通だよ」"
    voice "audio/log/KAW1B5_KAW001.ogg"
    kaw mca05 "「Bizda ham oddiy.」"

# game/scripts/game/OG_TALK_KAW1.rpy:363
translate uzbek OG_TALK_KAW1_10_7dcb0945:

    # hcm "「あれが普通ならほとんどのご家庭、戦争状態だぞ。普通の意味辞書で引いとけ。さっきから俺が大志になんか言うたびに赤くなったり青くなったり、まったく、ブラ」"
    hcm "「Agar shu oddiy bo‘lsa, ko‘pchilik oilalar urush holatida ekanda. “Oddiy” so‘zining ma’nosini lug‘atdan qarab ko‘r. Boyadan beri Taishiga biror narsa desam, goh qizarib, goh ko‘karib ketyapsan, rosa...」"

# game/scripts/game/OG_TALK_KAW1.rpy:365
translate uzbek OG_TALK_KAW1_10_4758949c:

    # voice "audio/log/KAW1B5_KAW002.ogg"
    # kaw mca11 "「はあ？　ブラ、なに？　あんた、あたしと大志になんか文句でもあんの？」"
    voice "audio/log/KAW1B5_KAW002.ogg"
    kaw mca11 "「A? Rosa nima? Sening men bilan Taishiga qandaydir e’tirozing bormi?」"

# game/scripts/game/OG_TALK_KAW1.rpy:369
translate uzbek OG_TALK_KAW1_10_78ed59d2:

    # hcm "「……ぶ、ブラインドタッチが得意なんですよ俺」"
    hcm "「...Me-men ko‘rmasdan yoza olaman.」"

# game/scripts/game/OG_TALK_KAW1.rpy:371
translate uzbek OG_TALK_KAW1_10_a13f16d3:

    # voice "audio/log/KAW1B5_KAW003.ogg"
    # kaw mca07 "「話の前後がつながってないよ」"
    voice "audio/log/KAW1B5_KAW003.ogg"
    kaw mca07 "「Gaping bir-biriga to‘g‘ri kelmayapti.」"

# game/scripts/game/OG_TALK_KAW1.rpy:375
translate uzbek OG_TALK_KAW1_10_59c3e59a:

    # hcm "「とりあえず、そう睨むのやめてくれませんか？」"
    hcm "「Iltimos, menga bunday o‘qrayib qaramang?」"

# game/scripts/game/OG_TALK_KAW1.rpy:392
translate uzbek OG_TALK_KAW1_11_cb96f666:

    # hcm "「ところでひとつ言っておくことがあるんだが」"
    hcm "「Aytgancha, bitta narsani aytib qo‘yay.」"

# game/scripts/game/OG_TALK_KAW1.rpy:394
translate uzbek OG_TALK_KAW1_11_baf4e996:

    # voice "audio/log/KAW1B6_KAW000.ogg"
    # kaw "「なに？」"
    voice "audio/log/KAW1B6_KAW000.ogg"
    kaw "「Nima?」"

# game/scripts/game/OG_TALK_KAW1.rpy:398
translate uzbek OG_TALK_KAW1_11_b9b3e671:

    # hcm "「お前の弟をうちの妹にあまり近付けないようにしてもらいたい」"
    hcm "「Ukangni singlimga yaqinlashtirmasligingni so‘rayman.」"

# game/scripts/game/OG_TALK_KAW1.rpy:400
translate uzbek OG_TALK_KAW1_11_146f09d5:

    # voice "audio/log/KAW1B6_KAW001.ogg"
    # kaw mca02 "「……はあ？　なんのハナシ？」"
    voice "audio/log/KAW1B6_KAW001.ogg"
    kaw mca02 "「...A? Nima deyapsan?」"

# game/scripts/game/OG_TALK_KAW1.rpy:404
translate uzbek OG_TALK_KAW1_11_ee24f274:

    # hcm "「あ、いやその。よく言うだろうが、男女七歳にして席を同じうせずと言ってだな、やはりお前の弟のようなヤカラと俺の妹がいっしょにいるのはだな」"
    hcm "「A, yo‘q, anavi. Eshitgandirsan, “Yetti yoshdan keyin o‘g‘il va qiz bir joyda o‘tirmasligi kerak” degan gap bor. Sening ukangga o‘xshagan bezori bilan mening singlimning birga bo‘lishi...」"

# game/scripts/game/OG_TALK_KAW1.rpy:406
translate uzbek OG_TALK_KAW1_11_f452067d:

    # voice "audio/log/KAW1B6_KAW002.ogg"
    # kaw mca03 "「うちの弟のようなヤカラってどういう意味さ」"
    voice "audio/log/KAW1B6_KAW002.ogg"
    kaw mca03 "「Mening ukamga o‘xshagan bezori deganing nimasi?」"

# game/scripts/game/OG_TALK_KAW1.rpy:410
translate uzbek OG_TALK_KAW1_11_0395a683:

    # hcm "「そのまんまの意味だよ、あ、お湯沸かすヤツじゃねぇぞ。ヤカラな、それ以上でもそれ以下でもない」"
    hcm "「To‘g‘ridan-to‘g‘ri ma’noda aytdim. A, choynak emas, bezori demoqchiman. Undan ortiq ham, kam ham emas.」"

# game/scripts/game/OG_TALK_KAW1.rpy:412
translate uzbek OG_TALK_KAW1_11_f597ede8:

    # voice "audio/log/KAW1B6_KAW003.ogg"
    # kaw "「あんたね、いい加減にしないとマジで怒るよ」"
    voice "audio/log/KAW1B6_KAW003.ogg"
    kaw "「Sen, haddingdan oshsang rostdan ham jahlimni chiqarasana.」"

# game/scripts/game/OG_TALK_KAW1.rpy:416
translate uzbek OG_TALK_KAW1_11_0421f9ea:

    # hcm "「もう激怒してるだろ……」"
    hcm "「Allaqachon jahlning chiqyapti-ku...」"

