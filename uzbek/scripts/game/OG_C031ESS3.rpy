    # tot "「ぼくにできることがあったら、なんでも言って、ね？」"
    voice "audio/log/C031ESS3_TOT005.ogg"
    tot "「Mendan biror yordam kerak bo‘lsa, bemalol ayt, xo‘pmi?」"

# game/scripts/game/OG_C031ESS3.rpy:79
translate uzbek OG_C031ESS3_0_9f61586e:

