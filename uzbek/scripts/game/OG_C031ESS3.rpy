# game/scripts/game/OG_C031ESS3.rpy:71
translate uzbek OG_C031ESS3_0_86986f76:

    # voice "audio/log/C031ESS3_HCM004.ogg"
    # hcm "「悪い、１７年間生きてきた中でそんなこと言われたのは初めてだと思ったら、こう、ぐっとこみ上げるものが……」"
    voice "audio/log/C031ESS3_HCM004.ogg"
    hcm "「Uzr, 17 yillik umrim davomida bunday gapni birinchi marta eshitishim, shunga biroz ko‘nglim bo‘shab ketdi...」"

# game/scripts/game/OG_C031ESS3.rpy:75
translate uzbek OG_C031ESS3_0_018d2e72:

