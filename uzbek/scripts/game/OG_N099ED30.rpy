    # hcm "「ああ」"
    voice "audio/log/N099ED30_HCM023.ogg"
    hcm "Ha."

# game/scripts/game/OG_N099ED30.rpy:911
translate uzbek OG_N099ED30_0_c7da7fab:

    # voice "audio/log/N099ED30_HAY017.ogg"
    # hay "「こういうとき漫画とかだったら、きっと戦友のような友情が芽生えるところなんだろうけど――」"
    voice "audio/log/N099ED30_HAY017.ogg"
    hay "Manga bo'lganda, bunday paytda jangchi do'stlardek rishta bog'lanardi, lekin—"

# game/scripts/game/OG_N099ED30.rpy:915
translate uzbek OG_N099ED30_0_01205b81:

    # hcm "（……それでも俺とは、仲良くやれない。わかってるさ、今更あんときの話蒸し返すまでもなくな）"
    hcm "(...Shunda ham men bilan do'st bo'la olmaysan. Bilaman, o'sha voqeani eslatish ham shart emas.)"

# game/scripts/game/OG_N099ED30.rpy:923
translate uzbek OG_N099ED30_0_69894dbb:

    # voice "audio/log/N099ED30_HAY018.ogg"
    # hay "「ただ、こうしていろんなヤツと、いろんな世界を知るっていうのも、悪くないもんだよな……比企谷くん」"
    voice "audio/log/N099ED30_HAY018.ogg"
    hay "Lekin, shunday qilib har xil odamlar va dunyolarni tanish ham yomon emas ekan... Hikigaya-kun."

# game/scripts/game/OG_N099ED30.rpy:927
translate uzbek OG_N099ED30_0_2ddb38a9:

    # voice "audio/log/N099ED30_HCM024.ogg"
    # hcm "「…………ああ、まあな」"
    voice "audio/log/N099ED30_HCM024.ogg"
    hcm "........Ha, shunday."

# game/scripts/game/OG_N099ED30.rpy:931
translate uzbek OG_N099ED30_0_81d96633:

    # hcm "（こいつ、俺の名前……普通に呼んだな）"
    hcm "(Bu bola ismimni... to'g'ri aytdi-ya.)"

# game/scripts/game/OG_N099ED30.rpy:933
translate uzbek OG_N099ED30_0_e267acca:

    # voice "audio/log/N099ED30_HAY019.ogg"
    # hay "「俺は楽しかったよ」"
    voice "audio/log/N099ED30_HAY019.ogg"
    hay "Men mazza qildim."

# game/scripts/game/OG_N099ED30.rpy:937
translate uzbek OG_N099ED30_0_8d228de4:

    # voice "audio/log/N099ED30_HCM025.ogg"
    # hcm "「そうか、俺は疲れた」"
    voice "audio/log/N099ED30_HCM025.ogg"
    hcm "Shunaqami, men charchadim."

# game/scripts/game/OG_N099ED30.rpy:947
translate uzbek OG_N099ED30_0_63a8d9a9:

    # voice "audio/log/N099ED30_HAY020.ogg"
    # hay "「ハハ……だろうな、顔に思いっきり出てる」"
    voice "audio/log/N099ED30_HAY020.ogg"
    hay "Haha... sezilyapti, yuzingdan ma'lum."

# game/scripts/game/OG_N099ED30.rpy:951
translate uzbek OG_N099ED30_0_02999c3d:

    # voice "audio/log/N099ED30_HCM026.ogg"
    # hcm "「……」"
    voice "audio/log/N099ED30_HCM026.ogg"
    hcm "..."

# game/scripts/game/OG_N099ED30.rpy:955
translate uzbek OG_N099ED30_0_8d6decc5:

    # hcm "（……仲良くはなれなくとも、楽しくはやれるとか……なるほど、それがお前の人付き合いの仕方ってことか）"
    hcm "(...Do'st bo'lmasak ham, yaxshi vaqt o'tkazish mumkin, degin... tushunarli, demak sen odamlar bilan shunday muomala qilarkansan-da.)"

# game/scripts/game/OG_N099ED30.rpy:957
translate uzbek OG_N099ED30_0_901ad8a7:

    # hcm "（好き嫌いだけで片付けないだなんて、そんなの俺には肩がこりすぎて、それだけで死んじまいそうだけどな……）"
    hcm "(Faqat yoqtirish-yoqtirmaslik bilan ish bitmaydi deb o'ylash, men uchun juda og'irlik qiladi, shuning o'zi meni holdan toydirib qo'yishi mumkin...)"

# game/scripts/game/OG_N099ED30.rpy:959
translate uzbek OG_N099ED30_0_af9cc104:

    # voice "audio/log/N099ED30_EBI022.ogg"
    # ebi "「わっ、全部あったー！　よかったー！」"
    voice "audio/log/N099ED30_EBI022.ogg"
    ebi "Voy, hammasi bor ekan! Xayriyat!"

# game/scripts/game/OG_N099ED30.rpy:963
translate uzbek OG_N099ED30_0_234ce0f0:

    # voice "audio/log/N099ED30_ZAI017.ogg"
    # zai "「う、うん……よかったです……」"
    voice "audio/log/N099ED30_ZAI017.ogg"
    zai "H-ha... yaxshi bo'libdi..."

# game/scripts/game/OG_N099ED30.rpy:973
translate uzbek OG_N099ED30_0_963e2db9:

    # voice "audio/log/N099ED30_HAY021.ogg"
    # hay "「……っと、あっちも••••無事解決のようだな」"
    voice "audio/log/N099ED30_HAY021.ogg"
    hay "...U tomon•••• ham hal bo'ldi shekilli."

# game/scripts/game/OG_N099ED30.rpy:977
translate uzbek OG_N099ED30_0_03dda5ac:

    # voice "audio/log/N099ED30_MIU014.ogg"
    # miu "「ねー、あーしお腹空いた。結局お昼もまともに食べてないし」"
    voice "audio/log/N099ED30_MIU014.ogg"
    miu "Eshit, qornim ochdi. Tushlik ham qilolmadik-ku."

# game/scripts/game/OG_N099ED30.rpy:987
translate uzbek OG_N099ED30_0_f0f38a86:

    # voice "audio/log/N099ED30_HAY022.ogg"
    # hay "「ああ、みんなでファミレスでも行こうか？」"
    voice "audio/log/N099ED30_HAY022.ogg"
    hay "Ha, hammamiz kafega boraylikmi?"

# game/scripts/game/OG_N099ED30.rpy:1002
translate uzbek OG_N099ED30_0_987baa97:

    # voice "audio/log/N099ED30_EBI023.ogg"
    # ebi "「うん、行こうよ！　ザイモクザくんも行くでしょ？」"
    voice "audio/log/N099ED30_EBI023.ogg"
    ebi "Ha, ketdik! Zaimokuza-kun ham boradi-a?"

# game/scripts/game/OG_N099ED30.rpy:1006
translate uzbek OG_N099ED30_0_32a6cc1e:

    # voice "audio/log/N099ED30_ZAI018.ogg"
    # zai "「はっいっ……いぎまっすふ……」"
    voice "audio/log/N099ED30_ZAI018.ogg"
    zai "Ha... boraman..."

# game/scripts/game/OG_N099ED30.rpy:1016
translate uzbek OG_N099ED30_0_a0cb6afd:

    # voice "audio/log/N099ED30_EBI024.ogg"
    # ebi "「ヒキタニくんは？」"
    voice "audio/log/N099ED30_EBI024.ogg"
    ebi "Hikitani-kun-chi?"

# game/scripts/game/OG_N099ED30.rpy:1020
translate uzbek OG_N099ED30_0_e361b1e1:

    # voice "audio/log/N099ED30_HCM027.ogg"
    # hcm "「……ん、行く」"
    voice "audio/log/N099ED30_HCM027.ogg"
    hcm "...Ha, boraman."

# game/scripts/game/OG_N099ED30.rpy:1024
translate uzbek OG_N099ED30_0_09cd28b4:

    # voice "audio/log/N099ED30_HAY023.ogg"
    # hay "「よし、決まりだな」"
    voice "audio/log/N099ED30_HAY023.ogg"
    hay "Yaxshi, kelishdik."

# game/scripts/game/OG_N099ED30.rpy:1034
translate uzbek OG_N099ED30_0_b7c584dc:

    # narrator "――『うまくやれ』と、あのとき平塚先生は言った。"
    narrator "—'Kelishib ishlanglar' degandi o'shanda Hiratsuka-sensey."

# game/scripts/game/OG_N099ED30.rpy:1036
translate uzbek OG_N099ED30_0_271deb2d:

    # narrator "合宿中がどうだったかはわからないが、しかし、今日の俺はそこそこ『うまく』やれたのかもしれない。"
    narrator "Lagerda qanday bo'lgan bilmadim-u, lekin bugun men 'kelishib ishlashni' eplay oldim shekilli."

# game/scripts/game/OG_N099ED30.rpy:1038
translate uzbek OG_N099ED30_0_a2730868:

    # narrator "なんせ経験不足なもんでな、そういうのイマイチわかんねーんだわ。けど、まぁ……"
    narrator "Axir tajribam yo'q-da, bunaqa narsalarni unchalik tushunmayman. Lekin, mayli..."

# game/scripts/game/OG_N099ED30.rpy:1040
translate uzbek OG_N099ED30_0_92e45d67:

    # voice "audio/log/N099ED30_EBI025.ogg"
    # ebi "「ヒキタニくーん、早くおいでよー」"
    voice "audio/log/N099ED30_EBI025.ogg"
    ebi "Hikitani-kun, tezroq keling!"

# game/scripts/game/OG_N099ED30.rpy:1044
translate uzbek OG_N099ED30_0_7cc687e7:

    # voice "audio/log/N099ED30_HCM028.ogg"
    # hcm "「お、おう！」"
    voice "audio/log/N099ED30_HCM028.ogg"
    hcm "X-xo'p!"

# game/scripts/game/OG_N099ED30.rpy:1056
translate uzbek OG_N099ED30_0_30c23c97:

    # narrator "――けどまぁ、とりあえず今日のところだけは、あいつらとメシを食いに行こうと思った。"
    narrator "—Lekin mayli, buguncha ular bilan ovqatlanishga borsam bo'lar."