    voice "audio/log/D052ESS4_TAI002.ogg"
    tai "T-to'g'ri..."

# game/scripts/game/OG_D052ESS4.rpy:85
translate uzbek OG_D052ESS4_0_4140d0ab:

    # hcm "（おいこらそこ！　ふたりで話をするんじゃない！　できるだけ３メートル以上離れろ！）"
    hcm "(Hoy, sizlar! Ikkalang gaplashmanglar! Iloji boricha bir-biringizdan 3 metr uzoqda turinglar!)"

# game/scripts/game/OG_D052ESS4.rpy:97
translate uzbek OG_D052ESS4_0_6ccd279d:

    # voice "audio/log/D052ESS4_TAI003.ogg"
    # tai "「や、お兄さんすいません、俺の質問が良くなかったっすね。お兄さんに答えられないこと聞いてほんとすんません」"
    voice "audio/log/D052ESS4_TAI003.ogg"
    tai "Yo'q, aka, uzr, men noto'g'ri savol beribman. Javob berolmaydigan narsani so'raganim uchun uzr."

# game/scripts/game/OG_D052ESS4.rpy:101
translate uzbek OG_D052ESS4_0_ae93c15e:

    # hcm "（……まあそうなんだが。しかし、俺に答えられないと断定されると、それはそれでそこはかとなく腹が立つのはなぜだ……）"
    hcm "(...Aslida-ku shunday. Lekin, javob berolmasligimga bunchalik ishonch bilan qarashsa, negadir odamning jahli chiqarkan...)"