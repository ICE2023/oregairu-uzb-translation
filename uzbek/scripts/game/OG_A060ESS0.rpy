    voice "audio/log/A060ESS0_HCM026.ogg"
    hcm "「Yuqori ball bo‘ldi.」"

# game/scripts/game/OG_A060ESS0.rpy:401
translate uzbek OG_A060ESS0_0_cfdc7c6e:

    # voice "audio/log/A060ESS0_KOM030.ogg"
    # kom "「で、でしょー！？　だ、だから、これからそのきっかけをですね……」"
    voice "audio/log/A060ESS0_KOM030.ogg"
    kom "「Shu-shundaymi!? Shu-shuning uchun, endi o‘sha imkoniyatni...」"

# game/scripts/game/OG_A060ESS0.rpy:407
translate uzbek OG_A060ESS0_0_3086cd16:

    # voice "audio/log/A060ESS0_HCM027.ogg"
    # hcm "「わかった。……付き合ってやるよ」"
    voice "audio/log/A060ESS0_HCM027.ogg"
    hcm "「Tushunarli. ...Bajaraman.」"

