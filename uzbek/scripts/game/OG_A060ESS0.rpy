# game/scripts/game/OG_A060ESS0.rpy:421
translate uzbek OG_A060ESS0_0_9be02545:

    # voice "audio/log/A060ESS0_HCM028.ogg"
    # hcm "「ああ。だから俺の気が変わらんうちに早く言え。なにすんだよ」"
    voice "audio/log/A060ESS0_HCM028.ogg"
    hcm "「Ha. Fikrim o‘zgarib qolmasidan tezroq ayt. Nima qilishim kerak?」"

# game/scripts/game/OG_A060ESS0.rpy:431
translate uzbek OG_A060ESS0_0_02ad8cb4:

    # voice "audio/log/A060ESS0_KOM032.ogg"
    # kom "「さすがお兄ちゃんだよ！　実はもう準備は整っちゃってるんだなー、これが」"
    voice "audio/log/A060ESS0_KOM032.ogg"
    kom "「Sizdan shuni kutgandim, akajon! Aslida hammasi tayyor, mana.」"

# game/scripts/game/OG_A060ESS0.rpy:435
translate uzbek OG_A060ESS0_0_8300c04d:

    # voice "audio/log/A060ESS0_HCM029.ogg"
    # hcm "「……昨日からなんかガサゴソやってると思ってたら……なにする気だ？」"
    voice "audio/log/A060ESS0_HCM029.ogg"
    hcm "「...Kechadan beri g‘imir-g‘imir qilib yurganding-a... nima qilmoqchisan o‘zi?」"

# game/scripts/game/OG_A060ESS0.rpy:445
translate uzbek OG_A060ESS0_0_63a6646e:

    # voice "audio/log/A060ESS0_KOM033.ogg"
    # kom "「それはあとのお楽しみ！　さ、行くよ！　今日は小町もいっしょだからね。おっでかっけおっでかっけ〜♪」"
    voice "audio/log/A060ESS0_KOM033.ogg"
    kom "「Uni keyin bilib olasiz! Qani, ketdik! Bugun Komachi ham siz bilan boradi. Sayrga chiqamiz, sayrga~♪」"

# game/scripts/game/OG_A060ESS0.rpy:449
translate uzbek OG_A060ESS0_0_8a806b7a:

    # voice "audio/log/A060ESS0_HCM030.ogg"
    # hcm "「え？　今から出掛けんの？　ちょっと急じゃない？　俺にも都合というものが」"
    voice "audio/log/A060ESS0_HCM030.ogg"
    hcm "「E? Hozir ketamizmi? Shoshilinch emasmi? Mening ham o‘z rejalarim bor...」"

# game/scripts/game/OG_A060ESS0.rpy:459
translate uzbek OG_A060ESS0_0_aa5305f5:

    # voice "audio/log/A060ESS0_KOM034.ogg"
    # kom "「お兄ちゃんが今日は一日ヒマで、ほっといたらどうせ引きこもって寂しく読書かなんかしてるだけだってことくらい、把握していない小町だと思う？」"
    voice "audio/log/A060ESS0_KOM034.ogg"
    kom "「Bugun akam kun bo‘yi bekorchi, o‘z holiga qo‘ysa uyda qamalib olib, yolg‘iz kitob o‘qiydi, deb o‘ylaysizmi Komachi buni bilmaydi?」"

# game/scripts/game/OG_A060ESS0.rpy:463
translate uzbek OG_A060ESS0_0_53dc173c:

    # voice "audio/log/A060ESS0_HCM031.ogg"
    # hcm "「違うぞ小町、読書は寂しくするもんじゃない、楽しくするもんだ。あとはおっしゃるとおりです」"
    voice "audio/log/A060ESS0_HCM031.ogg"
    hcm "「Adashding Komachi, kitob o‘qish yolg‘izlik emas, zavqlanishdir. Qolgan gaplaring to‘g‘ri.」"

# game/scripts/game/OG_A060ESS0.rpy:473
translate uzbek OG_A060ESS0_0_c27f9953:

    # voice "audio/log/A060ESS0_KOM035.ogg"
