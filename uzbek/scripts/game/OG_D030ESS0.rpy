    voice "audio/log/D030ESS0_HCM012.ogg"
    hcm "...Kawasaki, nega ishlayotganingni, pul nima uchun kerakligini aytib beraman."

# game/scripts/game/OG_D030ESS0.rpy:443
translate uzbek OG_D030ESS0_0_8c56b9e5:

    # voice "audio/log/D030ESS0_HCM013.ogg"
    # hcm "「大志、お前が中３になってからなにか変わったことはなかったか？」"
    voice "audio/log/D030ESS0_HCM013.ogg"
    hcm "Taishi, 3-sinfga o'tganingdan beri biror o'zgarish bo'ldimi?"

# game/scripts/game/OG_D030ESS0.rpy:456
