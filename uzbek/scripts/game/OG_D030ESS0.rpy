    # voice "audio/log/D030ESS0_KAW009.ogg"
    # kaw "「……大志には関係ないでしょ」"
    voice "audio/log/D030ESS0_KAW009.ogg"
    kaw "...Taishiga aloqasi yo'q."

# game/scripts/game/OG_D030ESS0.rpy:427
translate uzbek OG_D030ESS0_0_0dcd06fa:

    # voice "audio/log/D030ESS0_TAI010.ogg"
    # tai "「関係なくねえよ。家族じゃん」"
    voice "audio/log/D030ESS0_TAI010.ogg"
    tai "Aloqasi bor. Olamiz-ku."

# game/scripts/game/OG_D030ESS0.rpy:431
