# game/scripts/game/OG_N071HIR0.rpy:36
translate uzbek OG_N071HIR0_0_eebaf42c:

    # voice "audio/log/N071HIR0_HIR001.ogg"
    # hir "「こう見えて仕事中だ。夏休みで浮かれるお前たちの生活指導に借り出されているところでね」"
    voice "audio/log/N071HIR0_HIR001.ogg"
    hir "「Ko‘rinishimdan dam olayotganga o‘xshasam-da, ishlayapman. Yozgi ta’tilda aqlini yo‘qotib qo‘yadigan o‘quvchilarning tartibini nazorat qilib yuribman.」"

# game/scripts/game/OG_N071HIR0.rpy:40
translate uzbek OG_N071HIR0_0_7195ac07:

    # voice "audio/log/N071HIR0_HCM001.ogg"
    # hcm "「お疲れさまです。ですが、今の発言には誤りがありますね」"
    voice "audio/log/N071HIR0_HCM001.ogg"
    hcm "「Hormang. Lekin, hozirgi gapingizda xato bor.」"

# game/scripts/game/OG_N071HIR0.rpy:50
translate uzbek OG_N071HIR0_0_2b3006fb:

    # voice "audio/log/N071HIR0_HIR002.ogg"
    # hir "「ふむ、『お前たち』というのは君が含まれることを前提としているから、それは間違っていると言いたいのかね？」"
    voice "audio/log/N071HIR0_HIR002.ogg"
    hir "「Hm, “o‘quvchilar” deganda seni ham nazarda tutdim. Demak, senga bu gap taalluqli emas demoqchimisan?」"

# game/scripts/game/OG_N071HIR0.rpy:54
translate uzbek OG_N071HIR0_0_a1b3fa1b:

    # voice "audio/log/N071HIR0_HCM002.ogg"
    # hcm "「当然です。常に孤高を貫くこの俺が、誰かと浮かれモードでどうこうなんて、まずありえませんね」"
    voice "audio/log/N071HIR0_HCM002.ogg"
    hcm "「Albatta. Doim yolg‘izlikni afzal ko‘radigan men uchun, kim bilandir xursandchilik qilib yurish imkonsiz.」"

# game/scripts/game/OG_N071HIR0.rpy:64
translate uzbek OG_N071HIR0_0_ac2a9d9f:

    # voice "audio/log/N071HIR0_HIR003.ogg"
    # hir "「いいだろう、誤りを訂正すると共に謝罪しよう」"
    voice "audio/log/N071HIR0_HIR003.ogg"
    hir "「Mayli, xatoyimni tuzataman va uzr so‘rayman.」"

# game/scripts/game/OG_N071HIR0.rpy:68
translate uzbek OG_N071HIR0_0_c6895513:

    # voice "audio/log/N071HIR0_HCM003.ogg"
    # hcm "「自認していることとはいえ、他者から客観的に事実を突きつけられるってのもやっぱりアレなんで、そろそろいいっすかね？」"
    voice "audio/log/N071HIR0_HCM003.ogg"
    hcm "「O‘zim tan olsam ham, boshqalar tomonidan bu haqiqatni yuzimga solinishi yoqimsiz. Bo‘ldimi endi?」"

# game/scripts/game/OG_N071HIR0.rpy:81
translate uzbek OG_N071HIR0_0_c76f48af:

    # voice "audio/log/N071HIR0_HIR004.ogg"
    # hir "「おや、それは失礼。それで？　浮かれモードとは無縁の君が、こんなところでなにをしているんだい？」"
    voice "audio/log/N071HIR0_HIR004.ogg"
    hir "「O, uzr. Xo‘sh? Xursandchilikdan yiroq bo‘lgan sen bu yerda nima qilyapsan?」"

# game/scripts/game/OG_N071HIR0.rpy:85
translate uzbek OG_N071HIR0_0_1223249b:

    # voice "audio/log/N071HIR0_HCM004.ogg"
    # hcm "「浮かれてなくても買い物くらいはしますし、つーか、予備校の帰りっすよ」"
    voice "audio/log/N071HIR0_HCM004.ogg"
    hcm "「Xursand bo‘lmasam ham bozor-o‘char qilaman-ku. O‘zi kursdan qaytayotgandim.」"

# game/scripts/game/OG_N071HIR0.rpy:95
translate uzbek OG_N071HIR0_0_e39ea9f8:

    # voice "audio/log/N071HIR0_HIR005.ogg"
    # hir "「ほう、なかなか感心じゃないか。予備校は楽しいかね？」"
    voice "audio/log/N071HIR0_HIR005.ogg"
    hir "「O‘h-ho, maqtovga loyiqsan. Tayyorlov kursi qiziqarli ekanmi?」"

# game/scripts/game/OG_N071HIR0.rpy:99
translate uzbek OG_N071HIR0_0_1dd95b8e:

    # voice "audio/log/N071HIR0_HCM005.ogg"
    # hcm "「勉強を目的とし、そのためだけにある空間というのは居心地がいいですし、授業内容も悪くない。それを『楽しい』と表現できるのであれば、楽しいですね」"
    voice "audio/log/N071HIR0_HCM005.ogg"
    hcm "「Faqat o‘qish uchun mo‘ljallangan joyda bo‘lish menga yoqadi. Darslar ham yomon emas. Agar buni “qiziqarli” deb atasa bo‘lsa, demak qiziqarli.」"

# game/scripts/game/OG_N071HIR0.rpy:109
translate uzbek OG_N071HIR0_0_9381857b:

    # voice "audio/log/N071HIR0_HIR006.ogg"
    # hir "「相変わらずだな君は。だが、楽しいならなによりだ。君ならきっと、大学はもっと楽しいところに感じられるかもしれんな」"
    voice "audio/log/N071HIR0_HIR006.ogg"
    hir "「Haliyam o‘sh-o‘shasan-a. Lekin, senga yoqayotgan bo‘lsa, shuning o‘zi yetarli. Balki universitet senga bundan ham qiziqarliroq tuyular.」"

# game/scripts/game/OG_N071HIR0.rpy:113
translate uzbek OG_N071HIR0_0_21d3bdb2:

    # voice "audio/log/N071HIR0_HCM006.ogg"
    # hcm "「おっと、ここで進路指導ですか？」"
    voice "audio/log/N071HIR0_HCM006.ogg"
    hcm "「Iye, shu yerda ham kelajak haqida maslahatlashamizmi?」"

# game/scripts/game/OG_N071HIR0.rpy:123
translate uzbek OG_N071HIR0_0_2c753286:

    # voice "audio/log/N071HIR0_HIR007.ogg"
    # hir "「それでも構わんよ？　ちょうど小腹も空いていたところだ、なんなら、そこでラーメンでも食べながら話そうか？　これでもトンコツには少しうるさくてね」"
    voice "audio/log/N071HIR0_HIR007.ogg"
    hir "「Nega endi bo‘lmas ekan? Mening ham qornim ochqab turgandi. Agar xohlasang, ramen yeb gaplashamiz. Tonkotsu ramenini yaxshi tushunaman.」"

# game/scripts/game/OG_N071HIR0.rpy:127
translate uzbek OG_N071HIR0_0_15d4ba59:

    # voice "audio/log/N071HIR0_HCM007.ogg"
    # hcm "「ラーメンは俺も好きですけど、今食うにはちょっと重すぎなんで。また改めて」"
    voice "audio/log/N071HIR0_HCM007.ogg"
    hcm "「Ramen menga ham yoqadi, lekin hozir yeyishga og‘irlik qiladi. Boshqa safar.」"

# game/scripts/game/OG_N071HIR0.rpy:131
translate uzbek OG_N071HIR0_0_76a577f9:

    # voice "audio/log/N071HIR0_HIR008.ogg"
    # hir "「そうか、ではまた今度。私は小腹を満たして行くことにしよう」"
    voice "audio/log/N071HIR0_HIR008.ogg"
    hir "「Shunaqami, unda boshqa safar. Men esa borib qornimni to‘yg‘azib olaman.」"

# game/scripts/game/OG_N071HIR0.rpy:135
translate uzbek OG_N071HIR0_0_76d0bae6:

    # voice "audio/log/N071HIR0_HCM008.ogg"
    # hcm "「小腹をラーメンで満たすような人が、そのスタイルを維持できるもんなんすか？」"
    voice "audio/log/N071HIR0_HCM008.ogg"
    hcm "「Ramen bilan qornini to‘yg‘azadigan odam qanday qilib qomatini saqlab qolishi mumkin?」"

# game/scripts/game/OG_N071HIR0.rpy:145
translate uzbek OG_N071HIR0_0_ec485290:

    # voice "audio/log/N071HIR0_HIR009.ogg"
    # hir "「女性には秘密があるのさ」"
    voice "audio/log/N071HIR0_HIR009.ogg"
    hir "「Ayollarning sirlari bo‘ladi.」"

# game/scripts/game/OG_N071HIR0.rpy:149
translate uzbek OG_N071HIR0_0_9be3c34c:

    # voice "audio/log/N071HIR0_HCM009.ogg"
    # hcm "「いや、そんな急に女性を意識させられても」"
    voice "audio/log/N071HIR0_HCM009.ogg"
    hcm "「E, birdaniga ayolligingizni eslatib qo‘ymasangiz ham bo‘lardi.」"

# game/scripts/game/OG_N071HIR0.rpy:159
translate uzbek OG_N071HIR0_0_80b4402d:

    # voice "audio/log/N071HIR0_HIR010.ogg"
    # hir "「フフ、今君がした一連の発言は、セクハラに抵触しかねんので気をつけるように。ではな」"
    voice "audio/log/N071HIR0_HIR010.ogg"
    hir "「Fufu, hozirgi gaplaring shilqimlikka kirib qolishi mumkin, ehtiyot bo‘l. Bo‘pti, xayr.」"

# game/scripts/game/OG_N071HIR0.rpy:163
translate uzbek OG_N071HIR0_0_587fdf9e:

    # voice "audio/log/N071HIR0_HCM010.ogg"
    # hcm "「あ、はい、失礼します」"
    voice "audio/log/N071HIR0_HCM010.ogg"
    hcm "「A, ha, xayr.」"