# game/scripts/game/OG_C099ED10.rpy:14
translate uzbek OG_C099ED10_0_dff24d20:

    # voice "audio/log/C099ED10_HCM000.ogg"
    # hcm "「――それからの、残り少ない夏休み。」"
    voice "audio/log/C099ED10_HCM000.ogg"
    hcm "—Shundan so'ng, yozgi ta'tilning qolgan sanoqli kunlari."

# game/scripts/game/OG_C099ED10.rpy:18
translate uzbek OG_C099ED10_0_28f10025:

    # voice "audio/log/C099ED10_HCM001.ogg"
    # hcm "「『世界滅亡まであと何日ッ！』という感覚で、冷蔵庫に貼られたカレンダーの×印はその数を増やしていく。」"
    voice "audio/log/C099ED10_HCM001.ogg"
    hcm "Muzlatgichga yopishtirilgan taqvimdagi x belgilari xuddi 'Dunyo oxirigacha shuncha kun qoldi!' degan his bilan ko'payib borardi."

# game/scripts/game/OG_C099ED10.rpy:22
translate uzbek OG_C099ED10_0_cd5f0793:

    # voice "audio/log/C099ED10_HCM002.ogg"
    # hcm "「メメントモリ――『死を思え』とはよく言ったもんで、終わりの日を思えば、今日という一日がいかに貴重で愛しいかがよくわかる。」"
    voice "audio/log/C099ED10_HCM002.ogg"
    hcm "Memento mori — 'O'limni esla', deb bejiz aytishmagan. Oxirat kunini o'ylasang, bugungi kuning qanchalik qadrli va aziz ekanini tushunib yetasan."

# game/scripts/game/OG_C099ED10.rpy:26
translate uzbek OG_C099ED10_0_025c533f:

    # voice "audio/log/C099ED10_HCM003.ogg"
    # hcm "「そして俺たちは、残り少ない夏休みを満喫することに躍起になるのだ。」"
    voice "audio/log/C099ED10_HCM003.ogg"
    hcm "Shunday qilib, biz yozgi ta'tilning qolgan qismidan to'liq zavqlanishga astoydil harakat qilardik."

# game/scripts/game/OG_C099ED10.rpy:38
translate uzbek OG_C099ED10_0_87f6de24:

    # voice "audio/log/C099ED10_HCM004.ogg"
    # hcm "「じゃあな小町、予備校行ってくる」"
    voice "audio/log/C099ED10_HCM004.ogg"
    hcm "Bo'pti Komachi, men tayyorlov kursiga ketdim."

# game/scripts/game/OG_C099ED10.rpy:48
translate uzbek OG_C099ED10_0_b21a4987:

    # voice "audio/log/C099ED10_KOM000.ogg"
    # kom "「うん。お母さんが、たまには夜ゴハンみんなで食べに行こうって言ってたよ。お兄ちゃん、どう？」"
    voice "audio/log/C099ED10_KOM000.ogg"
    kom "Xo'p. Oyim 'balki bugun kechki ovqatni hamma birga yermiz', degandilar. Aka, senga to'g'ri keladimi?"

# game/scripts/game/OG_C099ED10.rpy:52
translate uzbek OG_C099ED10_0_e8f36e8d:

    # voice "audio/log/C099ED10_HCM005.ogg"
    # hcm "「あー……悪い、今日は予備校のあとに映画観る約束があるから、今度な」"
    voice "audio/log/C099ED10_HCM005.ogg"
    hcm "E... uzr, bugun darsdan keyin kino ko'rishga kelishib qo'ygandim, keyingi safar."

# game/scripts/game/OG_C099ED10.rpy:62
translate uzbek OG_C099ED10_0_0a78831d:

    # voice "audio/log/C099ED10_KOM001.ogg"
    # kom "「約束って、もしかしてまた戸塚さん？」"
    voice "audio/log/C099ED10_KOM001.ogg"
    kom "Kelishuv deysanmi, yo yana Totsuka bilanmi?"

# game/scripts/game/OG_C099ED10.rpy:66
translate uzbek OG_C099ED10_0_e5c45836:

    # voice "audio/log/C099ED10_HCM006.ogg"
    # hcm "「またってほどじゃないだろ。２日ぶりだし」"
    voice "audio/log/C099ED10_HCM006.ogg"
    hcm "Yana dema. Endigina ikki kun bo'ldi-ku."

# game/scripts/game/OG_C099ED10.rpy:76
translate uzbek OG_C099ED10_0_5da70737:

    # voice "audio/log/C099ED10_KOM002.ogg"
    # kom "「たったの２日……！　あの出不精のお兄ちゃんの発言とは思えない」"
    voice "audio/log/C099ED10_KOM002.ogg"
    kom "Borb-yo'g'i ikki kun...! Uydan chiqmaydigan akamdan bu gapni eshitish g'alati."

# game/scripts/game/OG_C099ED10.rpy:80
translate uzbek OG_C099ED10_0_91320db3:

    # voice "audio/log/C099ED10_HCM007.ogg"
    # hcm "「おおげさだな、戸塚と遊ぶために出歩くくらい普通だろ」"
    voice "audio/log/C099ED10_HCM007.ogg"
    hcm "Oshirib yubording, Totsuka bilan aylanish uchun ko'chaga chiqish oddiy hol-ku."

# game/scripts/game/OG_C099ED10.rpy:90
translate uzbek OG_C099ED10_0_fcbc662d:

    # voice "audio/log/C099ED10_KOM003.ogg"
    # kom "「戸塚さん、恐ろしい人……！」"
    voice "audio/log/C099ED10_KOM003.ogg"
    kom "Totsuka, dahshatli inson...!"

# game/scripts/game/OG_C099ED10.rpy:94
translate uzbek OG_C099ED10_0_4155b2b6:

    # voice "audio/log/C099ED10_HCM008.ogg"
    # hcm "「そういうことだから、母ちゃんにはテキトーに言っといてくれ。じゃあな」"
    voice "audio/log/C099ED10_HCM008.ogg"
    hcm "Xullas shunaqa gaplar, oyimga bir nimalar deb qo'yarsan. Bo'pti."

# game/scripts/game/OG_C099ED10.rpy:104
translate uzbek OG_C099ED10_0_06674bb4:

    # voice "audio/log/C099ED10_KOM004.ogg"
    # kom "「はぁ……小町、いろんな意味でお兄ちゃんが心配だよ」"
    voice "audio/log/C099ED10_KOM004.ogg"
    kom "Eh... Komachi akasidan har tomonlama xavotirda."

# game/scripts/game/OG_C099ED10.rpy:118
translate uzbek OG_C099ED10_0_54800597:

    # hcm "（俺だって心配だよ。この時間がいつか終わってしまうんじゃないかと）"
    hcm "(O'zim ham xavotirdaman. Bu vaqt bir kun kelib tugab qolmasmikin deb.)"

# game/scripts/game/OG_C099ED10.rpy:126
translate uzbek OG_C099ED10_0_b91b8cbd:

    # voice "audio/log/C099ED10_TOT000.ogg"
    # tot "「八幡っ！」"
    voice "audio/log/C099ED10_TOT000.ogg"
    tot "Hachiman!"

# game/scripts/game/OG_C099ED10.rpy:132
translate uzbek OG_C099ED10_0_68555077:

    # hcm "（けれど、一瞬の輝きは永遠にも勝る）"
    hcm "(Ammo, bir lahzalik yorqinlik abadiyatdan ham ustundir.)"

# game/scripts/game/OG_C099ED10.rpy:140
translate uzbek OG_C099ED10_0_7b19c06c:

    # hcm "（戸塚が笑顔で迎えてくれるなら、その時間に価値を見出せる――守りたい、この笑顔）"
    hcm "(Agar Totsuka meni tabassum bilan kutib olsa, bu vaqtning qadri bo'ladi — bu tabassumni asrashim kerak.)"

# game/scripts/game/OG_C099ED10.rpy:142
translate uzbek OG_C099ED10_0_25444642:

    # voice "audio/log/C099ED10_HCM009.ogg"
    # hcm "「おう。今日も部活だったのか？　おつかれさん」"
    voice "audio/log/C099ED10_HCM009.ogg"
    hcm "Salom. Bugun ham klub mashg'ulotlari bor edimi? Charchamadingmi?"

# game/scripts/game/OG_C099ED10.rpy:152
translate uzbek OG_C099ED10_0_90bd602d:

    # voice "audio/log/C099ED10_TOT001.ogg"
    # tot "「八幡も、予備校おつかれさま。えらいよね、スカラシップまで取って。ぼくもそろそろ受験対策を真面目に考えなきゃいけないんだけど……」"
    voice "audio/log/C099ED10_TOT001.ogg"
    tot "Hachiman, sen ham o'qishdan charchading. Grant yutib olganing tahsinga sazovor. Men ham endi imtihonlarga jiddiy tayyorlanishim kerak..."

# game/scripts/game/OG_C099ED10.rpy:156
translate uzbek OG_C099ED10_0_db1a6720:

    # voice "audio/log/C099ED10_HCM010.ogg"
    # hcm "「俺はヒマだから行ってるだけだ。まぁ、早めに勉強を始めるに越したことはねぇけどな」"
    voice "audio/log/C099ED10_HCM010.ogg"
    hcm "Men bekorchilikdan qatnayapman, xolos. Mayli, o'qishni ertaroq boshlagan yaxshi baribir."

# game/scripts/game/OG_C099ED10.rpy:160
translate uzbek OG_C099ED10_0_a27dc7cb:

    # voice "audio/log/C099ED10_TOT002.ogg"
    # tot "「うん。それに、頑張らないと同じ大学に行けなくなっちゃうし」"
    voice "audio/log/C099ED10_TOT002.ogg"
    tot "Ha. Qolaversa, harakat qilmasam, bir xil universitetga kira olmay qolamiz."

# game/scripts/game/OG_C099ED10.rpy:164
translate uzbek OG_C099ED10_0_7d786c45:

    # voice "audio/log/C099ED10_HCM011.ogg"
    # hcm "「誰と」"
    voice "audio/log/C099ED10_HCM011.ogg"
    hcm "Kim bilan?"

# game/scripts/game/OG_C099ED10.rpy:174
translate uzbek OG_C099ED10_0_cc686e86:

    # voice "audio/log/C099ED10_TOT003.ogg"
    # tot "「八幡と、だよ？」"
    voice "audio/log/C099ED10_TOT003.ogg"
    tot "Hachiman bilan, albatta?"

# game/scripts/game/OG_C099ED10.rpy:178
translate uzbek OG_C099ED10_0_de2938ff:

    # hcm "（お、おおおお同じ大学どころか、同じ墓に入りませんか！？　で、でもでも戸塚は天使だから、仏式のうちの墓だとちょっとどうなの！？）"
    hcm "(Bi-bi-bir xil universitet tugul, bitta qabrga kirmaymizmi!? Le-lekin Totsuka farishta-ku, bizning budda uslubidagi qabrimizga to'g'ri kelarmikin!?)"

# game/scripts/game/OG_C099ED10.rpy:180
translate uzbek OG_C099ED10_0_9c745b5c:

    # voice "audio/log/C099ED10_HCM012.ogg"
    # hcm "「たぶん冬期講習も行くと思うから、いっしょに申し込むか？」"
    voice "audio/log/C099ED10_HCM012.ogg"
    hcm "Qishki kurslarga ham borsam kerak, birga yozilamizmi?"

# game/scripts/game/OG_C099ED10.rpy:190
translate uzbek OG_C099ED10_0_fd32fd27:

    # voice "audio/log/C099ED10_TOT004.ogg"
    # tot "「うん！　あ、そろそろチケット買った方がいいかな……映画、なに観る？」"
    voice "audio/log/C099ED10_TOT004.ogg"
    tot "Mayli! Aytgancha, chipta olish kerakmidi... qaysi kinoni ko'ramiz?"

# game/scripts/game/OG_C099ED10.rpy:194
translate uzbek OG_C099ED10_0_eef92d87:

    # voice "audio/log/C099ED10_HCM013.ogg"
    # hcm "「戸塚の観たいヤツでいいぞ」"
    voice "audio/log/C099ED10_HCM013.ogg"
    hcm "Totsuka qaysinisini xohlasa, shuni ko'ramiz."

# game/scripts/game/OG_C099ED10.rpy:198
translate uzbek OG_C099ED10_0_c85abebc:

    # voice "audio/log/C099ED10_TOT005.ogg"
    # tot "「そんな、悪いよ。八幡は気になる映画、ないの？」"
    voice "audio/log/C099ED10_TOT005.ogg"
    tot "Bu noqulay-ku. Hachiman, o'zing qiziqqan kino yo'qmi?"

# game/scripts/game/OG_C099ED10.rpy:202
translate uzbek OG_C099ED10_0_645d8313:

    # voice "audio/log/C099ED10_HCM014.ogg"
    # hcm "「予告を見てちょいちょい気になったのはあるけどな……今なにが上映中なのか、イマイチ把握してねぇし」"
    voice "audio/log/C099ED10_HCM014.ogg"
    hcm "Treyerlarda qiziq tuyulganlari bor edi-yu... lekin hozir nimalar ko'rsatilayotganini aniq bilmayman."

# game/scripts/game/OG_C099ED10.rpy:206
translate uzbek OG_C099ED10_0_6970147b:

    # voice "audio/log/C099ED10_TOT006.ogg"
    # tot "「じゃあ、ポスター見ながらふたりで決めようか」"
    voice "audio/log/C099ED10_TOT006.ogg"
    tot "Unda, afishalarni ko'rib birga tanlaymiz."

# game/scripts/game/OG_C099ED10.rpy:210
translate uzbek OG_C099ED10_0_b987fb0d:

    # hcm "（ふたりで。ふたりで。　ふ　た　り　で　！　）"
    hcm "(Birga. Birga. Birga!)"

# game/scripts/game/OG_C099ED10.rpy:212
translate uzbek OG_C099ED10_0_128185db:

    # hcm "（映画なんてぼっち鑑賞が当たり前、映画どころかオールウェイズぼっちが当たり前だった俺に、新しい文化が開花。ビバ文明開化！）"
    hcm "(Kino tugul, doimo yolg'iz yurishga o'rgangan menda yangi madaniyat gullamoqda. Yashasin sivilizatsiya!)"

# game/scripts/game/OG_C099ED10.rpy:220
translate uzbek OG_C099ED10_0_e925f0ae:

    # voice "audio/log/C099ED10_TOT007.ogg"
    # tot "「うーん、なにがいいかな？」"
    voice "audio/log/C099ED10_TOT007.ogg"
    tot "Xo'sh, qaysi biri yaxshi ekan?"

# game/scripts/game/OG_C099ED10.rpy:224
translate uzbek OG_C099ED10_0_a7a011c0:

    # voice "audio/log/C099ED10_HCM015.ogg"
    # hcm "「そうだな、しいて言えば……」"
    voice "audio/log/C099ED10_HCM015.ogg"
    hcm "Ko'ramiz, masalan mana bu..."

# game/scripts/game/OG_C099ED10.rpy:234
translate uzbek OG_C099ED10_1_ce070db3:

    # voice "audio/log/C099ED10_TOT008.ogg"
    # tot "「あっ、ぼくもそれ観たいって思ってたんだ。それにしようよ」"
    voice "audio/log/C099ED10_TOT008.ogg"
    tot "A, men ham shuni ko'rmoqchi edim. Shuni tanlaylik."

# game/scripts/game/OG_C099ED10.rpy:238
translate uzbek OG_C099ED10_1_640c3cfc:

    # voice "audio/log/C099ED10_HCM016.ogg"
    # hcm "「じゃあ決まりだな」"
    voice "audio/log/C099ED10_HCM016.ogg"
    hcm "Kelishdik unda."

# game/scripts/game/OG_C099ED10.rpy:252
translate uzbek OG_C099ED10_1_d2696527:

    # voice "audio/log/C099ED10_TOT009.ogg"
    # tot "「……ひっ！」"
    voice "audio/log/C099ED10_TOT009.ogg"
    tot "...Hih!"

# game/scripts/game/OG_C099ED10.rpy:256
translate uzbek OG_C099ED10_1_31e6621f:

    # hcm "（うおぉぉ、戸塚の指が！　ちまって、ちまって俺の服を……！）"
    hcm "(Voh, Totsukaning barmoqlari! Kiyimimni mahkam ushlab oldi...!)"

# game/scripts/game/OG_C099ED10.rpy:258
translate uzbek OG_C099ED10_1_2bd63279:

    # hcm "（ヤバイ、この映画まじ怖い。なにが怖いって、超えてはいけない一線を軽々と超えてしまいそうで怖い……！）"
    hcm "(Dahshat, bu kino rostdan ham qo'rqinchli. Eng qo'rqinchlisi shundaki, men o'tmaslik kerak bo'lgan chegaradan osongina o'tib ketishim mumkin...!)"

# game/scripts/game/OG_C099ED10.rpy:266
translate uzbek OG_C099ED10_1_09e7c64b:

    # voice "audio/log/C099ED10_TOT010.ogg"
    # tot "「……っ！」"
    voice "audio/log/C099ED10_TOT010.ogg"
    tot "...Ah!"

# game/scripts/game/OG_C099ED10.rpy:270
translate uzbek OG_C099ED10_1_56712b61:

    # hcm "（あああああ、なんかぎゅってされた！　わー、戸塚ってばプルプルしてるぅ。怖い、だから怖いって！）"
    hcm "(Aaaaa, meni quchoqlab oldi! Voy, Totsuka qaltirayapti. Qo'rqinchli, aytyapman-ku qo'rqinchli deb!)"

# game/scripts/game/OG_C099ED10.rpy:282
translate uzbek OG_C099ED10_1_d68fe670:

    # voice "audio/log/C099ED10_TOT011.ogg"
    # tot "「はぁ……面白かったぁ。すっごく怖かったね」"
    voice "audio/log/C099ED10_TOT011.ogg"
    tot "Uh... Qiziq ekan. Juda qo'rqinchli bo'ldi-a."

# game/scripts/game/OG_C099ED10.rpy:286
translate uzbek OG_C099ED10_1_c813b181:

    # voice "audio/log/C099ED10_HCM017.ogg"
    # hcm "「ああ、色んな意味で、本当にな……」"
    voice "audio/log/C099ED10_HCM017.ogg"
    hcm "Ha, har xil ma'noda, rostdan ham..."

# game/scripts/game/OG_C099ED10.rpy:290
translate uzbek OG_C099ED10_1_92f9f81e:

    # hcm "（ていうか、疲れた。心臓がバクバクしすぎてもうぐったりだ）"
    hcm "(To'g'risi, charchadim. Yuragim shu qadar dukulladiki, holdan toydim.)"

# game/scripts/game/OG_C099ED10.rpy:302
translate uzbek OG_C099ED10_2_595a2d4e:

    # voice "audio/log/C099ED10_TOT012.ogg"
    # tot "「うん、いいね！　スカッとしそう」"
    voice "audio/log/C099ED10_TOT012.ogg"
    tot "Ha, zo'r! Mazza qilamiz."

# game/scripts/game/OG_C099ED10.rpy:306
translate uzbek OG_C099ED10_2_596f28eb:

    # voice "audio/log/C099ED10_HCM018.ogg"
    # hcm "「オッケー、決まりな。チケット買ってくるか」"
    voice "audio/log/C099ED10_HCM018.ogg"
    hcm "Yaxshi, kelishdik. Men chipta olib kelaman."

# game/scripts/game/OG_C099ED10.rpy:320
translate uzbek OG_C099ED10_2_047444b0:

    # voice "audio/log/C099ED10_TOT013.ogg"
    # tot "「わっ、すごい……！」"
    voice "audio/log/C099ED10_TOT013.ogg"
    tot "Voah, ajoyib...!"

# game/scripts/game/OG_C099ED10.rpy:324
translate uzbek OG_C099ED10_2_a06f0470:

    # hcm "（お、身を乗り出して観てるな……戸塚、結構男っぽいモン好きだったりするよな）"
    hcm "(O, berilib ko'ryapti... Totsuka o'g'il bolalarga xos narsalarni yoqtiradi-da baribir.)"

# game/scripts/game/OG_C099ED10.rpy:328
translate uzbek OG_C099ED10_2_e3d6cc8b:

    # voice "audio/log/C099ED10_TOT014.ogg"
    # tot "「あ、ん、あっ……」"
    voice "audio/log/C099ED10_TOT014.ogg"
    tot "A, hm, ah..."

# game/scripts/game/OG_C099ED10.rpy:332
translate uzbek OG_C099ED10_2_03092eb5:

    # hcm "（……さっきから、とっさに出る声がいちいちアレなのはなんなんだ！？）"
    hcm "(...Boyadan beri og'zidan chiqayotgan bu g'alati tovushlar nimasi!?)"

# game/scripts/game/OG_C099ED10.rpy:336
translate uzbek OG_C099ED10_2_b02b25c6:

    # voice "audio/log/C099ED10_TOT015.ogg"
    # tot "「やっ、あ……！」"
    voice "audio/log/C099ED10_TOT015.ogg"
    tot "Voh, a...!"

# game/scripts/game/OG_C099ED10.rpy:340
translate uzbek OG_C099ED10_2_d390563e:

    # hcm "（いや、だから！）"
    hcm "(E, bo'ldi-da endi!)"

# game/scripts/game/OG_C099ED10.rpy:354
translate uzbek OG_C099ED10_2_3dbbec3e:

    # voice "audio/log/C099ED10_TOT016.ogg"
    # tot "「はー、興奮したー。ずっとドキドキしてたから、ちょっと疲れちゃった」"
    voice "audio/log/C099ED10_TOT016.ogg"
    tot "Uh, hayajonlanib ketdim. Yuragim tez urib turgani uchun biroz charchadim."

# game/scripts/game/OG_C099ED10.rpy:358
translate uzbek OG_C099ED10_2_2f113b11:

    # voice "audio/log/C099ED10_HCM019.ogg"
    # hcm "「……ああ。俺も別の意味でずっと興奮してて、もうぐったりだ」"
    voice "audio/log/C099ED10_HCM019.ogg"
    hcm "...Ha. Men ham boshqa ma'noda hayajonlanib, tamom bo'ldim."

# game/scripts/game/OG_C099ED10.rpy:372
translate uzbek OG_C099ED10_3_78c17387:

    # voice "audio/log/C099ED10_TOT017.ogg"
    # tot "「恋愛もの？　八幡、こういうの好きなの？」"
    voice "audio/log/C099ED10_TOT017.ogg"
    tot "Romantikami? Hachiman, senga shunaqalar yoqadimi?"

# game/scripts/game/OG_C099ED10.rpy:376
translate uzbek OG_C099ED10_3_4c877cbd:

    # voice "audio/log/C099ED10_HCM020.ogg"
    # hcm "「いや、好きっていうか……なんとなく」"
    voice "audio/log/C099ED10_HCM020.ogg"
    hcm "Yoqadi deyamayman-u... shunchaki."

# game/scripts/game/OG_C099ED10.rpy:380
translate uzbek OG_C099ED10_3_fc2bd065:

    # hcm "（リア充さんたちってこういう映画見るじゃないですか。結果、メイクリア充なわけでしょう？　たぶん、なんかサブリミナル的なアレがあんだよきっと。それを利用させてもらおう）"
