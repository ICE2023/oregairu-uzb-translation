translate uzbek OG_B022ESS4_0_253f605d:

    # hcm "（うわ、すげぇ人だな……千葉市民大集合って感じ）"
    hcm "（Voy-bo‘, buncha odam ko‘p... Butun Chiba xalqi yig‘ilibdi-ku.）"

# game/scripts/game/OG_B022ESS4.rpy:18
translate uzbek OG_B022ESS4_0_a9e709d5:

    # hcm "（由比ケ浜は……ダメだ、来てるか来てないかもわかんねぇ。とりあえずこのあたりで待つしかないか）"
    hcm "（Yuigahama... yo‘q, kelganmi-yo‘qmi bilib bo‘lmayapti. Hozircha shu atrofda kutib turaman shekilli.）"

# game/scripts/game/OG_B022ESS4.rpy:26
translate uzbek OG_B022ESS4_0_9cc3f07b:

    # hcm "（……会えるのか、これ。これだけの人数の中からたったひとりを探すとか、どう考えても無理ゲーなんだが）"
    hcm "（...Ko‘risha olarmikanmiz o‘zi. Shuncha odamning orasidan bittasini topish... umuman iloji yo‘q narsa-ku.）"

# game/scripts/game/OG_B022ESS4.rpy:34
translate uzbek OG_B022ESS4_0_a7c4369f:

    # hcm "「……」"
    hcm "「......」"

# game/scripts/game/OG_B022ESS4.rpy:42
translate uzbek OG_B022ESS4_0_eaca9f57:

    # hcm "（……っていうか、由比ケ浜遅ぇ……もうかなり約束の時間は過ぎてるぞ）"
    hcm "（...O‘zi, Yuigahama buncha kechikdi... Kelishilgan vaqt allaqachon o‘tib ketdi.）"

# game/scripts/game/OG_B022ESS4.rpy:50
translate uzbek OG_B022ESS4_0_ad5f745d:

    # hcm "（電話してみるか……）"
    hcm "（Qo‘ng‘iroq qilib ko‘raymi...）"

# game/scripts/game/OG_B022ESS4.rpy:64
translate uzbek OG_B022ESS4_0_6d925db8:

    # voice "audio/log/B022ESS4_HCM000.ogg"
    # hcm "「あ？　なんでつながらないんだ？」"
    voice "audio/log/B022ESS4_HCM000.ogg"
    hcm "「A? Nega tushmayapti?」"

# game/scripts/game/OG_B022ESS4.rpy:72
translate uzbek OG_B022ESS4_0_591f5c60:

    # hcm "（これだけ人が多いと電波も悪くなるんだろな……めんどくせぇ。このまま繋がらなかったら帰るぞ、俺は）"
    hcm "（Odam ko‘pligidan aloqa ham yomonlashgandir-da... qiyin bo‘ldi. Tusholmasam, uyga ketaveraman.）"

# game/scripts/game/OG_B022ESS4.rpy:78
translate uzbek OG_B022ESS4_0_68041075:

    # hcm "（お、やっとかかった）"
    hcm "（O, nihoyat tushdim.）"

# game/scripts/game/OG_B022ESS4.rpy:84
translate uzbek OG_B022ESS4_0_b8b53db5:

    # voice "audio/log/B022ESS4_YUI000.ogg"
    # yui "「あ、ヒッキー？　いまどこ？」"
    voice "audio/log/B022ESS4_YUI000.ogg"
    yui "「A, Hikki? Hozir qayerdasan?」"

# game/scripts/game/OG_B022ESS4.rpy:88
translate uzbek OG_B022ESS4_0_ee6a515d:

    # voice "audio/log/B022ESS4_HCM001.ogg"
    # hcm "「今どこ？　じゃねぇよ。さっきからずっと会場にいるぞ。お前こそ今どこだよ」"
    voice "audio/log/B022ESS4_HCM001.ogg"
    hcm "「Qayerda bo‘lardim. Boyadan beri tomoshagohdaman. O‘zing qayerdasan?」"

# game/scripts/game/OG_B022ESS4.rpy:92
translate uzbek OG_B022ESS4_0_7e3fb088:

    # voice "audio/log/B022ESS4_YUI001.ogg"
    # yui "「えっ？　待ち合わせ、海浜幕張駅前って言ったよね……？」"
    voice "audio/log/B022ESS4_YUI001.ogg"
    yui "「E? Kaihinmakuhari bekati oldida ko‘rishamiz demaganmidik...?」"

# game/scripts/game/OG_B022ESS4.rpy:96
translate uzbek OG_B022ESS4_0_2a6769ef:

    # voice "audio/log/B022ESS4_HCM002.ogg"
    # hcm "「は？」"
    voice "audio/log/B022ESS4_HCM002.ogg"
    hcm "「A?」"

# game/scripts/game/OG_B022ESS4.rpy:100
translate uzbek OG_B022ESS4_0_dfb5cd67:

    # hcm "（そんなはずねぇだろ。確かに現地集合って……）"
    hcm "（Bo‘lishi mumkin emas. Axir o‘sha yerda ko‘rishamiz deb...）"

# game/scripts/game/OG_B022ESS4.rpy:102
translate uzbek OG_B022ESS4_0_8aa6f229:

    # hcm "（……いや、違う！　現地は人が多いから海浜幕張の駅前っていうことになったんだった……！）"
    hcm "（...Yo‘q, to‘xta! Odam ko‘pligi uchun Kaihinmakuhari bekatida ko‘rishamiz degandik-ku...!）"

# game/scripts/game/OG_B022ESS4.rpy:104
translate uzbek OG_B022ESS4_0_fd6f5a2b:

    # voice "audio/log/B022ESS4_HCM003.ogg"
    # hcm "「悪い、間違えた！　お前、海浜幕張にいるんだよな？」"
    voice "audio/log/B022ESS4_HCM003.ogg"
    hcm "「Uzr, men adashibman! Hozir Kaihinmakuharidamisan?」"

# game/scripts/game/OG_B022ESS4.rpy:108
translate uzbek OG_B022ESS4_0_8f7e50a8:

    # voice "audio/log/B022ESS4_YUI002.ogg"
    # yui "「もう、仕方ないなぁ。ヒッキーもう会場なんだよね？　あたし今からそっちに行くから、そこで待ってて」"
    voice "audio/log/B022ESS4_YUI002.ogg"
    yui "「E-e, iloj yo‘q-da. Hikki, hozir tomoshagohdamisan? Men o‘sha yoqqa boraman, kutib tur.」"

# game/scripts/game/OG_B022ESS4.rpy:112
translate uzbek OG_B022ESS4_0_fe9f5c3d:

    # voice "audio/log/B022ESS4_YUI003.ogg"
    # yui "「すれ違いになるとイヤだから、絶対そこ動かないでね？」"
    voice "audio/log/B022ESS4_YUI003.ogg"
    yui "「Topisha olmay qolmaylik, joyingdan qimirlama, xo‘pmi?」"

# game/scripts/game/OG_B022ESS4.rpy:118
translate uzbek OG_B022ESS4_0_6b284a0d:

    # hcm "（……やべぇ。ほんとすまん、由比ケ浜！）"
    hcm "（...Yomon bo‘ldi. Rostdan ham uzr, Yuigahama!）"