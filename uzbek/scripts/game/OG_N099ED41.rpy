# game/scripts/game/OG_N099ED41.rpy:18
translate uzbek OG_N099ED41_0_4ebcab56:

    # voice "audio/log/N099ED41_HIR000.ogg"
    # hir "「……比企谷、君もやはり相当ラーメンが好きなんだな！　私はうれしいぞ！」"
    voice "audio/log/N099ED41_HIR000.ogg"
    hir "「...Hikigaya, sen ham ramenni yaxshi ko‘rarkansan-a! Men bundan xursandman!」"

# game/scripts/game/OG_N099ED41.rpy:22
translate uzbek OG_N099ED41_0_f71d0fcd:

    # voice "audio/log/N099ED41_HCM000.ogg"
    # hcm "「そうっすね、まあ、うまいラーメンは好きですよ……じゃあ、まあ、今度いっしょに行きますか、食べ歩き」"
    voice "audio/log/N099ED41_HCM000.ogg"
    hcm "「Ha, endi, mazali ramen yoqadi-da... Unda, keyingi safar birga aylangani borarmiz?」"

# game/scripts/game/OG_N099ED41.rpy:32
translate uzbek OG_N099ED41_0_d98a6098:

    # voice "audio/log/N099ED41_HIR001.ogg"
    # hir "「そうか！　私はほぼオールジャンルのラーメンについて網羅している。二度とさっきのような店には連れて行かないからな」"
    voice "audio/log/N099ED41_HIR001.ogg"
    hir "「Shunaqami! Men deyarli barcha turdagi ramenlarni bilaman. Boyagidek joyga boshqa olib bormayman.」"

# game/scripts/game/OG_N099ED41.rpy:36
translate uzbek OG_N099ED41_0_2da9b625:

    # voice "audio/log/N099ED41_HCM001.ogg"
    # hcm "「あ、いや。……さっきの店はハズレでしたけど、まああれはあれで」"
    voice "audio/log/N099ED41_HCM001.ogg"
    hcm "「A, yo‘q... boyagi do‘kon unchalik yaxshi emasdi-yu, lekin u ham bo‘laveradi.」"

# game/scripts/game/OG_N099ED41.rpy:46
translate uzbek OG_N099ED41_0_4646450e:

    # voice "audio/log/N099ED41_HIR002.ogg"
    # hir "「いや、あんなものはラーメンじゃない。任せておけ、必ずや君に満足してもらえる自信があるぞ」"
    voice "audio/log/N099ED41_HIR002.ogg"
    hir "「Yo‘q, u ramen emas. Menga qo‘yib ber, seni albatta mamnun qilishimga ishonaman.」"

# game/scripts/game/OG_N099ED41.rpy:50
translate uzbek OG_N099ED41_0_0ad488d9:

    # voice "audio/log/N099ED41_HCM002.ogg"
    # hcm "「そっすか。……先生と行くなら、まあ、どこのラーメン屋でもまあそれなりに満足できるんじゃないすか」"
    voice "audio/log/N099ED41_HCM002.ogg"
    hcm "「Shunaqami. ...Agar siz bilan borsam, qaysi ramen do‘koni bo‘lsa ham qoniqaman deb o‘ylayman.」"

# game/scripts/game/OG_N099ED41.rpy:62
translate uzbek OG_N099ED41_0_65213a34:

    # voice "audio/log/N099ED41_HIR003.ogg"
    # hir "「……え？　私と行くなら、満足できる？　そ、そう、か？」"
    voice "audio/log/N099ED41_HIR003.ogg"
    hir "「...E? Men bilan borsang, qoniqasanmi? Shu-shunaqami?」"

# game/scripts/game/OG_N099ED41.rpy:66
translate uzbek OG_N099ED41_0_dcdb3805:

    # hcm "（なんで照れてんだよ、この人……）"
    hcm "（Nega uyalyapti bu odam...）"

# game/scripts/game/OG_N099ED41.rpy:76
translate uzbek OG_N099ED41_0_02a01014:

    # voice "audio/log/N099ED41_HIR004.ogg"
    # hir "「い、いや、あ、あれだな、私のら、ラーメンち、知識にた、たいする尊敬、って意味だよ、な……？」"
    voice "audio/log/N099ED41_HIR004.ogg"
    hir "「Yo-yo‘q, a-anavi, mening ra-ramen bi-bilimimga ni-nisbatan hurmatingni bildiryapsan, shundaymi...?」"

# game/scripts/game/OG_N099ED41.rpy:80
translate uzbek OG_N099ED41_0_a6bc9369:

    # voice "audio/log/N099ED41_HCM003.ogg"
    # hcm "「……い、いや、つっかえつっかえ言われると、なんか照れるからやめて下さい」"
    voice "audio/log/N099ED41_HCM003.ogg"
    hcm "「...Yo-yo‘q, duduqlanib gapirsangiz, men ham uyalib qolyapman, bas qiling.」"

# game/scripts/game/OG_N099ED41.rpy:84
translate uzbek OG_N099ED41_0_d08a003e:

    # voice "audio/log/N099ED41_HIR005.ogg"
    # hir "「な、なんの話をしているんだ君はっ！　というより、なんの話をしたかったんだかな、私は……」"
    voice "audio/log/N099ED41_HIR005.ogg"
    hir "「Ni-nima deyapsan o‘zi! To‘g‘rirog‘i, men nima demoqchi edim o‘zi...」"

# game/scripts/game/OG_N099ED41.rpy:88
translate uzbek OG_N099ED41_0_43ac905a:

    # hcm "（だ、大丈夫かこの人。え、えーと）"
    hcm "（Tu-tuzukmikin bu odam. E-e...）"

# game/scripts/game/OG_N099ED41.rpy:90
translate uzbek OG_N099ED41_0_f80864d5:

    # voice "audio/log/N099ED41_HCM004.ogg"
    # hcm "「あ、いや……その、なんつーか、食いものって、なにを食うかも大事ですけど、誰と食うかも大事ですからね、はは……ん？」"
    voice "audio/log/N099ED41_HCM004.ogg"
    hcm "「A, yo‘q... nima desam ekan, ovqatning nimaligi muhim, lekin kim bilan yeyish ham muhim-ku, xaxa... hm?」"

# game/scripts/game/OG_N099ED41.rpy:94
translate uzbek OG_N099ED41_0_bdbf2b01:

    # hcm "（え？　これじゃまるで、平塚先生と行くことが大事みたいな流れになっちゃってない？　え？　どどど、どうしよ？）"
    hcm "（E? Bu Hiratsuka ustoz bilan borish muhim degandek bo‘lib qolmadimi? E? Ni-ni-ni, nima qilsam ekan?）"

# game/scripts/game/OG_N099ED41.rpy:96
translate uzbek OG_N099ED41_0_b6ad69d1:

    # voice "audio/log/N099ED41_HIR006.ogg"
    # hir "「ひ、比企谷……き、君は」"
    voice "audio/log/N099ED41_HIR006.ogg"
    hir "「Hi-Hikigaya... Se-sen...」"

# game/scripts/game/OG_N099ED41.rpy:100
translate uzbek OG_N099ED41_0_d0c55959:

    # voice "audio/log/N099ED41_HCM005.ogg"
    # hcm "「せ、先生……」"
    voice "audio/log/N099ED41_HCM005.ogg"
    hcm "「U-ustoz...」"

# game/scripts/game/OG_N099ED41.rpy:104
translate uzbek OG_N099ED41_0_e897b64d:

    # hcm "（……なんだこの雰囲気？）"
    hcm "（...Bu qanday muhit?）"

# game/scripts/game/OG_N099ED41.rpy:108
translate uzbek OG_N099ED41_0_b1541356:

    # voice "audio/log/N099ED41_HIR007.ogg"
    # hir "「……トマトは好きか？」"
    voice "audio/log/N099ED41_HIR007.ogg"
    hir "「...Pomidorni yaxshi ko‘rasanmi?」"

# game/scripts/game/OG_N099ED41.rpy:120
translate uzbek OG_N099ED41_0_6f7c9e6d:

    # voice "audio/log/N099ED41_HCM006.ogg"
    # hcm "「……は？」"　
    voice "audio/log/N099ED41_HCM006.ogg"
    hcm "「...A?」"

# game/scripts/game/OG_N099ED41.rpy:130
translate uzbek OG_N099ED41_0_c1570dc4:

    # voice "audio/log/N099ED41_HIR008.ogg"
    # hir "「いや、私はトマトが嫌いで、トマトを使った麺はいまだに許すことができない。私のデータベースにあのジャンルだけはないと言う話をしておこうと」"
    voice "audio/log/N099ED41_HIR008.ogg"
    hir "「Yo‘q, men pomidorni yomon ko‘raman, pomidorli lag‘monni ham hali qabul qila olmayman. Mening ma’lumotlar bazamda faqat shu janr yo‘q ekanini aytib qo‘ymoqchi edim.」"

# game/scripts/game/OG_N099ED41.rpy:134
translate uzbek OG_N099ED41_0_0125dd31:

    # voice "audio/log/N099ED41_HCM007.ogg"
    # hcm "「……あ、トマト嫌いなんすか」"
    voice "audio/log/N099ED41_HCM007.ogg"
    hcm "「...A, pomidorni yomon ko‘rasizmi?」"

# game/scripts/game/OG_N099ED41.rpy:144
translate uzbek OG_N099ED41_0_a49867d2:

    # voice "audio/log/N099ED41_HIR009.ogg"
    # hir "「あのぐしゅっとした感じと独特の青臭さが苦手でな……」"
    voice "audio/log/N099ED41_HIR009.ogg"
    hir "「Ezilgan holati va g‘alati hidi yoqmaydi...」"

# game/scripts/game/OG_N099ED41.rpy:150
translate uzbek OG_N099ED41_0_5a026483:

    # hcm "（……さっきの雰囲気消し飛んだな。この人やっぱりただのラーメン好きか。……ほっ）"
    hcm "（...Boyagi muhit yo‘qoldi-ku. Bu odam rostdan ham faqat ramenni yaxshi ko‘rar ekan. ...Uf-f.）"

# game/scripts/game/OG_N099ED41.rpy:152
translate uzbek OG_N099ED41_0_1b18f948:

    # voice "audio/log/N099ED41_HCM008.ogg"
    # hcm "「……まあ、トマトは別にどっちでもいいですよ。俺もトマト嫌いだし。とりあえず先生のおススメの店に連れてってくれれば」"
    voice "audio/log/N099ED41_HCM008.ogg"
    hcm "「...Mayli, pomidorning farqi yo‘q. Men ham pomidorni yomon ko‘raman. Asosiysi o‘zingiz tavsiya qilgan joyga olib borsangiz bo‘ldi.」"

# game/scripts/game/OG_N099ED41.rpy:162
translate uzbek OG_N099ED41_0_11a8ea8a:

    # voice "audio/log/N099ED41_HIR010.ogg"
    # hir "「よし、わかった！　ふふ……楽しみにしておきたまえ」"
    voice "audio/log/N099ED41_HIR010.ogg"
    hir "「Yaxshi, kelishdik! Fufu... kutavergin.」"

# game/scripts/game/OG_N099ED41.rpy:166
translate uzbek OG_N099ED41_0_1411296e:

    # hcm "（そう言う先生の方が、なんだかよっぽど楽しそうだな……）"
    hcm "（Shunday deyayotgan ustozning o‘zi, negadir juda xursand ko‘rinyapti...）"