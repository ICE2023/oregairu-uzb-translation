# game/scripts/game/OG_TALK_TOT4.rpy:162
translate uzbek OG_TALK_TOT4_6_96124b45:

    # hcm "「プールが嫌いとか、水にトラウマがあるとかってわけじゃないんだろ？」"
    hcm "「Basseyni yomon ko‘rmaysanmi? Suvdan qo‘rqmaysanmi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:164
translate uzbek OG_TALK_TOT4_6_19fa1ad7:

    # voice "audio/log/TOT4B1_TOT000.ogg"
    # tot mcc01 "「う、うんっ！　大好きだよ。水の中にいると気持ちいいし、ぼくお風呂も好きだし」"
    voice "audio/log/TOT4B1_TOT000.ogg"
    tot mcc01 "「H-ha! Juda yaxshi ko‘raman. Suvda bo‘lish yoqimli, men hammomni ham yaxshi ko‘raman.」"

# game/scripts/game/OG_TALK_TOT4.rpy:168
translate uzbek OG_TALK_TOT4_6_93643d5e:

    # hcm "「風呂か……風呂……風呂ねぇ……」"
    hcm "「Hammom... hammom... hammom-a...」"

# game/scripts/game/OG_TALK_TOT4.rpy:170
translate uzbek OG_TALK_TOT4_6_5c85f0b4:

    # voice "audio/log/TOT4B1_TOT001.ogg"
    # tot "「え？　八幡はお風呂、嫌い？」"
    voice "audio/log/TOT4B1_TOT001.ogg"
    tot "「E? Hachiman, hammomni yomon ko‘rasanmi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:174
translate uzbek OG_TALK_TOT4_6_18b7e797:

    # hcm "「いや、好きだ大好きだ、風呂最高っ！」"
    hcm "「Yo‘q, yaxshi ko‘raman, juda yaxshi ko‘raman, hammom zo‘r!」"

# game/scripts/game/OG_TALK_TOT4.rpy:176
translate uzbek OG_TALK_TOT4_6_b644b862:

    # voice "audio/log/TOT4B1_TOT002.ogg"
    # tot "「スパとか楽しいよね。いろんな種類のお風呂があって、休憩スペースも広くて、ずっといても飽きないし……」"
    voice "audio/log/TOT4B1_TOT002.ogg"
    tot "「Spalar ham qiziq-a. Har xil turdagi hammomlar bor, dam olish joyi ham keng, uzoq vaqt zerikmay o‘tirish mumkin...」"

# game/scripts/game/OG_TALK_TOT4.rpy:180
