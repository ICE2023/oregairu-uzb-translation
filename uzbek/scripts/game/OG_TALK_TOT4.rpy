    # tot "「う、うん、わかった」"
    voice "audio/log/TOT4B1_TOT005.ogg"
    tot "「X-xo‘p, tushunarli.」"

# game/scripts/game/OG_TALK_TOT4.rpy:208
translate uzbek OG_TALK_TOT4_7_f87343c2:

    # hcm "「平気だっつわれてもな……気分が悪いとか、腹が痛いとかいうわけじゃないんだよな？」"
    hcm "「Yaxshiman deganing bilan... Mazang qochmayaptimi, qorning og‘rimayaptimi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:210
translate uzbek OG_TALK_TOT4_7_aef4be6c:

    # voice "audio/log/TOT4B2_TOT000.ogg"
    # tot mcc01 "「ふふっ……八幡はやさしいね」"
    voice "audio/log/TOT4B2_TOT000.ogg"
    tot mcc01 "「Fufu... Hachiman, mehribonsan.」"

# game/scripts/game/OG_TALK_TOT4.rpy:214
translate uzbek OG_TALK_TOT4_7_af551d47:

    # hcm "「よくあるだろ、重病なヤツほど平気と言い、仮病のヤツほど大げさに苦しがるのって。俺が仮病使うとき、気を付けてるポイントだ。だから多少心配してるんだよ」"
    hcm "「Bilasan-ku, qattiq kasal bo‘lgan odam yaxshiman deydi, soxta kasal bo‘lgan odam esa o‘zini juda og‘ir ahvolda ko‘rsatadi. Men ham soxta kasal bo‘lganimda shunga e’tibor beraman. Shuning uchun xavotir olyapman.」"

# game/scripts/game/OG_TALK_TOT4.rpy:216
translate uzbek OG_TALK_TOT4_7_cfa2e21a:

    # voice "audio/log/TOT4B2_TOT001.ogg"
    # tot mcc00 "「本当に大丈夫だから。おでこ触ってみる？　熱もないよ」"
    voice "audio/log/TOT4B2_TOT001.ogg"
    tot mcc00 "「Rostdan ham yaxshiman. Peshonamni ushlab ko‘rasanmi? Isitmam yo‘q.」"

# game/scripts/game/OG_TALK_TOT4.rpy:220
translate uzbek OG_TALK_TOT4_7_4a3785a5:

    # hcm "「そ、そうか……それならいいんだ」"
    hcm "「Sh-shunaqami... unda yaxshi.」"

# game/scripts/game/OG_TALK_TOT4.rpy:222
translate uzbek OG_TALK_TOT4_7_07295a56:

    # voice "audio/log/TOT4B2_TOT002.ogg"
    # tot mcc01 "「心配してくれてありがとう。うれしいな」"
    voice "audio/log/TOT4B2_TOT002.ogg"
    tot mcc01 "「Qayg‘urganing uchun rahmat. Xursandman.」"

# game/scripts/game/OG_TALK_TOT4.rpy:238
translate uzbek OG_TALK_TOT4_8_7e891e24:

    # hcm "「戸塚の中で、夏の思い出の定番ってなんだ？」"
    hcm "「Totsuka uchun yozgi xotiralarning eng esda qolarlisi nima?」"

# game/scripts/game/OG_TALK_TOT4.rpy:240
translate uzbek OG_TALK_TOT4_8_42585a5f:

    # voice "audio/log/TOT4B3_TOT000.ogg"
    # tot mcc02 "「んー……なんだろう。最近はテニスばっかりだけど……」"
    voice "audio/log/TOT4B3_TOT000.ogg"
    tot mcc02 "「Hmm... nima ekan. Oxirgi paytlarda faqat tennis o‘ynayapman-u...」"

# game/scripts/game/OG_TALK_TOT4.rpy:244
translate uzbek OG_TALK_TOT4_8_31d3c236:

    # voice "audio/log/TOT4B3_TOT001.ogg"
    # tot mcc01 "「そういえば小さい頃はよく、家にあるちっちゃなビニールプールで遊んでたみたいだよ。写真がいっぱい残ってるし」"
    voice "audio/log/TOT4B3_TOT001.ogg"
    tot mcc01 "「Aytgancha, yoshligimda uydagi kichkina rezina basseynida ko‘p o‘ynagan ekanman. Rasmlarim ko‘p.」"

# game/scripts/game/OG_TALK_TOT4.rpy:248
translate uzbek OG_TALK_TOT4_8_b97db153:

    # hcm "「おー、あれな！　ウチにもあるぜ。俺と小町が素っ裸でビニールプールで遊んでる写真」"
    hcm "「O‘, shunaqami! Bizda ham bor. Men va Komachi yalang‘och bo‘lib rezina basseynida o‘ynayotgan rasmlarimiz.」"

# game/scripts/game/OG_TALK_TOT4.rpy:250
translate uzbek OG_TALK_TOT4_8_4a1f2778:

    # voice "audio/log/TOT4B3_TOT002.ogg"
    # tot "「わぁ、きっとかわいいんだろうなぁ」"
    voice "audio/log/TOT4B3_TOT002.ogg"
    tot "「Vay, juda shirin bo‘lsanglar kerak.」"

# game/scripts/game/OG_TALK_TOT4.rpy:254
translate uzbek OG_TALK_TOT4_8_e2d428dd:

    # hcm "「ああ！　俺も小町も最高に可愛いぜ！　むしろ、俺の可愛さのピークまである」"
    hcm "「Ha! Men ham, Komachi ham juda shirinmiz! Hattoki, mening eng shirin paytim shu bo‘lsa kerak.」"

# game/scripts/game/OG_TALK_TOT4.rpy:256
translate uzbek OG_TALK_TOT4_8_5db8fc26:

    # voice "audio/log/TOT4B3_TOT003.ogg"
    # tot mcc00 "「そう言われるとすごく見たくなっちゃうなぁ、昔の八幡」"
    voice "audio/log/TOT4B3_TOT003.ogg"
    tot mcc00 "「Shunday desang, seni yoshligingni ko‘rgim kelib qoldi.」"

# game/scripts/game/OG_TALK_TOT4.rpy:260
translate uzbek OG_TALK_TOT4_8_154c1410:

    # hcm "「ま、まぁ、そのうち見にくるか？」"
    hcm "「Ma-mayli, bir kuni ko‘rgani kelasanmi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:262
translate uzbek OG_TALK_TOT4_8_212aa278:

    # voice "audio/log/TOT4B3_TOT004.ogg"
    # tot "「いいの？　ふふっ、楽しみにしてるね！」"
    voice "audio/log/TOT4B3_TOT004.ogg"
    tot "「Maylimi? Fufu, kutib qolaman!」"

# game/scripts/game/OG_TALK_TOT4.rpy:278
translate uzbek OG_TALK_TOT4_9_2814f180:

    # hcm "「課題、やってるか？」"
    hcm "「Uyga vazifalarni qilyapsanmi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:280
translate uzbek OG_TALK_TOT4_9_d7025826:

    # voice "audio/log/TOT4B4_TOT000.ogg"
    # tot mcc02 "「え……えーっと、八幡は？」"
    voice "audio/log/TOT4B4_TOT000.ogg"
    tot mcc02 "「E... anavi, o‘zing-chi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:284
translate uzbek OG_TALK_TOT4_9_070081a1:

    # hcm "「俺はそれなりに、だな。ヒマ潰しついでに解いてたら結構進んでた……なんならもうすぐ終わる。……まだ夏休み残ってんだけどなー、超ヒマなんだな、俺」"
    hcm "「Men bir amallayapman. Zerikkanimdan yechib o‘tirgandim, ancha qilib qo‘ydim... deyarli tugatay deb qoldim. ...Hali yozgi ta’til tugagani ham yo‘q, men shunchalik bekorchiman-a.」"

# game/scripts/game/OG_TALK_TOT4.rpy:286
translate uzbek OG_TALK_TOT4_9_75b44d55:

    # voice "audio/log/TOT4B4_TOT001.ogg"
    # tot "「えらいなぁ。ぼく、家に帰るとすぐ眠くなっちゃって、なかなか進まなくて」"
    voice "audio/log/TOT4B4_TOT001.ogg"
    tot "「Barakalla. Men uyga borsam uyqum kelib qoladi, shuning uchun unchalik qilolmayapman.」"

# game/scripts/game/OG_TALK_TOT4.rpy:290
translate uzbek OG_TALK_TOT4_9_a1229198:

    # hcm "「集中してやればたいした量じゃないし、部活がない日にでもサッサと片付ければいいと思うぞ。図書館は静かで涼しくてオススメだ」"
    hcm "「Diqqat bilan qilsang unchalik ko‘p emas, to‘garak bo‘lmagan kunda tezroq tugatib qo‘yganing ma’qul. Kutubxona tinch va salqin, o‘sha yerni tavsiya qilaman.」"

# game/scripts/game/OG_TALK_TOT4.rpy:292
translate uzbek OG_TALK_TOT4_9_3f50c034:

    # voice "audio/log/TOT4B4_TOT002.ogg"
    # tot mcc00 "「うん、頑張る……」"
    voice "audio/log/TOT4B4_TOT002.ogg"
    tot mcc00 "「Ha, harakat qilaman...」"

# game/scripts/game/OG_TALK_TOT4.rpy:308
translate uzbek OG_TALK_TOT4_10_7f0dfac3:

    # hcm "「小町がいっしょだとうるさくないか？」"
    hcm "「Komachi bilan birga bo‘lsang, shovqin qilmayaptimi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:310
translate uzbek OG_TALK_TOT4_10_b95047ea:

    # voice "audio/log/TOT4B5_TOT000.ogg"
    # tot "「どうして？　楽しいよ。もともと小町ちゃんがプールに来たいって言ったんだよね？　ぼくの方こそ、お邪魔じゃないかな」"
    voice "audio/log/TOT4B5_TOT000.ogg"
    tot "「Nega? Qiziq-ku. O‘zi Komachi-chan basseynga kelishni xohlagandi-ku? Men xalaqit bermayapmanmi?」"

# game/scripts/game/OG_TALK_TOT4.rpy:314
translate uzbek OG_TALK_TOT4_10_f2350c70:

    # hcm "「お邪魔なんかじゃない、むしろおいでなさいませ！」"
    hcm "「Aslo xalaqit bermaysan, aksincha, xush kelibsan!」"

# game/scripts/game/OG_TALK_TOT4.rpy:316
translate uzbek OG_TALK_TOT4_10_6efbcc26:

    # voice "audio/log/TOT4B5_TOT001.ogg"
    # tot "「八幡と小町ちゃん、いつも仲良くてうらやましいな」"
    voice "audio/log/TOT4B5_TOT001.ogg"
    tot "「Hachiman va Komachi doim inoq, havasim keladi.」"

# game/scripts/game/OG_TALK_TOT4.rpy:320
translate uzbek OG_TALK_TOT4_10_701d042d:

    # hcm "「仲良いっつーか、あれは俺のことを都合のいいなにかだと思ってるだけだぞ。それにいつもじゃない。飯のこととかでよく言い争ってるし」"
    hcm "「Inoq emish... U meni o‘ziga qulay buyum deb o‘ylaydi. Doim ham inoq emasmiz. Ovqat masalasida ko‘p tortishamiz.」"

# game/scripts/game/OG_TALK_TOT4.rpy:322
translate uzbek OG_TALK_TOT4_10_79a80c13:

    # voice "audio/log/TOT4B5_TOT002.ogg"
    # tot mcc01 "「仲良しだね」"
    voice "audio/log/TOT4B5_TOT002.ogg"
    tot mcc01 "「Inoq ekansizlar.」"

# game/scripts/game/OG_TALK_TOT4.rpy:326
translate uzbek OG_TALK_TOT4_10_79e06d4c:

    # hcm "「違うって。あいつ勝手に俺の服着てるし、部屋入って掃除してくるし、連絡しないで遅く帰るとなんか怒るし」"
    hcm "「Unday emas. U ruxsatsiz kiyimimni kiyadi, xonamga kirib tozalaydi, ogohlantirmasdan kech qaytsam jahl qiladi.」"

# game/scripts/game/OG_TALK_TOT4.rpy:328
translate uzbek OG_TALK_TOT4_10_e42739d1:

    # hcm "「なのにあいつが遅く帰ってくるときは、連絡しないんだぜ？　それでこっちが電話したら、うるさいとか言うんだ。ひでぇ妹だろ」"
    hcm "「O‘zi kech qaytganida esa, xabar ham bermaydi. Telefon qilsam, “jim bo‘l” deydi. Yomon singil-a?」"

# game/scripts/game/OG_TALK_TOT4.rpy:330
translate uzbek OG_TALK_TOT4_10_4beb7191:

    # voice "audio/log/TOT4B5_TOT003.ogg"
    # tot "「……仲良しだよね」"
    voice "audio/log/TOT4B5_TOT003.ogg"
    tot "「...Inoqsizlar.」"

# game/scripts/game/OG_TALK_TOT4.rpy:346
translate uzbek OG_TALK_TOT4_11_81f98529:

    # hcm "「目の保養ってよく言うけど……」"
    hcm "「Ko‘zni quvontiradi deyishadi-yu...」"

# game/scripts/game/OG_TALK_TOT4.rpy:348
translate uzbek OG_TALK_TOT4_11_3f362d29:

    # voice "audio/log/TOT4B6_TOT000.ogg"
    # tot mcc02 "「ああ、うん……でも、女の人をそういう目で見るのは、あんまりよくないと思うよ？」"
    voice "audio/log/TOT4B6_TOT000.ogg"
    tot mcc02 "「Ha, tushunarli... lekin ayollarga bunday qarash unchalik yaxshi emas deb o‘ylayman?」"

# game/scripts/game/OG_TALK_TOT4.rpy:352
translate uzbek OG_TALK_TOT4_11_072052b1:

    # hcm "「えっ？　……ああいや、そっちじゃなくて」"
    hcm "「E? ...A, yo‘q, men uni nazarda tutmadim.」"

# game/scripts/game/OG_TALK_TOT4.rpy:354
translate uzbek OG_TALK_TOT4_11_368bed29:

    # voice "audio/log/TOT4B6_TOT001.ogg"
    # tot mcc04 "「あれっ、水着とか、そういう話じゃなかったの？」"
    voice "audio/log/TOT4B6_TOT001.ogg"
    tot mcc04 "「Iye, cho‘milish kiyimi haqida gapirmayotganmiding?」"

# game/scripts/game/OG_TALK_TOT4.rpy:358
translate uzbek OG_TALK_TOT4_11_71fdc6ff:

    # hcm "「そういう話、では、あるといえばあるんだが……いや、結論としてよくないことには違いない。すまん忘れてくれ」"
    hcm "「O‘shanaqa gap desam ham bo‘ladi-yu, lekin... yo‘q, xulosa qilib aytganda, bu yaxshi narsa emasligi aniq. Uzr, unutib yubor.」"

# game/scripts/game/OG_TALK_TOT4.rpy:360
translate uzbek OG_TALK_TOT4_11_4c2748f8:

    # voice "audio/log/TOT4B6_TOT002.ogg"
    # tot "「うん、わかった……」"
    voice "audio/log/TOT4B6_TOT002.ogg"
    tot "「Xo‘p, tushundim...」"

# game/scripts/game/OG_TALK_TOT4.rpy:364
translate uzbek OG_TALK_TOT4_11_a7c4369f:

    # hcm "「……」"
    hcm "「...」"

