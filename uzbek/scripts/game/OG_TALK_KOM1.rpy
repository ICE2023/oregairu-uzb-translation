# game/scripts/game/OG_TALK_KOM1.rpy:323
translate uzbek OG_TALK_KOM16_f93ac388:

    # voice "audio/log/KOM1B4_KOM000.ogg"
    # kom mca05 "「うん、今はちょっといいかな」"
    voice "audio/log/KOM1B4_KOM000.ogg"
    kom mca05 "「Rahmat, hozir kerak emas.」"

# game/scripts/game/OG_TALK_KOM1.rpy:327
translate uzbek OG_TALK_KOM16_a1574d8c:

    # hcm "「そう言うなよ。まぁ聞け」"
    hcm "「Unday dema. Eshitib tur.」"

# game/scripts/game/OG_TALK_KOM1.rpy:329
translate uzbek OG_TALK_KOM16_c3cd1b44:

    # voice "audio/log/KOM1B4_KOM001.ogg"
    # kom mca09 "「そういう態度はどうかと思うなー。というわけで、ここまでの進め方によってはこのあと強制終了となることもあるから、そのつもりでね」"
    voice "audio/log/KOM1B4_KOM001.ogg"
    kom mca09 "「Bunday muomala yaxshi emas-da. Shuning uchun, bu yerdagi harakatlaringizga qarab, suhbat majburan to‘xtatilishi mumkin, shuni unutmang.」"

# game/scripts/game/OG_TALK_KOM1.rpy:333
translate uzbek OG_TALK_KOM16_e4a92067:

    # hcm "「つまり、今回のこれはハズレカードってことか。それとも、俺がハズレってことなの？」"
    hcm "「Demak, bu safar omadsiz karta tushdi. Yoki men omadsizmanmi?」"

# game/scripts/game/OG_TALK_KOM1.rpy:335
translate uzbek OG_TALK_KOM16_d2fca8aa:

    # voice "audio/log/KOM1B4_KOM002.ogg"
    # kom mca01 "「だね。どっちも正解」"
    voice "audio/log/KOM1B4_KOM002.ogg"
    kom mca01 "「Ha. Ikkalasi ham to‘g‘ri.」"

# game/scripts/game/OG_TALK_KOM1.rpy:346
translate uzbek OG_TALK_KOM17_ef475961:

    # hcm "「ちょっと思ってたんだが、ｉｆ的な流れで考えると、やっぱ俺の未来には平塚先生との結婚もありうるってことか」"
    hcm "「O‘ylab qoldim, agar “agar” deb faraz qilsak, kelajakda Hiratsuka ustoz bilan turmush qurishim mumkinmi?」"

# game/scripts/game/OG_TALK_KOM1.rpy:348
translate uzbek OG_TALK_KOM17_51038f17:

    # voice "audio/log/KOM1B5_KOM000.ogg"
    # kom mca09 "「そうだね、ある意味お兄ちゃんだけがハッピーな悲劇の結末だね」"
    voice "audio/log/KOM1B5_KOM000.ogg"
    kom mca09 "「Ha, bir tomondan, faqat akam baxtli bo‘ladigan fojiali yakun bu.」"

# game/scripts/game/OG_TALK_KOM1.rpy:352
translate uzbek OG_TALK_KOM17_b26661f8:

    # hcm "「最大多数の最大幸福という観点からすれば悲劇かもしれんが、俺はハッピーなんだろ？　だったら……」"
    hcm "「Ko‘pchilikning baxti nuqtai nazaridan fojia bo‘lishi mumkin, lekin men baxtli bo‘laman-ku? Unda...」"

# game/scripts/game/OG_TALK_KOM1.rpy:354
translate uzbek OG_TALK_KOM17_72bd4556:

    # voice "audio/log/KOM1B5_KOM001.ogg"
    # kom mca10 "「ダメだからね？」"
    voice "audio/log/KOM1B5_KOM001.ogg"
    kom mca10 "「Mumkin emas, xo‘pmi?」"

# game/scripts/game/OG_TALK_KOM1.rpy:358
translate uzbek OG_TALK_KOM17_2f721a95:

    # hcm "「ダメか？」"
    hcm "「Mumkin emasmi?」"

# game/scripts/game/OG_TALK_KOM1.rpy:360
translate uzbek OG_TALK_KOM17_d7de3793:

    # voice "audio/log/KOM1B5_KOM002.ogg"
    # kom mca06 "「……はぁ〜、やっぱダメだお兄ちゃんは……というわけで小町のやる気ゼロになりました。ここで強制終了となります」"
    voice "audio/log/KOM1B5_KOM002.ogg"
    kom mca06 "「...Uf-f, baribir bo‘lmaydi... Shuning uchun Komachining hafsalasi pir bo‘ldi. Suhbat shu yerda tugaydi.」"

# game/scripts/game/OG_TALK_KOM1.rpy:364
translate uzbek OG_TALK_KOM17_81eb1da4:

    # hcm "「マジか？　ということは、これは大ハズレカードだったってことか」"
    hcm "「Rostdanmi? Demak, bu eng omadsiz karta ekan.」"

# game/scripts/game/OG_TALK_KOM1.rpy:366
translate uzbek OG_TALK_KOM17_836cc4f4:

    # voice "audio/log/KOM1B5_KOM003.ogg"
    # kom mca09 "「そういうことだね。一発アウトってヤツだよ」"
    voice "audio/log/KOM1B5_KOM003.ogg"
    kom mca09 "「Shunaqa. Bir urunishda yutqazdingiz.」"

# game/scripts/game/OG_TALK_KOM1.rpy:370
translate uzbek OG_TALK_KOM17_3a15dd69:

    # hcm "「よりにもよって先生の話題が大ハズレとは、なんて残酷な……！」"
    hcm "「Aynan ustoz haqidagi mavzu omadsiz bo‘lib chiqishi... qanchalar shafqatsizlik...!」"

# game/scripts/game/OG_TALK_KOM1.rpy:372
translate uzbek OG_TALK_KOM17_1ff25320:

    # voice "audio/log/KOM1B5_KOM004.ogg"
    # kom "「回避しなきゃって言ってたお話を蒸し返すからだよ、先生に罪はないよ……とにかく終了！」"
    voice "audio/log/KOM1B5_KOM004.ogg"
    kom "「Qochish kerak bo‘lgan mavzuni gapirganingiz uchun shunday bo‘ldi, ustozning aybi yo‘q... Xullas, tamom!」"

# game/scripts/game/OG_TALK_KOM1.rpy:388
translate uzbek OG_TALK_KOM18_620d6a4d:

    # hcm "「これ以上ダラダラ話すこともねーな。よし、おしまい」"
    hcm "「Bekorchi gap sotishning keragi yo‘q. Bo‘ldi, tugatdik.」"

# game/scripts/game/OG_TALK_KOM1.rpy:390
translate uzbek OG_TALK_KOM18_a8cb73de:

    # voice "audio/log/KOM1B6_KOM000.ogg"
    # kom mca08 "「えっ、もう終わらせるの？」"
    voice "audio/log/KOM1B6_KOM000.ogg"
    kom mca08 "「E, tugatmoqchimisiz?」"

# game/scripts/game/OG_TALK_KOM1.rpy:394
translate uzbek OG_TALK_KOM18_670cfe9c:

    # hcm "「ああ、終わりだ」"
    hcm "「Ha, tamom.」"

# game/scripts/game/OG_TALK_KOM1.rpy:396
translate uzbek OG_TALK_KOM18_6005b1b5:

    # voice "audio/log/KOM1B6_KOM001.ogg"
    # kom mca00 "「そっか……つまり、これ以上練習する必要はないということだね？」"
    voice "audio/log/KOM1B6_KOM001.ogg"
    kom mca00 "「Tushunarli... demak, ortiqcha mashq qilishga hojat yo‘q ekan-da?」"

# game/scripts/game/OG_TALK_KOM1.rpy:400
translate uzbek OG_TALK_KOM18_ad1bc226:

    # hcm "「そう考えてくれてもいいぞ」"
    hcm "「Shunday deb o‘ylasang ham bo‘ladi.」"

# game/scripts/game/OG_TALK_KOM1.rpy:402
translate uzbek OG_TALK_KOM18_aa5e3520:

    # voice "audio/log/KOM1B6_KOM002.ogg"
    # kom mca01 "「あれ、おかしいな、お兄ちゃんが頼もしく見える……？」"
    voice "audio/log/KOM1B6_KOM002.ogg"
    kom mca01 "「Iye, g‘alati-ku, akam ishonchli ko‘rinyaptimi...?」"

# game/scripts/game/OG_TALK_KOM1.rpy:406
translate uzbek OG_TALK_KOM18_3ffc4909:

    # hcm "「いや、いつだって俺は頼れる兄貴だろうが」"
    hcm "「Yo‘q, men har doim ishonchli aka bo‘lganman.」"

# game/scripts/game/OG_TALK_KOM1.rpy:408
translate uzbek OG_TALK_KOM18_51add85f:

    # voice "audio/log/KOM1B6_KOM003.ogg"
    # kom mca00 "「……反論がないこともないけど、小町がそう思っちゃった以上、今回はいい反応のまま終わるけどさ」"
    voice "audio/log/KOM1B6_KOM003.ogg"
    kom mca00 "「...E’tirozim yo‘q emas, lekin Komachi shunday o‘ylagani uchun, bu safar yaxshi yakunlaymiz.」"

# game/scripts/game/OG_TALK_KOM1.rpy:412
translate uzbek OG_TALK_KOM18_8e9b8639:

    # hcm "「つまり一発成功の大当たりカードだったってことか。そういうのもあるんだな」"
    hcm "「Demak, bu bir urunishda muvaffaqiyat keltiradigan omadli karta ekan. Shunaqasi ham bo‘lar ekan-da.」"

# game/scripts/game/OG_TALK_KOM1.rpy:414
translate uzbek OG_TALK_KOM18_27cd444c:

    # voice "audio/log/KOM1B6_KOM004.ogg"
    # kom "「うん、まぁね。でも普通お話を一方的に切り上げられたら、相手は傷つくと思うから、そこは忘れちゃダメだよ？」"
    voice "audio/log/KOM1B6_KOM004.ogg"
    kom "「Ha, shunday. Lekin odatda suhbatni bir tomonlama tugatsangiz, suhbatdoshingiz xafa bo‘lishi mumkin, shuni unutmang.」"

# game/scripts/game/OG_TALK_KOM1.rpy:418
translate uzbek OG_TALK_KOM18_85f772e5:

    # hcm "「ん、わかった」"
    hcm "「Hm, tushundim.」"

